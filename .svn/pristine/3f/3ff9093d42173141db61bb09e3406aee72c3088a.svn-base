import { Component } from '@angular/core';
import { FormBuilder, FormControl, FormGroup, Validators,  ValidationErrors,
  AbstractControl,
  ValidatorFn } from '@angular/forms';
import { ToastrService } from 'ngx-toastr';
import { SocketService } from 'src/app/ems.utilities/services/socket.service';

@Component({
  selector: 'app-crm-smm-telegramsummary',
  templateUrl: './crm-smm-telegramsummary.component.html',
  styleUrls: ['./crm-smm-telegramsummary.component.scss']
})
export class CrmSmmTelegramsummaryComponent {

  responsedata: any;
  instagram_list: any;

  image_list: any;
  video_list: any;
  media_url: any;
  profile_photo: any;
  parameterValue: any;

  invite_link: any;
  id: any;
  bot_name: any;
  group: any;
  username: any;
  telegram_list: any;
  constructor(private formBuilder: FormBuilder, private ToastrService: ToastrService, public service: SocketService) {
   
  }
  ngOnInit(): void {
    var url = 'Telegram/GetTelegram'
    this.service.get(url).subscribe((result,) => {

      this.telegram_list = result;
      this.id =this.telegram_list.result.id;
      this.bot_name = this.telegram_list.result.username;
      this.group = this.telegram_list.result.title;
      this.invite_link = this.telegram_list.result.invite_link;

    });
    var url1 = 'Telegram/GetTelegramImage'
    this.service.get(url1).subscribe((result,) => {

      this.responsedata = result;
      this.image_list = this.responsedata.telegramimage_list;
    

    });
    var url2 = 'Telegram/GetTelegramVideo'
    this.service.get(url2).subscribe((result,) => {
      this.responsedata = result;
      this.video_list = this.responsedata.telegramvideo_list;
     
   

    });
  

}
modalimage(parameter: string) {
  this.parameterValue = parameter
console.log(this.parameterValue)
}
}
