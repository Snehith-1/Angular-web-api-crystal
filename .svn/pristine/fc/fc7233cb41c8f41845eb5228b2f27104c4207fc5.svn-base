      // console.log(this.CurObj)
      this.NgxSpinnerService.show();
      var url1 = 'ShopifyCustomer/GetLeadmoved'
      this.service.post(url1, this.CurObj).pipe().subscribe((result: any) => {

        if (result.status == false) {

