  openModalamend(){}
  onaddinfo(){}
onadd(params: any){
  const secretKey = 'storyboarderp';

  const param = (params);

  const encryptedParam = AES.encrypt(param,secretKey).toString() ;
  this.route.navigate(['/smr/SmrTrnQuoteToOrder',encryptedParam]);
}



}



