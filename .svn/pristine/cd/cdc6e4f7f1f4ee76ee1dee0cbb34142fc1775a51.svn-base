<!-- begin::Add PO -->
<div class="card ">
    <!-- begin::Heading -->
    <div class="card-header bg-lightblue card-height55">
        <div class="card-title m-0">
            <h5 class="fw-bolder text-blue">VIEW PURCHASE ORDER</h5>
        </div>
    </div>
    <!-- end::Heading -->

    <!-- begin:: PO Add Form-->



    <div *ngFor="let data of purchaseorder_list;">
        <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom">
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>PO No</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.purchaseorder_gid}}</p>
                </div>
            </div>

            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>PO Ref.No</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.manualporef_no}}</p>
                </div>
            </div>


            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>PO Date</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.purchaseorder_date}}</p>

                </div>
            </div>


            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Branch Name</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.branch_name}}</p>

                </div>
            </div>
        </div>
    </div>


    <div *ngFor="let data of purchaseorder_list;">
        <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom">
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Name</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.vendor_companyname}}</p>

                </div>
            </div>


            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Despatch To Branch</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.deliverytobranch}}</p>

                </div>
            </div>

            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Mobile</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.vendor_contactnumber}}</p>

                </div>
            </div>

            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Contact Person</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.vendor_contact_person}}</p>

                </div>
            </div>
        </div>
    </div>


    <div *ngFor="let data of purchaseorder_list;">
        <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom">
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Fax</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.vendor_faxnumber}}</p>
                    <!-- <input type="text"
                        class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
                        placeholder="Vendor Fax"  formControlName="vendor_fax" class="form-control" /> -->
                </div>
            </div>

            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Email </b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.vendor_emailid}}</p>

                </div>
            </div>

            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Address</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.vendor_address}}</p>

                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Exchange Rate</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.exchange_rate}}</p>

                </div>
            </div>

        </div>

    </div>


    <div *ngFor="let data of purchaseorder_list;">
        <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom">
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Ship Via</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.ship_via}}</p>

                </div>
            </div>


            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Payment Terms</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.payment_terms}}</p>

                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Freight Terms</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.freight_terms}}</p>

                </div>
            </div>



            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Delivery Location</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.delivery_location}}</p>

                </div>
            </div>
        </div>
    </div>

    <div *ngFor="let data of purchaseorder_list;">
        <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom">
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Currency</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.currency_code}}</p>

                </div>
            </div>

            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Shipping Address</b>
                <div class="col-lg-11 col-12 col-md-12 col-sm-12 fv-row mt-2">
                    <p>{{data.shipping_address}}</p>

                </div>
            </div>

            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>PO Cover Note</b>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <p>{{data.purchaseorder_reference}}</p>

                </div>
            </div>
            <div class="col-12 col-lg-3 col-md-6 col-sm-12"><b>Priority</b>
                <div class="d-flex justify-content-start mt-4 ms-2" >
                    <p>{{data.priority_n}}</p>
                </div>
            </div>
        </div>
    </div>

    <div class="d-flex flex-row m-6 flex-wrap">
        <div *ngFor="let data of purchaseorder_list;">
            <div class="col-12 col-lg-12 col-md-12 col-sm-12"><b>Remarks</b>
                <div class="col-12 col-lg-12 col-md-12 col-sm-12 fv-row mt-2">
                    <p>{{data.approver_remarks}}</p>

                </div>
            </div>
        </div>
    </div>


<!-- end:: PO Add Form -->


<!-- begin::table -->

<div class="table-responsive">
    <div *ngFor="let data of purchaseorder_list;">
        <table id="employee_list" class="table table-striped table-row-bordered gy-5 gs-7 ">
            <thead>
                <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                    <th>Product Group</th>
                    <th>Product Code</th>
                    <th>Product Name</th>
                    <th>Unit</th>
                    <th>Qty Ordered</th>
                    <th>Qty Recieved</th>
                    <th>Qty Adjusted</th>
                    <th>Unit Price</th>
                    <th>Discount(%)</th>
                    <th>Tax</th>
                    <th>Total Amount</th>
                    
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>{{data.productgroup_name}}</td>
                    <td>{{data.product_code}}</td>
                    <td>{{data.product_name}}</td>
                    <td>{{data.productuom_name}}</td>
                    <td>{{data.qty_ordered}}</td>
                    <td>{{data.qty_Received}}</td>
                    <td>{{data.qty_grnadjusted}}</td>
                    <td>{{data.product_price}}</td>
                    <td>{{data.discount_percentage}}</td>
                    <td>{{data.tax_gid}}&nbsp;&nbsp;: {{data.tax_amount}}
                        <br>{{data.tax_gid}}&nbsp;&nbsp;: {{data.tax_amount2}}
                        <br>{{data.tax_gid}}&nbsp;&nbsp;: {{data.tax_amount3}}
                    </td>
                    <td>{{data.total_amount}}</td>
                    
                </tr>
            </tbody>
        </table>
    </div>
</div></div>
<!-- end::table -->
<!-- end::Product Summary -->

<div class="card">

    <div class="me-10" *ngFor="let data of purchaseorder_list;">

        <div class="d-flex justify-content-end mt-4">
            
            <div class="col-md-3 d-flex justify-content-between mt-4 ">
                <label class="tbl_lab ">Net Amount:</label>
                <span class="tbl_spn ms-12">INR</span>
            </div>
        </div>

        <div class=" d-flex justify-content-end mt-4">
            <div class="col-md-3 d-flex justify-content-between mt-4">
                <label class="tbl_lab">Tax :</label>
                <span class="tbl_spn">{{data.tax_amount}}</span>&nbsp;&nbsp;<span class="tbl_spn text-end">INR</span>
            </div>
        </div>

        <div class="d-flex justify-content-end mt-4">
            <div class="col-md-3 d-flex justify-content-between mt-4">
                <label class="tbl_lab">Add On Charges :</label>
                <span class="tbl_spn">{{data.addon_amount}}</span><span class="tbl_spn ms-12">INR</span>
            </div>
        </div>
        <div class="d-flex justify-content-end mt-4">
            <div class="col-md-3 d-flex justify-content-between mt-4">
                <label class="tbl_lab">Additional Discount :</label>
                <span class="tbl_spn">{{data.discount_amount}}</span><span class="tbl_spn ms-12">INR</span>
            </div>
        </div>
        <div *ngFor="let data of purchaseorder_list;">
            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-3 d-flex justify-content-between mt-4">
                    <label class="tbl_lab">Freight Charges :</label>

                    <span class="tbl_spn">{{data.freighttax_amount}}</span><span class="tbl_spn ms-12">INR</span>
                </div>
            </div>
            <div class="d-flex justify-content-end mt-4 ">
                <div class="col-md-3 d-flex justify-content-between mt-4">
                    <label class="tbl_lab"> Freight Tax :</label>
                    <span class="tbl_spn">{{data.freighttax_amount}}</span><span class="tbl_spn ms-12">INR</span>
                </div>
            </div>

            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-3 d-flex justify-content-between mt-4">
                    <label class="tbl_lab">Buy Back/Scrap :</label>
                    <span class="tbl_spn">{{data.buybackorscrap}}</span><span class="tbl_spn ms-12">INR</span>
                </div>
            </div>
            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-3 d-flex justify-content-between mt-4">
                    <label class="tbl_lab">Packing Charges :</label>
                    <span class="tbl_spn">{{data.packing_charges}}</span><span class="tbl_spn ms-12">INR</span>
                </div>
            </div>
            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-3 d-flex justify-content-between mt-4">
                    <label class="tbl_lab">Insurance Charges :</label>
                    <span class="tbl_spn">{{data.insurance_charges}}</span><span class="tbl_spn ms-12">INR</span>
                </div>
            </div>
            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-3 d-flex justify-content-between mt-4">
                    <label class="tbl_lab">Round Off :</label>
                    <span class="tbl_spn">{{data.roundoff}}</span><span class="tbl_spn ms-12">INR</span>
                </div>
            </div>

            <div class="d-flex justify-content-end mt-4">
                <div class="col-md-3 d-flex justify-content-between mt-4">
                    <label class="tbl_lab">Grand Total :</label>
                    <span class="tbl_spn">{{data.total_amount}}</span><span class="tbl_spn ms-12">INR</span>
                </div>
            </div>

            <div class="d-flex justify-content-end mt-4">
                    <div class="col-md-3 d-flex justify-content-between mt-4">
                        <label class="tbl_lab col-lg-3">Payment :</label>
                        <span class="tbl_spn ms-3">{{data.payment_days}}</span>
                        <span class="tbl_spn ms-12">Days</span>
                    </div>
                </div>
                <div class="d-flex justify-content-end mt-4">
                    <div class="col-md-3 d-flex justify-content-between mt-4">
                        <label class="tbl_lab">Delivery Period :</label>
                        <span class="tbl_spn ">{{data.delivery_days}}</span><span class="tbl_spn ms-12">Days</span>
                    </div>
                </div>
            </div>
        </div>
    </div>

<div>
    <div class="card-header bg-lightblue card-height55">
        <div class="card-title m-0">
            <h5 class="fw-bolder text-blue">Terms and Conditions</h5>
        </div>
    </div>
    <div *ngFor="let data of purchaseorder_list;">
        <div class="d-flex flex-row m-6 flex-wrap">
            <div class="col-12 col-lg-12 col-md-12 col-sm-12 fv-row mt-2">
                <angular-editor [config]="config"
                    formControlName="template_content">{{data.termsandconditions}}</angular-editor>
            </div>
        </div>
    </div>
</div>

<!-- begin::Submit button -->
<div class="text-center my-4">
    <button class="btn-primary btn-sm text-white me-4 mt-3-center " routerLink="/pmr/PmrTrnPurchaseorderSummary">
        &nbsp;Back
    </button>
</div>
<!-- end::Submit button -->

<!-- end::Add PO -->