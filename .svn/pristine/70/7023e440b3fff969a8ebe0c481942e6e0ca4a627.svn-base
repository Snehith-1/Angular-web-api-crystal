<div class="card">
  <div class="row ms-3 mt-3 d-flex align-items-center">
    <div class="col">
      <h3 class="card-title">Register Lead - Corporate</h3>
    </div>

    <div class="col me-5">
      <button title="Add Lead" class="btn-primary btn-sm text-white me-4 float-end" (click)="onadd()" >
        <i class="fas fa-plus"></i> &nbsp;&nbsp;&nbsp; Add Lead
      </button>
    </div>
  </div>
  <br/>
    
    <div class="row"> 
      <!-- ADD Function -->
      <!-- <div class="col-lg-2 col-md-2 col-sm-2" style="width:170px;">
        <br/>
      <button title="Add Lead" class="btn-primary btn-sm text-white me-4" (click)="onadd()" >
        <i class="fas fa-plus"></i> &nbsp;&nbsp;&nbsp; Add Lead
      </button>
    </div> -->
      <div class="col-lg-3 col-md-3 col-sm-3" routerLink="/crm/CrmTrnLeadMasterSummary">
        <div class="cardHead radius-10 border-start border-0 border-3 border-violet">
           <div class="card-body12">
               <div class="d-flex align-items-center">
                   <div>
                    <h5 class="mb-0 text-secondary">Distributor</h5>
                    <div *ngFor="let RegisterLeadCount_Distributor of RegisterLeadCountList">
                        <h4 class="my-1 text-info">{{RegisterLeadCount_Distributor.distributor_count}}</h4>
                        </div>
                   </div>
                   <div class="widgets-icons-2 rounded-circle bg-gradient-violet text-white ms-auto"><i class="bi bi-calendar3"></i>
                   </div>
               </div>
           </div>
        </div>
      </div>
      <div class="col-lg-3 col-md-3 col-sm-3" routerLink="/crm/CrmTrnCorporateRegisterLead">
        <div class="cardHead radius-10 border-start border-0 border-3 border-brown" style="background-color: #f5d4cf;">
           <div class="card-body12">
               <div class="d-flex align-items-center">
                   <div>
                    <h5 class="mb-0 text-secondary"> Corporate </h5>
                    <div *ngFor="let RegisterLeadCount_Corporate of RegisterLeadCountList">
                        <h4 class="my-1 text-info">{{RegisterLeadCount_Corporate.corporate_count}}</h4>
                    </div>
                   </div>
                   <div class="widgets-icons-2 rounded-circle bg-gradient-brown text-white ms-auto"><i class="bi bi-arrow-up-right"></i>
                   </div>
               </div>
           </div>
        </div>
      </div>
      <div class="col-lg-3 col-md-3 col-sm-3" routerLink="/crm/CrmTrnRetailerRegisterLead">
        <div class="cardHead radius-10 border-start border-0 border-3 border-darkgreen">
           <div class="card-body12">
               <div class="d-flex align-items-center">
                   <div>
                    <h5 class="mb-0 text-secondary">Retailer</h5>
                    <div *ngFor="let RegisterLeadCount_Retailer of RegisterLeadCountList">
                        <h4 class="my-1 text-info">{{RegisterLeadCount_Retailer.retailer_counts}}</h4>
                        </div>
                   </div>
                   <div class="widgets-icons-2 rounded-circle bg-gradient-darkgreens text-white ms-auto"><i class="bi bi-asterisk"></i>
                   </div>
               </div>
           </div>
        </div>
      </div>
      <div class="col-lg-3 col-md-3 col-sm-3">
        <div class="cardHead radius-10 border-start border-0 border-3 border-darkorange">
          <div class="card-body12">
              <div class="d-flex align-items-center">
               <div>
                   <h5 class="mb-0 text-secondary">Total Leads</h5>
                   <div *ngFor="let RegisterLeadCount_total of RegisterLeadCountList">
                       <h4 class="my-1 text-info">{{RegisterLeadCount_total.total_count}}</h4>
                       </div>
                  </div>
                  <div class="widgets-icons-2 rounded-circle bg-gradient-purple text-white ms-auto"><i class="bi bi-browser-edge"></i>
                  </div>
              </div>
          </div>
       </div>
      </div>
    </div>
    <div class="table-responsive">
        <div class="card card-body m-3 shadow-xl p-3 mb-5 bg-white rounded">
          <table id="GetRegisterLeadSummary_list1" class="table table-striped table-row-bordered border-bottom gs-7">
            <thead>
              <tr class="fw-bolder fs-5 text-nowrap bg-lightblue text-blue align-middle">
                <th>S.No</th>
                <th>Created Date </th>
                <th>Company </th>
                <th>Contact </th>
                <th>Region</th>
                <!-- <th>Customer Type</th> -->
                <th>Created By</th>
                <th>Remarks</th>
                <th>Status</th>
                <th>Assigned To</th>              
                <th class="text-center">Action</th>
              </tr>
            </thead>
            <tbody>
              <tr *ngFor="let data of GetRegisterLeadSummary_list1; let i=index;" class="align-middle">
                <td>{{i+1}}</td>
                <td>{{data.created_date}}</td>
                <td>{{data.leadbank_name}}</td>
                <td>{{data.contact_details}}</td>
                <td>{{data.region_name}}</td>
                <!-- <td>{{data.customer_type}}</td> -->
                <td>{{data.created_by}}</td>
                <td>{{data.remarks}}</td>
                <td>{{data.lead_status}}</td>
                <!-- <td>{{data.assign_to}}</td> -->
                <!-- <td>{{data.approval_flag}}</td> -->
                <td>{{data.assignedto}}</td>
                <!-- <td>{{data.leadbank_gid}}</td> -->
                <!-- <td>{{data.approval_flag}}</td> -->
                <!-- <td>{{data.agent_name}}</td> -->
  
                <td class="text-center text-nowrap">
    
                 <button title="View" type="button" class="btn btn-icon btn-sm bg-primary me-2" (click)="onview(data.leadbank_gid)" routerLink="/crm/CrmTrnLeadbankview"><span class="fa-solid fa-eye text-white  fs-6"></span></button>
                  <button title="Edit" type="button" class="btn btn-icon btn-sm bg-success me-2" (click)="onedit(data.leadbank_gid)" routerLink="/crm/CrmTrnLeadbankedit"><span class="fa-solid fa-pencil text-white  fs-6"></span></button>      
                  <button title="Delete" type="button" (click)="openModaldelete(data.leadbank_gid)" data-bs-toggle="modal" data-bs-target="#myModaldelete"class="btn btn-icon btn-sm bg-danger me-2"><span
                    class="fa-solid fa-trash text-white  fs-6"></span></button>
                    <!-- <button title=" Branch" type="button" (click)="onbranch(data.country_gid,data.region_gid,data.leadbank_gid)" class="btn btn-icon btn-sm bg-warning me-2"><span class="fas fa-code-branch text-white fs-6"routerLink="/crm/CrmTrnLeadBankbranch"></span> </button>
                    <button title="Contact" type="button" class="btn btn-icon btn-sm bg-info me-2">
                      <span class="fas fa-phone text-white fs-6" (click)="oncontact(data.leadbank_gid)" routerLink="/crm/CrmTrnLeadbankcontact"></span></button> -->
  <!-- (click)="onedit(data.leadmaster_gid)"  -->
       
             </tr>
            </tbody>
          </table>
        </div>
      </div>   
      
  <div class="modal" id="myModaldelete" data-bs-backdrop="static" data-bs-keyboard="false">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="text-center my-4">
            <h2><b>Are you sure?</b></h2>
            <br>
            <p style="display: block;">Do You Want To Delete the Record ?</p>
            <br>
          <div class="text-center my-4">
            <button class="btn-primary btn-sm text-white me-2"  data-bs-dismiss="modal">
                <i class="fas fa-close text-white"></i> &nbsp;Close
                </button>&nbsp;
            <button class="btn-danger btn-sm text-white me-4" (click)="ondelete()"  data-bs-dismiss="modal" >          
                <i class="fas fa-trash text-white fs-5 "></i> &nbsp;Delete
            </button>
           
            <!-- (click)="ondelete()"  -->
          </div>
        </div>
        </div>
      </div>
    </div>
 