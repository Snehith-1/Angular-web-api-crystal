<div class="card m-3">
    <!-- begin::Heading -->
    <div class="card-header bg-lightblue card-height55">
        <div class="card-title m-0">
            <h5 class="fw-bolder text-blue">E-Invoice</h5>
        </div>
    </div>
    <!-- end::Heading -->
    <div class="card m-3">
        <form [formGroup]="einvoicedtlform"(ngSubmit)="submit()">
            <div class="card_heading">
                <h6>Transaction Details</h6>
            </div>
            <div class="d-flex flex-row m-6 flex-wrap">
                <div class="col-12 col-lg-6 col-md-6 col-sm-12 left">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Tax Scheme<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="left_input" formControlName="transactionDetails_TaxSch">
                                <div *ngIf="transdlttaxschemeControl.invalid && transdlttaxschemeControl.touched">
                                    <div *ngIf="transdlttaxschemeControl.hasError('required')"
                                        style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                        * Tax scheme is required.
                                    </div>
                                </div>
                                
                            </div>
                            <input formControlName="invoice_gid" hidden
    placeholder="Address Line 1"  type="text" class="
        form-control form-control-lg form-control-solid
        bg-white
        custom-input
        mb-3 mb-lg-0
        borderc-secondary
      " name="invoice_gid" />
      <input formControlName="invoicerefno" hidden
    placeholder="Address Line 1"  type="text" class="
        form-control form-control-lg form-control-solid
        bg-white
        custom-input
        mb-3 mb-lg-0
        borderc-secondary
      " name="invoicerefno" />
      
                        </div>
<!-- 
                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">e-comm GSTIN<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="left_input" formControlName="transactionDetails_ecomm_gstin">
                                <div *ngIf="transdltecommgstinControl.invalid && transdltecommgstinControl.touched">
                                    <div *ngIf="transdltecommgstinControl.hasError('required')"
                                        style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                        * E-comm gst is required.
                                    </div>
                                </div>
                            </div>
                        </div> -->

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">IGST on Intra Charges</label>
                            </div>
                            <div class="col-lg-3">
                                <select class="options" formControlName="transactionDetails_IgstOnIntra">
                                    <option value="">Select Charges</option>
                                    <option value="Y">Yes</option>
                                    <option value="N">No</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Transaction Category<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="left_input"
                                    formControlName="transactionDetails_SupTyp">
                                <div *ngIf="transdlttranscatControl.invalid && transdlttranscatControl.touched">
                                    <div *ngIf="transdlttranscatControl.hasError('required')"
                                        style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                        * Transaction category is required.
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Reverse Charge</label>
                        </div>

                        <div class="col-lg-3">
                            <select class="options" formControlName="transactionDetails_RegRev">
                                <option value="">Select Charges</option>
                                <option value="Y">Yes</option>
                                <option value="N">No</option>
                            </select>
                        </div>
                    </div>

                    
                </div>
                <div class="col-12 col-lg-6 col-md-6 col-sm-12">

                <div class="d-flex flex-row mt-4">
                    <div class="col-lg-3">
                        <label class="left_label">Transaction Type</label>
                    </div>
                    <div class="col-lg-3">
                        <select class="options" formControlName="transactionDetails_tranaction_type">
                            <option value="">Select Type</option>
                            <option value="Regular">Regular</option>
                            <option value="Ship To">Ship To</option>
                            <option value="Ship From">Ship From</option>
                            <option value="Both 2 and 3">Both 2 and 3</option>
                        </select>
                    </div>
                </div>
                </div>
            </div>

            <div class="card_heading">
                <h6>Document Details</h6>
            </div>
            <div class="d-flex flex-row m-6 flex-wrap">
                <div class="col-12 col-lg-6 col-md-6 col-sm-12 left">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Document Type</label>
                            </div>
                            <div class="col-lg-3">
                                <select name="" id="" class="options" formControlName="documentDetails_Typ">
                                    <option value="">Select Type</option>
                                    <option value="INV">Invoice</option>
                                    <option value="Credit Note">Credit Note</option>
                                    <option value="Debit Note">Debit Note</option>
                                </select>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Invoice Date</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="left_input bg-gray-300" formControlName="invoice_date"
                                    readonly>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Invoice No</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="left_input bg-gray-300" formControlName="invoicerefno"
                                    readonly>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Payment Term (In Days)</label>
                        </div>
                        <div class="col-lg-3 fo_read">
                            <input type="text" class="left_input bg-gray-300"
                                formControlName="documentDetails_payment_term" readonly>
                        </div>
                        <div class="col-lg-3">
                            <label style="margin-left: 105px; font-size: 12px; font-weight: bold; width: 80px;">Due
                                Date</label>
                        </div>
                        <div class="col-lg-3 fo_read">
                            <input type="text"
                                style="margin-left: 35px; font-size: 12px; width: 80px; background-color: #e1e3ea;"
                                formControlName="documentDetails_due_date" readonly>
                        </div>
                    </div>
                </div>
            </div>

            <div class="card_heading">
                <h6>Buyer Details</h6>
            </div>
            <div class="d-flex flex-row m-6 flex-wrap">
                <div class="col-12 col-lg-6 col-md-6 col-sm-12 left">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Customer GSTIN</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="left_input bg-gray-300" formControlName="buyerDetails_Gstin"
                                    readonly>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Company Name</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="left_input bg-gray-300"
                                    formControlName="buyerDetails_LglNm" readonly>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Contact Number</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="left_input bg-gray-300"
                                    formControlName="buyerDetails_cont_num" readonly>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Customer Address</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <textarea type="text" class="left_input1 bg-gray-300" formControlName="buyerDetails_Address"
                                readonly rows="1"></textarea>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Pincode</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="left_input bg-gray-300" formControlName="buyerDetails_Pin"
                                    readonly>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Remarks</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="left_input bg-gray-300" formControlName="buyerDetails_remarks"
                                    readonly>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Buyer POS</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="left_input bg-gray-300" formControlName="buyerDetails_Pos"
                                    >
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Contact Person</label>
                        </div>
                        <div class="col-lg-3 fo_read">
                            <input type="text" class="left_input bg-gray-300" formControlName="buyerDetails_cont_person"
                                readonly>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Email Address</label>
                        </div>
                        <div class="col-lg-3 fo_read">
                            <input type="text" class="left_input bg-gray-300" formControlName="buyerDetails_email"
                                readonly>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Location</label>
                        </div>
                        <div class="col-lg-3 fo_read">
                            <input type="text" class="left_input bg-gray-300" formControlName="buyerDetails_Loc"
                                readonly>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">State</label>
                        </div>
                        <div class="col-lg-3 fo_read">
                            <input type="text" class="left_input bg-gray-300" formControlName="buyerDetails_Stcd"
                                readonly>
                        </div>
                    </div>
                </div>
            </div>

            <div class="card_heading">
                <h6>Seller Details</h6>
            </div>
            <div class="d-flex flex-row m-6 flex-wrap">
                <div class="col-12 col-lg-6 col-md-6 col-sm-12 left">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Seller GSTIN<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="left_input" formControlName="sellerDetails_Gstin">
                                <div
                                    *ngIf="sellerDetails_sellergstinControl.invalid && sellerDetails_sellergstinControl.touched">
                                    <div *ngIf="sellerDetails_sellergstinControl.hasError('required')"
                                        style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                        * Seller gst is required.
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Trade Name<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="left_input" formControlName="sellerDetails_TrdNm">
                                <div
                                    *ngIf="sellerDetails_tradenameControl.invalid && sellerDetails_tradenameControl.touched">
                                    <div *ngIf="sellerDetails_tradenameControl.hasError('required')"
                                        style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                        * Seller trade name is required.
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Location</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="left_input bg-gray-300" formControlName="sellerDetails_Loc"
                                    readonly>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">State</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="left_input bg-gray-300" formControlName="sellerDetails_Stcd"
                                    >
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Email Address</label>
                            </div>
                            <div class="col-lg-3 fo_read">
                                <input type="text" class="left_input bg-gray-300" formControlName="sellerDetails_email"
                                    readonly>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Legal Name<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="left_input" formControlName="sellerDetails_LglNm">
                                <div
                                    *ngIf="sellerDetails_legalnameControl.invalid && sellerDetails_legalnameControl.touched">
                                    <div *ngIf="sellerDetails_legalnameControl.hasError('required')"
                                        style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                        * Seller legal name is required.
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Address</label>
                        </div>
                        <div class="col-lg-3 fo_read">
                                <textarea type="text" class="left_input1 bg-gray-300" formControlName="sellerDetails_Address"
                                 rows="1" style="border: #000000 solid 1px; background-color: #ffffff;"></textarea>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Pincode<span style="color: red"> *</span></label>
                        </div>
                        <div class="col-lg-3">
                            <input type="text" class="left_input" formControlName="sellerDetails_Pin">
                            <div *ngIf="sellerDetails_pincodeControl.invalid && sellerDetails_pincodeControl.touched">
                                <div *ngIf="sellerDetails_pincodeControl.hasError('required')"
                                    style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                    * Seller pincode is required.
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Contact Number</label>
                        </div>
                        <div class="col-lg-3 fo_read">
                            <input type="text" class="left_input bg-gray-300" formControlName="sellerDetails_cont_num"
                                readonly>
                        </div>
                    </div>
                </div>
            </div>

            <div class="card_heading">
                <h6>Dispatch Details</h6>
            </div>
            <div class="d-flex flex-row m-6 flex-wrap">
                <div class="col-12 col-lg-6 col-md-6 col-sm-12 left">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Name<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="left_input" formControlName="dispatchDetails_Nm">
                                <div *ngIf="dispatchDetails_nameControl.invalid && dispatchDetails_nameControl.touched">
                                    <div *ngIf="dispatchDetails_nameControl.hasError('required')"
                                        style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                        * Name is required.
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Location<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="left_input" formControlName="dispatchDetails_Loc">
                                <div
                                    *ngIf="dispatchDetails_locationControl.invalid && dispatchDetails_locationControl.touched">
                                    <div *ngIf="dispatchDetails_locationControl.hasError('required')"
                                        style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                        * Dispatch location is required.
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">State<span style="color: red"> *</span></label>
                            </div>

                            <div class="col-lg-3">
                                <input type="text" class="left_input" formControlName="dispatchDetails_Stcd">
                                <div
                                    *ngIf="dispatchDetails_stateControl.invalid && dispatchDetails_stateControl.touched">
                                    <div *ngIf="dispatchDetails_stateControl.hasError('required')"
                                        style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                        * Dispatch state is required.
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Address<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <textarea type="text" class="left_input1 bg-gray-300" formControlName="dispatchDetails_Address"
                                rows="1" style="border: #000000 solid 1px; background-color: #ffffff;"></textarea>
                                <div
                                    *ngIf="dispatchDetails_addressControl.invalid && dispatchDetails_addressControl.touched">
                                    <div *ngIf="dispatchDetails_addressControl.hasError('required')"
                                        style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                        * Dispatch address is required.
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Pincode<span style="color: red"> *</span></label>
                        </div>
                        <div class="col-lg-3">
                            <input type="text" class="left_input" formControlName="dispatchDetails_Pin">
                            <div
                                *ngIf="dispatchDetails_pincodedisControl.invalid && dispatchDetails_pincodedisControl.touched">
                                <div *ngIf="dispatchDetails_pincodedisControl.hasError('required')"
                                    style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                    * Dispatch pincode is required.
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="card_heading">
                <h6>Shipping To Details</h6>
            </div>
            <div class="d-flex flex-row m-6 flex-wrap">
                <div class="col-12 col-lg-6 col-md-6 col-sm-12 left">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">GSTIN<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="left_input" formControlName="shipDetails_Gstin">
                                <div *ngIf="shipDetails_gstinControl.invalid && shipDetails_gstinControl.touched">
                                    <div *ngIf="shipDetails_gstinControl.hasError('required')"
                                        style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                        * Shipping gst is required.
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Trade Name<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="left_input" formControlName="shipDetails_TrdNm">
                                <div
                                    *ngIf="shipDetails_tradenameshipControl.invalid && shipDetails_tradenameshipControl.touched">
                                    <div *ngIf="shipDetails_tradenameshipControl.hasError('required')"
                                        style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                        * Shipping trade name is required.
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">Location<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="left_input" formControlName="shipDetails_Loc">
                                <div
                                    *ngIf="shipDetails_locationshipControl.invalid && shipDetails_locationshipControl.touched">
                                    <div *ngIf="shipDetails_locationshipControl.hasError('required')"
                                        style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                        * Shipping location is required.
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="d-flex flex-row mt-4">
                            <div class="col-lg-3">
                                <label class="left_label">State<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="left_input" formControlName="shipDetails_Stcd">
                                <div
                                    *ngIf="shipDetails_stateshipControl.invalid && shipDetails_stateshipControl.touched">
                                    <div *ngIf="shipDetails_stateshipControl.hasError('required')"
                                        style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                        * Shipping state is required.
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                    <div class="col-lg-12 col-12 col-md-12 col-sm-12 fv-row">
                        <div class="d-flex flex-row">
                            <div class="col-lg-3">
                                <label class="left_label">Legal Name<span style="color: red"> *</span></label>
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="left_input" formControlName="shipDetails_LglNm">
                                <div
                                    *ngIf="shipDetails_legalnameshipControl.invalid && shipDetails_legalnameshipControl.touched">
                                    <div *ngIf="shipDetails_legalnameshipControl.hasError('required')"
                                        style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                        * Shipping legal name is required.
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Address<span style="color: red"> *</span></label>
                        </div>
                        <div class="col-lg-3">
                            <textarea type="text" class="left_input1 bg-gray-300" formControlName="shipDetails_Address"
                                rows="1" style="border: #000000 solid 1px; background-color: #ffffff;"></textarea>
                            <div *ngIf="shipDetails_addressControl.invalid && shipDetails_addressControl.touched">
                                <div *ngIf="shipDetails_addressControl.hasError('required')"
                                    style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                    * Shipping address is required.
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex flex-row mt-4">
                        <div class="col-lg-3">
                            <label class="left_label">Pincode<span style="color: red"> *</span></label>
                        </div>
                        <div class="col-lg-3">
                            <input type="text" class="left_input" formControlName="shipDetails_Pin">
                            <div *ngIf="shipDetails_pincodeControl.invalid && shipDetails_pincodeControl.touched">
                                <div *ngIf="shipDetails_pincodeControl.hasError('required')"
                                    style="color: red; width: 200px; margin-top: 1px; margin-left: 25px; font-size: 11px;">
                                    * Shipping pincode is required.
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- begin::table -->
        <div class="table-responsive">
            <div class="card card-body m-0 shadow-lg p-3 mb-5 bg-white rounded">
                <table id="product" class="table table-striped table-row-bordered gy-4 gs-2">
                    <thead class="table_head">
                      <tr class="text-white fw-bold fs-5 text-nowrap text-blue">
                        <th>S.No</th>
                        <th>Product Name</th>
                        <th>Product Group</th>
                        <th>Product Code</th>
                        <th>HSN Code / Description</th>
                        <th>Unit</th>
                        <th style="text-align: right;">Unit Price</th>
                        <th style="text-align: right;">MRP</th>
                        <th style="text-align: center;">Quantity</th>
                        <th style="text-align: right;">Discount (%)</th>
                        <th style="text-align: center; width: 120px;">Tax</th>
                        <th style="text-align: right;">Total Amount</th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr *ngFor="let data of invoiceproductlist; let i=index;">
                        <td>{{i+1}}</td>
                        <td>{{data.product_name}}</td>
                        <td>{{data.productgroup_name}}</td>
                        <td>{{data.product_code}}</td>
                        <td>{{data.hsn}}</td>
                        <td>{{data.uom_name}}</td>
                        <td style="text-align: right;">{{data.product_price | number}}</td>
                        <td style="text-align: right;">{{data.selling_price | number}}</td>
                        <td style="text-align: center;">{{data.qty_invoice}}</td>
                        <td style="text-align: right;">{{data.discount}}</td>
                        <td style="text-align: right;">{{data.tax}}</td>
                        <td style="text-align: right;">{{data.product_total | number}}</td>
                        <td class="text-center text-nowrap">
                        </td>
                      </tr>
                    </tbody>
                  </table>
            </div>
        </div>
        
        <!-- end::table -->

        <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4 amt_in">
                <label class="lab_name">Net Amount :</label>
                <input type="text" class="in"  placeholder="0.00" formControlName="netamount">
                <span class="spn">INR</span>
            </div>
        </div>
        <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4 amt_in">
                <label class="lab_name">Addon Amount :</label>
                <input type="text" class="in"  placeholder="0.00"formControlName="addoncharges">
                <span class="spn">INR</span>
            </div>
        </div>
        <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4 amt_in">
                <label class="lab_name">Additional Discount :</label>
                <input type="text" class="in"  placeholder="0.00"formControlName="invoicediscountamount">
                <span class="spn">INR</span>
            </div>
        </div>
        <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4 amt_in">
                <label class="lab_name">Freight Charges :</label>
                <input type="text" class="in"  placeholder="0.00"formControlName="frieghtcharges">
                <span class="spn">INR</span>
            </div>
        </div>
        <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4 amt_in">
                <label class="lab_name">Buyback Charges :</label>
                <input type="text" class="in"  placeholder="0.00"formControlName="buybackcharges">
                <span class="spn">INR</span>
            </div>
        </div>
        <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4 amt_in">
                <label class="lab_name">Packing/Forwarding Charges :</label>
                <input type="text" class="in" readonly placeholder="0.00"formControlName="packing_charges">
                <span class="spn">INR</span>
            </div>
        </div>
        <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4 amt_in">
                <label class="lab_name">Insurance Charges :</label>
                <input type="text" class="in"  placeholder="0.00"formControlName="insurancecharges">
                <span class="spn">INR</span>
            </div>
        </div>
        <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4 amt_in">
                <label class="lab_name">Round Off :</label>
                <input type="text" class="in"  placeholder="0.00"formControlName="roundoff">
                <span class="spn">INR</span>
            </div>
        </div>
        <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4 amt_in">
                <label class="lab_name">Grand Total :</label>
                <input type="text" class="in" readonly placeholder="0.00"formControlName="invoice_amount">
                <span class="spn">INR</span>
            </div>
        </div>
        </form>
    </div>

    <div class="card" m-3>
        <div class="text-center my-4">
            <button type="submit" class="btn-success btn-sm text-white me-4"(click)="submit()">
                <i class="bi bi-check-square fs-7 me-1"></i>
                Submit
            </button>

            <button class="btn-primary btn-sm text-white me-4"(click)="back()">
                <i class="bi bi-arrow-left-square fs-7 me-1"></i>
                Back
            </button>
        </div>
    </div>
</div>