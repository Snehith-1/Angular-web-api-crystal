<div class="card shadow-sm">
  <div class="card-header bg-lightblue card-height55">
  <div class="card-title m-0">
    <h3 class="fw-bolder text-blue">RAISE SALES ORDER</h3>
  </div>
  </div><br>
  <form [formGroup]="OrderForm" (ngSubmit)="onSubmit()">

    <div class="d-flex flex-row m-6 flex-wrap">
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>SO Ref. No</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="New Purchase Order" 
             formControlName="quotation_gid"
              class="form-control" />
        </div>
      </div>

      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>SO Date</b><span class="ms-1 text-red">*</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="date"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input date-picker"
             formControlName="salesorder_date"
              class="form-control" />
        </div>
      </div>
      <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Branch Name<span class="ms-1 text-red">*</span></b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">

        <input class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
        readonly  formControlName="branch_name"
          placeholder="Auto Populate" />
      </div>
    </div>
    <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>QO Ref. No</b>
      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
        <input type="text"
          class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
          placeholder="New Purchase Order"
           readonly
          formControlName="quotation_referenceno1" 
          class="form-control" />
      </div>
    </div>
    </div>
<div class="d-flex flex-row m-6 flex-wrap">
  <div class="col-12 col-lg-3 col-md-6 col-sm-12"><b>Customer Name<span class="ms-1 text-red">*</span></b>
    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
    <input  class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
     formControlName="customer_name"
      readonly 
      placeholder="Auto Populate">
    </div></div>

    <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Customer Ref. No</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="New Purchase Order"
            formControlName="so_referencenumber" 
            class="form-control" />
        </div>
      </div>

  <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Contact Person<span class="ms-1 text-red">*</span></b>
    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
      <input type="text"
      class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
      placeholder="Auto Populate" readonly formControlName="customercontact_names" class="form-control" />
    </div>
  </div>

  <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Contact Number</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text"
            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
            placeholder="Auto Populate" 
            formControlName="customer_mobile" 
            class="form-control" />
        </div>
      </div>
</div>
<div class="d-flex flex-row m-6 flex-wrap">

  <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Email Address</b>
    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
      <input type="text"
        class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
        placeholder="Auto Populate"
        formControlName="customer_email" 
        class="form-control" />
    </div>
  </div>

<div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Customer Address</b>
<div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
  <textarea type="text" rows="1"
    class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
    placeholder="Auto Populate" 
    formControlName="customer_address" 
    class="form-control">
  </textarea>
</div>
</div>

<div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Remarks</b>
<div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
<textarea type="text" rows="1"
  class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
  placeholder="Auto Populate" 
  readonly 
  formControlName="so_remarks" 
  class="form-control" >
</textarea>
</div>
</div>

<div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Shipping  Address</b>
<div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
  <textarea type="text" rows="1"
    class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
    placeholder="Shipping Address" 
    readonly 
    formControlName="shipping_to" 
    class="form-control">
  </textarea>
</div>
</div>
</div>
<div class="d-flex flex-row m-6 flex-wrap">
  <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Sales Person<span class="ms-1 text-red">*</span></b>
    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
  <ng-select [items]="user_list" class="
  custom
  col-lg-12 col-12
  fv-row
  mt-2"
   bindValue="user_gid" 
   bindLabel="user_name"
   formControlName="user_name" 
   placeholder="Select an option">
</ng-select>
</div>
  </div>

  <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Expected Start Date</b>
    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
      <input type="date"
        class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input date-picker"
       formControlName="start_date" 
       class="form-control" />
    </div>
  </div>


  <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Esimated Time of Arraival</b>
    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
      <input type="date"
        class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input date-picker"
         formControlName="end_date" 
         class="form-control" />
    </div>
  </div>
  <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Freight Terms</b>
    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
      <input type="text"
        class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
        placeholder="Freight terms" 
        formControlName="freight_terms" 
        class="form-control" />
    </div>
  </div>
</div>


<div class="d-flex flex-row m-6 flex-wrap">

  <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Payment Terms</b>
  <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
    <input type="text"
      class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
      placeholder="Payment Terms" 
      formControlName="payment_terms" 
      class="form-control" />
  </div>
</div>
    <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Currency<span class="ms-1 text-red">*</span></b>
      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
    <ng-select [items]="currency_list" class="
    custom
    col-lg-12 col-12
    fv-row
    mt-2 "
      bindValue="currencyexchange_gid"
      bindLabel="currency_code"
      formControlName="currency_code" 
      placeholder="Select an option"
      (change)="OnChangeCurrency($event)" 
      (change)="onCurrencyCodeChange($event)"></ng-select>
  </div>
    </div>
    <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Exchane Rate</b>
      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
        <input type="text"
          class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
          placeholder="Exchange Rate" 
          formControlName="exchange_rate" 
          class="form-control" />
      </div>
    </div>
</div>

<!-- end:: PO Add Form -->
    
        <!-- begin::Product Summary -->
  
       
          <!-- begin::Heading -->
          <div class="card-header bg-lightblue card-height55" style="border-radius: 0px;">
            <div class="card-title m-0">
              <h5 class="fw-bolder text-blue">PRODUCT SUMMARY</h5>
            </div>
          </div>
  
          <form [formGroup]="productform" (ngSubmit)="OnSubmit()">
            <div class="d-flex flex-row m-6 flex-wrap" style="border-radius: 0px;">
              <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Product Name<span class="ms-1 text-red">*</span></b>
                <div class="col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2">
          
                  <ng-select [items]="product_list" class="
                  custom
                col-lg-11 col-12
                fv-row
                mt-2
              " bindLabel="product_name" 
              (change)="GetOnChangeProductName()"
               formControlName="product_name" 
                placeholder="Select an option" >
              </ng-select>
                </div>
              </div>

              <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Product Group</b>
                <div class="col-lg-10 col-12 col-md-11 col-sm-11 fv-row mt-2">
                  <input type="text" class="form-control"
                   formControlName="productgroup_name" 
                   readonly
                    style="background-color: #e9e7e7">
                </div>
              </div>
              <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Customer Product Code</b>
                  <div class="col-lg-10 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <input type="text"
                     class="form-control"
                      formControlName="customerproduct_code" />
                  </div>
                </div>
              <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Product Code</b>
                <div class="col-lg-10 col-12 col-md-11 col-sm-11 fv-row mt-2">
                  <input type="text" class="form-control" formControlName="product_code" readonly
                    style="background-color: #e9e7e7">
                </div>
              </div>
            </div>

            <div class="d-flex flex-row m-6 flex-wrap">
            
              <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Unit</b>
                <div class="col-lg-10 col-12 col-md-11 col-sm-11 fv-row mt-2">
                  <input type="text" 
                   class="form-control" 
                   formControlName="productuom_name" 
                    readonly
                    style="background-color: #e9e7e7;">
                </div>
              </div>
              <div class="col-12 col-lg-3 col-md-3 col-sm-12">
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row">
                  <div class="d-flex flex-row m-0 flex-wrap">
                    <div class="col-lg-6 col-md-6 col-sm-6"><b>Unit Price</b>
                      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                        <input type="text"
                         class="form-control" 
                         placeholder="0.00"
                          style="text-align: right;"
                          formControlName="product_price"  
                          (input)="prodtotalcal()">
                      </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6"><b>Quantity</b>
                      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                        <input type="text"
                         class="form-control"
                          style="text-align: right;"
                           formControlName="qty_quoted"
                           (input)="prodtotalcal()">
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-12 col-lg-3 col-md-3 col-sm-12">
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row">
                  <div class="d-flex flex-row m-0 flex-wrap">
                    <div class="col-lg-6 col-md-6 col-sm-6"><b>Discount(%)</b>
                      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                        <input type="text" class="form-control" placeholder="0.00" style="text-align: right;"
                          formControlName="margin_percentage"  (input)="prodtotalcal()">
                      </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6">
                      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
                        <input type="text" class="form-control" placeholder="0.00" readonly
                          style="background-color: #e9e7e7; text-align: right;" formControlName="margin_amount"
                            (input)="prodtotalcal()">
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>MRP</b>
                <div class="col-lg-10 col-12 col-md-11 col-sm-11 fv-row mt-2">
                  <input type="text" class="form-control" formControlName="selling_price" readonly
                    style="background-color: #e9e7e7">
                </div>
              </div>
             
            </div>

            <div class="d-flex flex-row m-6 flex-wrap">
              <div class="col-12 col-lg-3 col-md-3 col-sm-12">
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row">
                  <div class="d-flex flex-row m-0 flex-wrap">
                    <div class="col-lg-6 col-md-6 col-sm-6"><b>Need By Date</b>
                      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                        <input type="text" class="form-control" placeholder="0.00" style="text-align: right;"
                          formControlName="product_requireddate">
                      </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6">
                      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
                        <textarea type="text" class="form-control" rows="1" readonly
                          style="background-color: #e9e7e7; text-align: right;" placeholder="Description" formControlName="product_requireddateremarks"></textarea>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            
              <div class="col-12 col-lg-3 col-md-3 col-sm-12">
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row">
                  <div class="d-flex flex-row m-0 flex-wrap">           
                    <div class="col-lg-6 col-md-6 col-sm-6"><b>Tax 1</b>
                      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                
                        <ng-select [items]="tax_list" class="
                        custom
                col-lg-10 col-12
                fv-row
                mt-2
              " 
              bindLabel="tax_name" formControlName="tax_name" bindValue="tax_gid" 
              
               (change)="OnChangeTaxAmount1()" placeholder="Select Tax" [(ngModel)]="mdlTaxName1"></ng-select>
                      </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6">
                      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
                        <input type="text" class="form-control" placeholder="0.00" readonly
                          style="text-align: right; background-color: #e9e7e7;" formControlName="tax_amount"
                           (input)="OnChangeTaxAmount1()">
                      </div>
                    </div>
                  </div>
                </div>
              </div>

              <div class="col-12 col-lg-3 col-md-3 col-sm-12">
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row">
                  <div class="d-flex flex-row m-0 flex-wrap">           
                    <div class="col-lg-6 col-md-6 col-sm-6"><b>Tax 2</b>
                      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                
                        <ng-select [items]="tax2_list" class="
                        custom
                col-lg-10 col-12
                fv-row
                mt-2
              " 
              bindLabel="tax_name2" formControlName="tax_name2" bindValue="tax_gid" 
              
               (change)="OnChangeTaxAmount2()" placeholder="Select Tax" [(ngModel)]="mdlTaxName2"></ng-select>
                      </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6">
                      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
                        <input type="text" class="form-control" placeholder="0.00" readonly
                          style="text-align: right; background-color: #e9e7e7;" formControlName="tax_amount2"
                           (input)="OnChangeTaxAmount2()">
                      </div>
                    </div>
                  </div>
                </div>
              </div>

              <div class="col-12 col-lg-3 col-md-3 col-sm-12">
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row">
                  <div class="d-flex flex-row m-0 flex-wrap">           
                    <div class="col-lg-6 col-md-6 col-sm-6"><b>Tax 3</b>
                      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                
                        <ng-select [items]="tax3_list" class="
                        custom
                col-lg-10 col-12
                fv-row
                mt-2
              " 
              bindLabel="tax_name3" formControlName="tax_name3" bindValue="tax_gid" 
              
               (change)="OnChangeTaxAmount3()" placeholder="Select Tax" [(ngModel)]="mdlTaxName3"></ng-select>
                      </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6">
                      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
                        <input type="text" class="form-control" placeholder="0.00" readonly
                          style="text-align: right; background-color: #e9e7e7;" formControlName="tax_amount3"
                           (input)="OnChangeTaxAmount3()">
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            
            <div class="d-flex flex-row m-6 flex-wrap">
              <div class="col-12 col-lg-4 col-md-3 col-sm-12">
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row">
                  <div class="d-flex flex-row m-0 flex-wrap">
                    <div class="col-lg-6 col-md-6 col-sm-6"><b>Total Amount</b>
                      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                        <input type="text" 
                        class="form-control"
                         placeholder="0.00"
                          style="background-color: #e9e7e7; text-align: right;" 
                          formControlName="price"
                           (input)="prodtotalcal()">
                      </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6">
                      <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
                        <button type="button" class="btn-primary" style="margin-left: 20px;"
                          (click)="OnSubmit()">ADD</button>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </form>

            <!-- end::Product Form -->
          <!-- begin::table -->

          <div class="table-responsive">
            <div class="card card-body table-responsive m-0 shadow-lg p-3 mb-5 bg-white rounded">
              <table id="POproductlist" class="table table-striped table-row-bordered gy-4 gs-2">
                <thead class="table_head">
                  <tr class="text-white fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                    <th>S.No</th>
                    <th>Product Name</th>
                    <th>Product Group</th>
                    <th>Customer Product Code</th>
                    <th>Product Code</th>
                    <!-- <th>HSN Code / Description</th> -->
                    <th>Unit</th>
                    <th style="text-align: right;">Unit Price</th>
                    <th style="text-align: right;">MRP</th>
                    <th style="text-align: center;">Quantity</th>
                    <th style="text-align: right;">Discount (%)</th>
                    <th style="text-align: right;">Need By Date</th>
                    <th style="text-align: right; width: 120px;">Tax</th>
                    <th style="text-align: right;">Total Amount</th>
                    <th class="text-center">Action</th>
                  </tr>
                </thead>
                <tbody>
                  <tr *ngFor="let data of QSOproductlist; let i=index;">
                    <td>{{i+1}}</td>
                    <td>{{data.product_name}}</td>
                    <td>{{data.productgroup_name}}</td>
                    <td>{{data.customerproduct_code}}</td>
                    <td>{{data.product_code}}</td>
                    <td>{{data.productuom_name}}</td>
                    <td style="text-align: right;">{{data.product_price | number}}</td>
                    <td style="text-align: right;">{{data.selling_price | number}}</td>
                    <td style="text-align: center;">{{data.qty_quoted}}</td>
                    <td style="text-align: right;">{{data.margin_percentage}}<br>{{data.margin_amount}}</td>
                    <td style="text-align: right;">{{data.product_requireddate}}</td>
                    <td style="text-align: right;">{{data.tax_percentage}}&nbsp;{{data.tax_percentage2}}&nbsp;{{data.taxpercentage3}}</td>
                    <td style="text-align: right;">{{data.price}}</td>
                    <td class="text-center text-nowrap">
                        <button title="Delete" type="button" (click)="onedit(data.product_gid)" data-bs-toggle="modal"
                        data-bs-target="#myModaldelete" class="btn btn-icon btn-sm bg-danger me-2"><span
                          class="fa-solid fa-trash text-white  fs-6"></span></button>
                      <button title="Delete" type="button" (click)="ondelete(data.product_gid)" data-bs-toggle="modal"
                        data-bs-target="#myModaldelete" class="btn btn-icon btn-sm bg-danger me-2"><span
                          class="fa-solid fa-trash text-white  fs-6"></span></button>
                    </td>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
          <!-- end::table -->
          <!-- begin::calculation part -->
          <div class="card">
            <div class="d-flex mt-4 me-15 m-6">
              <div>
                <label class="lab">Payment :</label>
                <input type="text" class="in" formControlName="payment_days">
                <span class="spn">Days</span>
              </div>
              <div>
                <label class="lab ms-10">Delivery Period :</label>
                <input type="text" class="in" formControlName="delivery_days">
                <span class="spn">Days</span>
              </div>
            </div>
            <div class="card">
