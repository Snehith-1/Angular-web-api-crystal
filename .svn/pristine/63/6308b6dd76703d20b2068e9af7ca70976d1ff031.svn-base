<div class="card m-3">
    <div class="card-header bg-lightblue card-height55">
        <div class="card-title m-0 ">
            <h5 class="fw-bolder text-blue">LEAD BANK CONTACT</h5>
        </div>
    </div>
    <br>
    <form [formGroup]="contactform" (ngSubmit)="onadd()">



        <div class="d-flex flex-row m-6 mt-0 flex-wrap">
            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Branch Name</b> <span class="ms-1 text-red">*</span>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2 ">
                    <select
                        class="form-select form-select-solid form-select-lg borderc-secondary bg-white custom-input select2"
                        formControlName="leadbankbranch_name">
                        <option value="">Select Branch Name</option>
                        <option *ngFor="let productunit of branch_list" [value]="productunit.leadbank_gid">
                            {{productunit.leadbankbranch_name}}</option>

                    </select>

                </div>
            </div>
            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Contact Person Name</b> <span
                    class="ms-1 text-red">*</span>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <input type="text" class="
                    form-control form-control-lg form-control-solid 
                    mb-3 mb-lg-0  borderc-secondary bg-white custom-input
                  " placeholder="Contact Person Name" required maxlength="30" pattern="^[A-Za-z0-9 ]+$"
                        formControlName="leadbankcontact_name" class="form-control" />
                </div>
                <div *ngIf="leadbankcontact_name.invalid && (leadbankcontact_name.dirty || leadbankcontact_name.touched)"
                    class="invalid-feedback">
                    <div *ngIf="leadbankcontact_name.errors?.['required']">
                        Contact is required.
                    </div>
                    <div *ngIf="leadbankcontact_name.errors?.['pattern']">
                        Enter Contact
                    </div>
                </div>

            </div>


            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Mobile Number</b> <span class="ms-1 text-red">*</span>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <input class="
                    form-control form-control-lg form-control-solid
                    mb-3 mb-lg-0 borderc-secondary bg-white custom-input
                  " placeholder="Mobile Number" pattern="^[0-9]{10}$" required maxlength="10" formControlName="mobile"
                        class="form-control" />
                </div>
                <div *ngIf="mobile.invalid && (mobile.dirty || mobile.touched)" class="invalid-feedback">
                    <div *ngIf="mobile.errors?.['required']">
                        mobile is required.
                    </div>
                    <div *ngIf="mobile.errors?.['pattern']">
                        Maximum 10 numbers
                    </div>
                </div>
            </div>
        </div>

        <div class="d-flex flex-row m-6 mt-0 flex-wrap">


            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Email Address</b> <span class="ms-1 text-red">*</span>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <input type="text" class="
                    form-control form-control-lg form-control-solid
                    mb-3 mb-lg-0 borderc-secondary bg-white custom-input
                  " placeholder="Email Address"
                        pattern="^([a-z0-9-]+|[a-z0-9-]+([.][a-z0-9-]+)*)@([a-z0-9-]+\.[a-z]{2,20}(\.[a-z]{2})?|\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\]|localhost)$"
                        formControlName="email" class="form-control" />
                </div>
                <div *ngIf="email.invalid && (email.dirty || email.touched)" class="invalid-feedback">
                    <div *ngIf="email.errors?.['required']">
                        email is required.
                    </div>
                    <div *ngIf="email.errors?.['pattern']">
                        Enter Valid Mail Id
                    </div>
                </div>
            </div>

            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Designation</b> <span class="ms-1 text-red">*</span>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <input type="text" class="
                    form-control form-control-lg form-control-solid
                    mb-3 mb-lg-0 borderc-secondary bg-white custom-input
                  " placeholder="Designation" required maxlength="30" pattern="^[A-Za-z0-9 ]+$"
                        formControlName="designation" class="form-control" />
                </div>
                <div *ngIf="designation.invalid && (designation.dirty || designation.touched)" class="invalid-feedback">
                    <div *ngIf="designation.errors?.['required']">
                        Designation is required.
                    </div>
                    <div *ngIf="designation.errors?.['pattern']">
                        Enter Designation
                    </div>
                </div>
            </div>
            <div class="row col-12 col-lg-4 col-md-6 col-sm-12"> <span class="font-bold"><b>Fax</b> <span
                        class="ms-1 text-red">*</span></span>
                <div class="col-sm-4">
                    <input type="text" class="
                    form-control form-control-lg form-control-solid
                    mb-3 mb-lg-0 borderc-secondary bg-white custom-input
                  " placeholder="Country" pattern="^[0-9]{10}$" required maxlength="3" formControlName="fax_area_code"
                        class="form-control" />

                </div>
                <div *ngIf="fax_area_code.invalid && (fax_area_code.dirty || fax_area_code.touched)"
                    class="invalid-feedback">
                    <div *ngIf="fax_area_code.errors?.['required']">
                        Fax Areacode is required.
                    </div>
                    <div *ngIf="fax_area_code.errors?.['pattern']">
                        Maximum 3 numbers
                    </div>
                </div>
                <div class="col-sm-4">
                    <input type="text" class="
                    form-control form-control-lg form-control-solid
                    mb-3 mb-lg-0 borderc-secondary bg-white custom-input
                  " placeholder="Area Code" pattern="^[0-9]{10}$" required maxlength="3"
                        formControlName="fax_country_code" class="form-control" />

                </div>
                <div *ngIf="fax_country_code.invalid && (fax_country_code.dirty || fax_country_code.touched)"
                    class="invalid-feedback">
                    <div *ngIf="fax_country_code.errors?.['required']">
                        Fax Contrycode is required.
                    </div>
                    <div *ngIf="fax_country_code.errors?.['pattern']">
                        Maximum 3 numbers
                    </div>
                </div>
                <div class="col-sm-4">
                    <input type="text" class="
                    form-control form-control-lg form-control-solid
                    mb-3 mb-lg-0 borderc-secondary bg-white custom-input
                  " placeholder="" pattern="^[0-9]{10}$" required maxlength="7" formControlName="fax"
                        class="form-control" />

                </div>
                <div *ngIf="fax.invalid && (fax.dirty || fax.touched)" class="invalid-feedback">
                    <div *ngIf="fax.errors?.['required']">
                        Fax is required.
                    </div>
                    <div *ngIf="fax.errors?.['pattern']">
                        Maximum 7 numbers
                    </div>
                </div>
            </div>
        </div>

        <div class="d-flex flex-row m-6 mt-0 flex-wrap">
            <div class="row col-12 col-lg-4 col-md-6 col-sm-12"> <span class="font-bold"><b>Telephone No I</b> <span
                        class="ms-1 text-red">*</span></span>
                <div class="col-sm-4">
                    <input type="text" class="
                    form-control form-control-lg form-control-solid
                    mb-3 mb-lg-0 borderc-secondary bg-white custom-input
                  " placeholder="Country" pattern="^[0-9]{10}$" required maxlength="3" formControlName="country_code1"
                        class="form-control" />

                </div>
                <div *ngIf="country_code1.invalid && (country_code1.dirty || country_code1.touched)"
                    class="invalid-feedback">
                    <div *ngIf="country_code1.errors?.['required']">
                        Countrycode is required.
                    </div>
                    <div *ngIf="country_code1.errors?.['pattern']">
                        Maximum 3 numbers
                    </div>
                </div>
                <div class="col-sm-4">
                    <input type="text" class="
                    form-control form-control-lg form-control-solid
                    mb-3 mb-lg-0 borderc-secondary bg-white custom-input
                  " placeholder="Area Code" pattern="^[0-9]{10}$" required maxlength="3" formControlName="area_code1"
                        class="form-control" />

                </div>

                <div *ngIf="area_code1.invalid && (area_code1.dirty || area_code1.touched)" class="invalid-feedback">
                    <div *ngIf="area_code1.errors?.['required']">
                        Areacode is required.
                    </div>
                    <div *ngIf="area_code1.errors?.['pattern']">
                        Maximum 3 numbers
                    </div>
                </div>
                <div class="col-sm-3">
                    <input type="text" class="
                    form-control form-control-lg form-control-solid
                    mb-3 mb-lg-0 borderc-secondary bg-white custom-input
                  " placeholder="" pattern="^[0-9]{10}$" required maxlength="4" formControlName="phone1"
                        class="form-control" />

                </div>
                <div *ngIf="phone1.invalid && (phone1.dirty || phone1.touched)" class="invalid-feedback">
                    <div *ngIf="phone1.errors?.['required']">
                        Fax is required.
                    </div>
                    <div *ngIf="phone1.errors?.['pattern']">
                        Maximum 4 numbers
                    </div>
                </div>
            </div>


            <div class="row col-14 col-lg-4 col-md-6 col-sm-12" style="margin-left: 10px;"> <span
                    class="font-bold"><b>Telephone No II</b> <span class="ms-1 text-red">*</span></span>
                <div class="col-sm-4">
                    <input type="text" class="
                    form-control form-control-lg form-control-solid
                    mb-3 mb-lg-0 borderc-secondary bg-white custom-input
                  " placeholder="Country" pattern="^[0-9]{10}$" required maxlength="3" formControlName="country_code2"
                        class="form-control" />

                </div>
                <div *ngIf="country_code2.invalid && (country_code2.dirty || country_code2.touched)"
                    class="invalid-feedback">
                    <div *ngIf="country_code2.errors?.['required']">
                        Countrycode is required.
                    </div>
                    <div *ngIf="country_code2.errors?.['pattern']">
                        Maximum 3 numbers
                    </div>
                </div>
                <div class="col-sm-4">
                    <input type="text" class="
                    form-control form-control-lg form-control-solid
                    mb-3 mb-lg-0 borderc-secondary bg-white custom-input
                  " placeholder="Area Code" pattern="^[0-9]{10}$" required maxlength="3" formControlName="area_code2"
                        class="form-control" />

                </div>
                <div *ngIf="area_code2.invalid && (area_code2.dirty || area_code2.touched)" class="invalid-feedback">
                    <div *ngIf="area_code2.errors?.['required']">
                        Areacode is required.
                    </div>
                    <div *ngIf="area_code2.errors?.['pattern']">
                        Maximum 3 numbers
                    </div>
                </div>
                <div class="col-sm-4">
                    <input type="text" class="
                    form-control form-control-lg form-control-solid
                    mb-3 mb-lg-0 borderc-secondary bg-white custom-input
                  " placeholder="" pattern="^[0-9]{10}$" required maxlength="4" formControlName="phone2"
                        class="form-control" />

                </div>
                <div *ngIf="phone2.invalid && (phone2.dirty || phone2.touched)" class="invalid-feedback">
                    <div *ngIf="phone2.errors?.['required']">
                        Phone is required.
                    </div>
                    <div *ngIf="phone2.errors?.['pattern']">
                        Maximum 4 numbers
                    </div>
                </div>
            </div>



        </div>

        <div class="text-center my-4">
            <button type="submit" class="btn-success btn-sm text-white me-2" (click)="onadd()">
                <i class="bi bi-check-square fs-5"></i>&nbsp;Submit</button>

            <button class="btn-primary btn-sm text-white me-4" (click)="null">
                <i class="ki-duotone ki-entrance-right fs-5">
                    <i class="path1"></i>
                    <i class="path2"></i>
                </i>&nbsp;Back</button>
        </div>
    </form>
</div>
<br>
<div class="card shadow-sm">
    <div class="card-header bg-lightblue card-height55">
        <div class="card-title m-0 ">
            <h5 class="fw-bolder text-blue">LEAD BANK CONTACT</h5>
        </div>
    </div>

    <br>

    <div class="table-responsive">
        <div class="card card-body m-3 shadow-lg p-3 mb-5 bg-white rounded">
            <table id="product" class="table table-striped table-row-bordered gy-5 gs-7 ">
                <thead>
                    <tr class=" text-white fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                        <th>S.NO</th>
                        <th>Branch Name</th>
                        <th>Contact Person</th>
                        <th>Mobile Number </th>
                        <th>Email ID</th>
                        <th>Designation</th>

                        <th class="text-center">Action</th>
                    </tr>
                </thead>
                <tbody>
                    <tr *ngFor="let data of contactlist; let i=index;">
                        <td>{{i+1}}</td>
                        <td>{{data.leadbankbranch_name}}</td>
                        <td>{{data.leadbankcontact_name}}</td>
                        <td>{{data.mobile}}</td>
                        <td>{{data.email}}</td>
                        <td>{{data.designation}}</td>


                        <td class="text-center text-nowrap">
                            <button title="Edit" type="button" class="btn btn-icon btn-sm bg-success me-2"
                                (click)="onedit(data.leadbank_gid)"><span
                                    class="fa-solid fa-pencil text-white  fs-6"></span></button>


                        </td>
                    </tr>
                </tbody>
            </table>
        </div>

    </div>

</div>