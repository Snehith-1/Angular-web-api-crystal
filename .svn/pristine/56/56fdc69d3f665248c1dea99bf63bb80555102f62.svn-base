<div class="card m-3">
  <div class="card-header bg-lightblue card-height55">
    <div class="card-title m-0 ">
      <h5 class="fw-bolder text-blue">Edit Lead Bank Branch</h5>
    </div>
  </div>
  <br>
  <form [formGroup]="brancheditform" (ngSubmit)="validate()">



    <div class="d-flex flex-row m-6 mt-0 flex-wrap">
      <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Branch Name</b> <span class="ms-1 text-red">*</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text" class="
              form-control form-control-lg form-control-solid 
              mb-3 mb-lg-0  borderc-secondary bg-white custom-input
            " placeholder="Branch Name" formControlName="leadbankbranch_name" class="form-control" />
            <input type="hidden" formControlName="leadbank_gid">
            <input type="hidden" formControlName="leadbankcontact_gid">
        </div>
      </div>
      <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Contact Person Name</b> <span class="ms-1 text-red">*</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text" class="
                  form-control form-control-lg form-control-solid 
                  mb-3 mb-lg-0  borderc-secondary bg-white custom-input
                " placeholder="Contact Person Name" formControlName="leadbankcontact_name" class="form-control" />
        </div>

      </div>


      <div class="col-12 col-lg-4 col-md-6 col-sm-12">
        <span class="font-bold">Mobile Number</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text" id="mobile" name="mobile" formControlName="mobile" class="
              form-control form-control-lg form-control-solid
              borderc-secondary
              custom-input
              mb-3 mb-lg-0
              bg-white
              custom-input
            "  pattern="^[0-9]{10}$" placeholder="Enter Mobile No" required maxlength="10" autocomplete="off">
        </div>
      </div>
    </div>
    <div class="d-flex flex-row m-6 mt-0 flex-wrap">


      <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Email Address</b> <span class="ms-1 text-red">*</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text" class="
                  form-control form-control-lg form-control-solid
                  mb-3 mb-lg-0 borderc-secondary bg-white custom-input
                " placeholder="Email Address" formControlName="email" class="form-control" />
        </div>
      </div>


      <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Designation</b> <span class="ms-1 text-red">*</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text" class="
                  form-control form-control-lg form-control-solid
                  mb-3 mb-lg-0 borderc-secondary bg-white custom-input
                " placeholder="Designation" formControlName="designation" class="form-control" />
        </div>
      </div>
      <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Address Line 1</b> <span class="ms-1 text-red">*</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text" class="
                form-control form-control-lg form-control-solid
                mb-3 mb-lg-0 borderc-secondary bg-white custom-input
              " placeholder="Address Line 1" formControlName="address1" class="form-control" />
        </div>
      </div>
    </div>

    <!-- ----------------- -->
    <div class="d-flex flex-row m-6 mt-0 flex-wrap">


      <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Address Line 2</b> <span class="ms-1 text-red">*</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text" class="
                form-control form-control-lg form-control-solid
                mb-3 mb-lg-0 borderc-secondary bg-white custom-input
              " placeholder="Address Line 2" formControlName="address2" class="form-control" />
        </div>
      </div>


      <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>City</b> <span class="ms-1 text-red">*</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text" class="
                form-control form-control-lg form-control-solid
                mb-3 mb-lg-0 borderc-secondary bg-white custom-input
              " placeholder="city" formControlName="city" class="form-control" />
        </div>
      </div>
      <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>State</b> <span class="ms-1 text-red">*</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text" class="
              form-control form-control-lg form-control-solid
              mb-3 mb-lg-0 borderc-secondary bg-white custom-input
            " placeholder="State" formControlName="state" class="form-control" />
        </div>
      </div>
    </div>

    <!-- ----------------- -->
    <div class="d-flex flex-row m-6 mt-0 flex-wrap">


      <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Country</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2 ">
          <select class="form-select form-select-solid form-select-lg borderc-secondary bg-white custom-input select2"
            formControlName="country_name">
            <option value="">Select Country Name</option>
            <option *ngFor="let productunit of country_list" [value]="productunit.leadbank_gid">
              {{productunit.country_name}}</option>

          </select>

        </div>
      </div>


      <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Postal code</b> <span class="ms-1 text-red">*</span>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
          <input type="text" class="
                    form-control form-control-lg form-control-solid
                    mb-3 mb-lg-0 borderc-secondary bg-white custom-input
                  " placeholder="Pincode" formControlName="pincode" class="form-control" />
        </div>
      </div>
      <div class="col-12 col-lg-4 col-md-6 col-sm-12"><b>Region</b>
        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2 ">
          <select class="form-select form-select-solid form-select-lg borderc-secondary bg-white custom-input select2"
            formControlName="region_name">
            <option value="">Select Region Name</option>
            <option *ngFor="let productunit of region_list" [value]="productunit.leadbank_gid">
              {{productunit.region_name}}</option>

          </select>

        </div>
      </div>
    </div>
    <div class="text-center my-4">
      <button type="submit" class="btn-success btn-sm text-white me-2" >
        <i class="bi bi-check-square fs-5"></i>&nbsp;Update</button>

        <button class="btn-primary btn-sm text-white me-4" (click)="onback()">
          <i class="fas fa-arrow-left"></i> &nbsp; Back
        </button>
    </div>
  </form>
</div>
<br>
