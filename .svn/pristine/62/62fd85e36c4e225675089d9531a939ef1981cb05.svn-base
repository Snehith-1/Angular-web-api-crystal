    <div class="table-responsive">
      
        
        <div class="card card-body m-8 shadow-xl p-5 mb-8 bg-white rounded">
            
            <table id="totallist" class="table table-striped table-row-bordered gy-5 gs-7 ">
              
                <thead>
                    
                      <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                        <th>S.No</th>
                        <th>Team/Employee</th>
                        <th>Lead</th>
                        <th>Contact</th>
                        <th>Customer Type</th>
                        <th class="text-center">Lead Notes</th>
                        <th>Lead stage</th>
                        <th>Generated By</th>
                        <th class="text-center">Action</th>
                    </tr>
                </thead>
                <tbody>
                    <tr *ngFor="let data of totallist; let i=index;" class="align-middle">
                        <td>{{i+1}}</td>
                        <td style="white-space: nowrap;">{{data.emp_team}}</td>
                        <td style="white-space: nowrap;">{{data.leadbank_name}}</td>
                        <td>{{data.contact_details}}</td>
                        <td>{{data.customer_type}}</td>
                    
                        <td class="text-center text-nowrap">
                            <label title="Remarks" data-bs-toggle="modal" data-bs-target="#myModalUpdateimage"
                                (click)="popmodal(data.internal_notes,data.leadbank_name)">
                                <i class="fas fa-question-circle fa-5x text-primary fs-2"></i>
                            </label>
                        </td>
                        <td>{{data.leadstage_name}}</td>
                        <td>{{data.created_by}}</td>
                        <td class="text-center text-nowrap">
                            <button title="360 View" type="button"
                                class="btn btn-icon btn-sm  me-2 bg-gradient-darkblue">
                                <i class="bi bi-arrow-repeat" style="font-size:14px;font-weight:bolder;"
                                    (click)="Onopen(data.leadbank_gid,data.lead2campaign_gid,data.contact_details)"></i></button>
                            <button (click)="openModallog3(data)" title="Schedule" type="button"
                                class="btn btn-icon btn-sm bg-gradient-darkoranges me-2" data-bs-toggle="modal"
                                data-bs-target="#myModalschedule">
                                <i class="fas fa-clock"></i>
                            </button>
                            <button (click)="openModallog4(data)" title="Transfer" type="button"
                                class="btn btn-icon btn-sm  bg-gradient-CancelOrder  me-2" data-bs-toggle="modal"
                                data-bs-target="#myModaltransfer"><i class="bi bi-arrows-move"></i>
                            </button>
                            <button (click)="OnBin(data.leadbank_gid)" title="Move to Drop" type="button" data-bs-toggle="modal"
                                data-bs-target="#myModaldelete" class="btn btn-icon btn-sm  bg-gradient-purple me-2"><i class="fa-solid fa-circle-down"></i></button>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>


<!---Remarks popup-->
<div class="modal fade" id="myModalUpdateimage" tabindex="-1" data-bs-backdrop='static'
    aria-labelledby="editamlcategoryLabel" aria-hidden="true" data-bs-keyboard="false">
    <div class="modal-dialog modal-md modal-dialog-centered">
        <div class="modal-content">
            <div class="card-header p-1 pb-1 pt-1 rounded-top bg-primary"
                style="display: flex; align-items: center; height: 40px;">
                <h3 style="color: white; font-weight: bold; text-align: left; width: 100%; margin-left: 10px;">
                    {{leadbank_name}}</h3>
            </div>
            <div class="modal-body">
                <div class="col-12 col-lg-3 col-md-6 col-lg-12">
                    <span class="fw-bold">Remarks: </span>
                    <p class="mt-2 wrap-text" style="max-width: 100%; overflow: auto; max-height: 200px;">
                        {{internal_notes}}</p>
                </div>
                <div class="text-center my-4">
                    <button class="btn-primary btn-sm text-white me-2" data-bs-dismiss="modal"
                        style="background-color: red;">
                        <i class="fas fa-arrow-left text-white"></i> &nbsp;Back
                    </button>&nbsp;
                </div>
            </div>
        </div>
    </div>
</div>

<!-- The Modal Popup Schedule -->
<form [formGroup]="reactiveFormfollow" #form="ngForm">
    <div class="modal" id="myModalschedule" data-bs-backdrop="static" data-bs-keyboard="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <!-- Modal Header -->
                <div class="card-header p-1 pb-2 pt-1 rounded-top bg-primary">
                    <h2 style="margin-top:10px;color:white;font-weight: bold;">&nbsp;&nbsp;<i style="font-size:20px;"
                            class="bi bi-calendar-event"></i> &nbsp;&nbsp;Schedule Lead - {{leadbank_name}}</h2>
                </div>
                <!-- Modal body -->
                <div class="modal-body">
                    <div class="row"><span class="font-bold">Schedule Date<span style="color: red;">*</span></span>
                        <div class="col-xl-12 col-md-12 col-sm-12 fv-row mt-2 d-flex ">
                            <input type="date" class="form-control
                                 form-control-lg form-control-solid
                                  custom-input mb-3 mb-lg-0 bg-white
                                  border-secondary
                                  custom-input date-picker" required formControlName="schedule_date" />
                        </div>
                    </div>
                    <br>
                    <div>
                        <div class="col-12 col-lg-12 col-md-12 col-sm-12">
                            <span class="font-bold">Schedule Time<span style="color: red;">*</span></span>
                            <div class="col-lg-12 col-12 col-md-12 col-sm-12">
                                <input type="time" formControlName="schedule_time" class="
                        form-control form-control-lg form-control-solid
                        custom-input
                        mb-3 mb-lg-0
                        bg-white
                        border-secondary
                        custom-input 
                      " name="schedule_time" required />
                            </div>
                        </div>
                    </div>
                    <br>
                    <div>
                        <span class="font-bold">Schedule Type<span style="color: red;">*</span></span>
                        <div>
                            <div class="col-12 col-lg-12 col-md-12 col-sm-12">
                                <ng-select formControlName="schedule_type" style="width:99%;" [items]="ScheduleType"
                                    required class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2"
                                    bindLabel="type" bindValue="type" [clearable]="false"
                                    placeholder="--Select Type--"></ng-select>
                            </div>
                        </div>
                        <br>
                        <div class="mb-3">
                            <label for="exampleFormControlTextarea1" class="form-label">Schedule
                                Remarks</label>
                            <textarea class="form-control" formControlName="schedule_remarks" pattern="^(?!\s*$).+"
                                id="exampleFormControlTextarea1" rows="3"></textarea>
                        </div>
                    </div>
                </div>

                <!-- Modal footer -->
                <div class="text-center my-4">

                    <button class="btn-success btn-sm text-white me-4" data-bs-dismiss="modal"  [disabled]="reactiveFormfollow.invalid"
                        (click)="onsubmitschedule()">
                        <i class="fas fa-check text-white fs-5 "></i> &nbsp;Submit
                    </button> &nbsp;&nbsp;
                    <button class="btn-danger btn-sm text-white me-4" (click)="oncloseschedule()"
                        data-bs-dismiss="modal">
                        <i class="fas fa-close text-white"></i> &nbsp;Close
                    </button>
                </div>

            </div>
        </div>
    </div>
</form>

<!-- transfer popup -->

<form [formGroup]="reactiveFormTransfer" #form="ngForm">
    <div class="modal" id="myModaltransfer" data-bs-backdrop="static" data-bs-keyboard="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <!-- Modal Header -->
                <div class="card-header p-1 pb-2 pt-1 rounded-top bg-primary">
                    <h2 style="margin-top:10px;color:white;font-weight: bold;">&nbsp;&nbsp;<i style="font-size:20px;"
                            class="bi bi-arrows-move"></i> &nbsp;&nbsp;Transfer Lead -{{leadbank_name}}</h2>
                </div>
                <!-- Modal body -->
                <div class="modal-body">
                    <span class="font-bold">Team Name<span style="color: red;">*</span></span>
                    <div>
                        <div class="col-12 col-lg-12 col-md-12 col-sm-12">
                            <ng-select formControlName="team_name" [items]="teamname_list" (change)="teamname()"
                                required class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2"
                                bindLabel="campaign_title" bindValue="campaign_gid" [clearable]="false"
                                placeholder="--Select Team--"></ng-select>
                        </div>
                    </div>
                    <br>
                    <span class="font-bold">Transfer To<span style="color: red;">*</span></span>
                    <div>
                        <div class="col-12 col-lg-12 col-md-12 col-sm-12">
                            <ng-select formControlName="team_member" [items]="employee_list" required
                                class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="user_name"
                                bindValue="employee_gid" [clearable]="false"
                                placeholder="--Select option--"></ng-select>
                        </div>
                    </div>
                </div>
                <!-- Modal footer -->
                <div class="text-center my-4">
                    <button class="btn-success btn-sm text-white me-4" (click)="OnTransfer()" data-bs-dismiss="modal"
                        [disabled]="reactiveFormTransfer.invalid">
                        <i class="fas fa-check text-white fs-5 "></i> &nbsp;Submit
                    </button> &nbsp;&nbsp;
                    <button class="btn-danger btn-sm text-white me-4" (click)="onclosetransfer()" data-bs-dismiss="modal">
                        <i class="fas fa-close text-white"></i> &nbsp;Close
                    </button>
                </div>
            </div>
        </div>
    </div>

</form>
