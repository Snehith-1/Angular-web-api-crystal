<div class="card card-bordered" style="background-color: white;">
  <div class="card-header  d-flex justify-content-center" style="background: rgb(24, 18, 134);background:  radial-gradient(circle, #5c0067 0%, #00d4ff 100%);">
      <h1 class="card-title fw-bold fs-3" style="color:white;"><b>Marketing Dashboard</b></h1>
  </div>
  <br/>
  <div class="d-flex flex-wrap flex-row col-12">
      <div class="col-lg-3 col-md-3 col-sm-3">
        <div class="card1" style="border-left: 10px solid  #b764fa !important;margin-left: 25px;padding-top: 10px">           
          <span class="d-flex justify-content-center" style="font-size:14px ;"><i class="bi bi-telephone-inbound-fill" style="font-size: 19px;color:#6b2e9c;"></i> &nbsp;&nbsp;&nbsp; <b style="color:#6b2e9c;"> My Calls </b></span>
          <div *ngFor="let getcountmycalls of DashboardCount_List">
            <p class="num" style="text-align: center ;font-size:16px ;color: black;padding-top: 4px;">
              <b >{{getcountmycalls.mycalls_count}}</b>
            </p>
          </div>
        </div>
      </div>
      <div class="col-lg-3 col-md-3 col-sm-3">
        <div class="card1" style="border-left: 10px solid  #fa6868 !important;margin-left: 25px;padding-top: 10px">           
          <span class="d-flex justify-content-center" style="font-size:14px ;"> <i class="bi bi-book-half" style="font-size: 22px;color:#c54949;"></i> &nbsp;&nbsp; <b style="color:#c54949;"> Appointments </b> </span>
          <div *ngFor="let getcountappointments of DashboardCount_List">
            <p class="num" style="text-align: center ;font-size:16px ;color: black;padding-top: 4px">
              <b>{{getcountappointments.myappointments_count}}</b>
            </p>
          </div>
        </div>
      </div>
      <div class="col-lg-3 col-md-3 col-sm-3">
        <div class="card1" style="border-left: 10px solid  #b9b9b8 !important;margin-left: 25px;padding-top: 10px">
          <span class="d-flex justify-content-center" style="font-size:14px ;"><i class="bi bi-person-fill" style="font-size: 22px;color:#636361"></i>&nbsp;&nbsp; <b style="color:#636361;"> Assign Visit </b> </span>
          <div *ngFor="let getcountassignvisit of DashboardCount_List">
            <p class="num" style="text-align: center ;font-size:16px ;color: black;padding-top: 4px">
              <b>{{getcountassignvisit.assignvisit_count}}</b>
            </p>          
          </div>
        </div>
      </div>
      <div class="col-lg-3 col-md-3 col-sm-3">
        <div class="card1" style="border-left: 10px solid  #c1c41e !important;margin-left: 25px;padding-top: 10px">          
          <span class="d-flex justify-content-center" style="font-size:14px ;"><i class="bi bi-yelp" style="font-size: 22px;color:#868817;"></i>&nbsp;&nbsp; <b style="color:#868817;"> Completed Visit </b> </span>
          <div *ngFor="let getcountcompletedvisit of DashboardCount_List">
            <p class="num" style="text-align: center ;font-size:16px ;color: black;padding-top: 4px">
              <b>{{getcountcompletedvisit.completedvisit_count}}</b>
            </p>
          </div>
        </div>
      </div>
</div>
<br/>
<div class="d-flex flex-wrap flex-row col-12">   
  <div class="col-lg-3 col-md-3 col-sm-3">
    <div class="card1" style="border-left: 10px solid  #f1a32d !important;margin-left: 25px;padding-top: 10px">        
      <span class="d-flex justify-content-center" style="font-size:14px ;"><i class="bi bi-file-text-fill" style="font-size: 22px;color:#fc9904;"></i>&nbsp;&nbsp; <b style="color:#fc9904;"> My Leads </b> </span>
      <div *ngFor="let getcountmyleads of DashboardCount_List">
        <p class="num" style="text-align: center ;font-size:16px ;color: black;padding-top: 4px">
          <b>{{getcountmyleads.myleads_count}}</b>
        </p>
      </div>
    </div>
  </div>
  <div class="col-lg-3 col-md-3 col-sm-3">
    <div class="card1" style="border-left: 10px solid  #57e234 !important;margin-left: 25px;padding-top: 10px">       
      <span class="d-flex justify-content-center" style="font-size:14px ;"><i class="bi bi-filetype-doc" style="font-size: 22px;color:#298313;"></i>&nbsp;&nbsp; <b style="color:#298313;"> Shared Proposals </b> </span>
      <div *ngFor="let getcountsharedproposals of DashboardCount_List">
        <p class="num" style="text-align: center ;font-size:16px ;color: black;padding-top: 4px">
          <b>{{getcountsharedproposals.shared_proposal}}</b>
        </p>
      </div>
    </div>
  </div>
  <div class="col-lg-3 col-md-3 col-sm-3">
    <div class="card1" style="border-left: 10px solid  #42cbcf !important;margin-left: 25px;padding-top: 10px">        
      <span class="d-flex justify-content-center" style="font-size:14px ;"><i class="bi bi-hexagon-half" style="font-size: 22px;color:#0c6d70;"></i>&nbsp;&nbsp; <b style="color:#0c6d70;"> Completed Order </b> </span>
      <div *ngFor="let getcountcompletedorder of DashboardCount_List">
        <p class="num" style="text-align: center ;font-size:16px ;color: black;padding-top: 4px">
          <b>{{getcountcompletedorder.completedorder_count}}</b>
        </p>
      </div>
    </div>
  </div> 
  <div class="col-lg-3 col-md-3 col-sm-3">
    <div class="card1" style="border-left: 10px solid  #fa78e4 !important;margin-left: 25px;padding-top: 10px">        
      <span class="d-flex justify-content-center" style="font-size:14px ;"><i class="bi bi-receipt-cutoff" style="font-size: 22px;color:#922680;"></i>&nbsp;&nbsp; <b style="color:#922680;"> Total Order </b> </span>
      <div *ngFor="let getcounttotalorder of DashboardCount_List">
        <p class="num" style="text-align: center ;font-size:16px ;color: black;padding-top: 4px">
          <b>{{getcounttotalorder.totalorder_count}}</b>
        </p>
      </div>
    </div>
  </div>
</div>
<br/>
  <div class="d-flex flex-wrap flex-row col-12 ">
       <div class="col-6 card m-3 ms-5 shadow-lg p-3 mb-5 bg-white rounded">
        <div class="me-2" >
          <span class="fw-bolder text-gray-800 d-block fs-4 p-3"> Yearly Wise Quotation Amount Details - {{year}}</span>
        <hr/>            
          <div class="card-body align-items-center position-relative ">
            <div class="table-responsive">
              <div class="m-3 shadow-lg p-3 mb-5 bg-white rounded">
                  <table id="productgroup_list" class="table table-striped table-row-bordered gy-5 gs-7 ">
                      <thead>
                          <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                              <th>Month</th>
                              <th>Quotation Amount</th>                       
                          </tr>
                      </thead>
                      <tbody>
                          <tr *ngFor="let data of DashboardQuotationAmt_List; let i=index;">
                              <td>{{data.month_name}}</td>
                              <td>{{data.total_amount}}</td>  
                          </tr>                           
                          <tr  style="background-color: white;" *ngIf="show">
                            <br/>
                            <span  style="color:gray;" >{{noquotation}}</span>
                          </tr>
                      </tbody>
                  </table>
              </div>
          </div>
           
          </div>
        </div>
       
         
                     
  </div>
  <div class="col-5 card m-3 ms-5 shadow-lg p-3 mb-5 bg-white rounded">

      <div class="me-2">
        <span class="fw-bolder text-gray-800 d-block fs-4 p-3">Lead Bank Status</span>
        <hr/>
        <!-- <span class="text-gray-400 fw-bold p-3">Feb - Sep 2023</span> -->
        <div class="card-body align-items-center position-relative ">
       
          <div class="d-flex flex-center me-5 pt-2">
            <div id="kt_card_widget_17_chart">  </div>
          </div>
                      

         
     <div class="d-flex flex-wrap ">
      <div id="chart">
          <apx-chart
            [series]="chartOptions1.series"
            [chart]="chartOptions1.chart"
            [labels]="chartOptions1.labels"
            [responsive]="chartOptions1.responsive"
          ></apx-chart>
        </div>
        
          </div>
        </div>
      </div>
     
                   
</div>
</div>

</div>


