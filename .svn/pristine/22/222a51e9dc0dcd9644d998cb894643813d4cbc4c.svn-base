                            <i class="fas fa-check text-white fs-5 "></i> &nbsp;Apply
                        </button> &nbsp;&nbsp;
                        <button class="btn-danger btn-sm text-white me-4" (click)="onclose()" data-bs-dismiss="modal">
                            Cancel
                        </button>
                    </div>

                </div>
            </div>
        </div>
    </div>
</form> 
<form #form="ngForm">
    <div class="modal modal-xl" id="myModaladd" data-bs-backdrop="static" data-bs-keyboard="false">
      <div class="modal-dialog">
        <div class="modal-content">  
          <!-- Modal Header -->  
          <div class="card-header p-1 pb-2 pt-1 rounded-top bg-primary">
            <h2 style="margin-top:10px;color:white;font-weight: bold;">&nbsp;&nbsp;<i
                class="fas fa-plus fw-bolder fs-3"></i> &nbsp;&nbsp;Compose Mail</h2>  
          </div>
          <!-- Modal body -->
          <div class="modal-body">             
            <div id="kt_app_content" class="app-content  flex-column-fluid ">


                <!--begin::Content container-->
                <div id="kt_app_content_container" class="app-container  container-xxl ">
                    <!--begin::Inbox App - Compose -->
                    <div>
                        <!--begin::Sidebar-->
            
                        <!--end::Sidebar-->
            
                        <!--begin::Content-->
                        <div>
            
                            <!--begin::Card-->
                            <div class="">
            
                                <div class="card-body p-0">
                                    <!--begin::Form-->
                                    <form [formGroup]="reactiveForm" #form="ngForm" id="kt_inbox_compose_form">
                                        <!--begin::Body-->
                                        <div class="d-block">
                                            <!--begin::From-->
                                            <div class="d-flex align-items-center border-bottom px-8 min-h-50px">
                                                <!--begin::Label-->
            
                                                <div class="text-gray-900 fw-bold w-75px">
                                                    From:
                                                </div>
                                                <!--end::Label-->
            
                                                <!--begin::Input-->
                                                <input type="text" class="form-control form-control-transparent border-0"
                                                    formControlName="mail_from" value="" data-kt-inbox-form="tagify" />
                                                <div *ngIf="mail_from.invalid && (mail_from.dirty || mail_from.touched)">
                                                    <div *ngIf="mail_from.errors?.['required']">
                                                        <span style="color:red;">&nbsp; From Mail Is required.</span>
                                                    </div>
                                                </div>
                                                <!--end::Input-->
            
            
                                            </div>
                                            <!--end::Reply To-->
                                            <!--begin::From-->
                                            <div class="d-flex align-items-center border-bottom px-8 min-h-50px">
                                                <!--begin::Label-->
                                                <div class="text-gray-900 fw-bold w-75px">
                                                    Reply To:
                                                </div>
                                                <!--end::Label-->
            
                                                <!--begin::Input-->
                                                <input type="text" class="form-control form-control-transparent border-0"
                                                    formControlName="reply_to" value="" data-kt-inbox-form="tagify" />
                                                <div *ngIf="reply_to.invalid && (reply_to.dirty || reply_to.touched)">
                                                    <div *ngIf="reply_to.errors?.['required']">
                                                        <span style="color:red;">&nbsp;Reply Is required.</span>
                                                    </div>
                                                </div>
                                                <!--end::Input-->
            
            
                                            </div>
                                            <!--end::Reply To-->
                                            <!--begin::To-->
                                            <div class="d-flex align-items-center border-bottom px-8 min-h-50px">
                                                <!--begin::Label-->
                                                <div class="text-gray-900 fw-bold w-75px">
                                                    To:
                                                </div>
                                                <!--end::Label-->
            
                                                <!--begin::Input-->
                                                <input type="text" class="form-control form-control-transparent border-0"
                                                    formControlName="to" value="" data-kt-inbox-form="tagify" />
                                                <div *ngIf="to.invalid && (to.dirty || to.touched)">
                                                    <div *ngIf="to.errors?.['required']">
                                                        <span style="color:red;">&nbsp;To Mail Is required.</span>
                                                    </div>
                                                    <div *ngIf="to.errors?.['pattern']">
                                                        <span style="color:red;">&nbsp; Invaild Email Format.</span>
                                                    </div>
                                                </div>
                                                <!--end::Input-->
            
                                                <!--begin::CC & BCC buttons-->
                                                <!-- <div class="ms-auto w-75px text-end">
                                                    <span class="text-muted fs-bold cursor-pointer text-hover-primary me-2"
                                                        data-kt-inbox-form="cc_button">Cc</span>
                                                </div> -->
                                                <!--end::CC & BCC buttons-->
                                            </div>
                                            <!--end::To-->
            
                                            <!--begin::CC-->
                                            <div class="d-none align-items-center border-bottom ps-8 pe-5 min-h-50px"
                                                data-kt-inbox-form="cc">
                                                <!--begin::Label-->
                                                <div class="text-gray-900 fw-bold w-75px">
                                                    Cc:
                                                </div>
                                                <!--end::Label-->
            
                                                <!--begin::Input-->
                                                <input type="text" class="form-control form-control-transparent border-0"
                                                    formControlName="cc" value="" data-kt-inbox-form="tagify" />
                                                <div *ngIf="cc.invalid && (cc.dirty || cc.touched)">
                                                    <div *ngIf="cc.errors?.['required']">
                                                        <span style="color:red;">&nbsp;CC Is required.</span>
                                                    </div>
                                                </div>
                                                <!--end::Input-->
            
                                                <!--begin::Close-->
                                                <span class="btn btn-clean btn-xs btn-icon" data-kt-inbox-form="cc_close">
                                                    <i class="ki-duotone ki-cross fs-5"><span class="path1"></span><span
                                                            class="path2"></span></i>
                                                </span>
                                                <!--end::Close-->
                                            </div>
                                            <!--end::CC-->
            
            
            
                                            <!--begin::Subject-->
                                            <div class="border-bottom">
                                                <input class="form-control form-control-transparent border-0 px-8 min-h-45px"
                                                    formControlName="sub" placeholder="Subject" />
                                                <div *ngIf="sub.invalid && (sub.dirty || sub.touched)">
                                                    <div *ngIf="sub.errors?.['required']">
                                                        <span style="color:red;">&nbsp;Subject Is required.</span>
                                                    </div>
                                                </div>
                                            </div><br>
                                            <!--end::Subject-->
                                            <div class="d-flex flex-row m-6 mt-0">
                                                <div class="col">
                                                    <span class="font-bold">Upload </span>
                                                    <div class="col-lg-12 fv-row mt-2">
                                                        <input (change)="onChange2($event)" accept="image/*," type="file" #fileInput
                                                            class="
                                  form-control form-control-lg form-control-solid bg-white
                                  mb-3 mb-lg-0  borderc-secondary custom-input
                                " name="fName" />
                                                    </div>
                                                </div>
                                            </div>
                                            <!--end::Message-->
                                            <div class="col-12 col-lg-12 col-sm-12">
            
            
            
                                                <div class="d-flex flex-row m-6 mt-0 ">
                                                    <div class="col">
                                                        <span class="text-gray-900 fw-bold">Body</span>
                                                        <div class="fv-row mt-2">
                                                            <angular-editor [config]="config"
                                                                formControlName="body"></angular-editor>
            
                                                        </div>
                                                    </div>
                                                </div>
            
            
            
                                            </div>
                                            
                                        </div>
                                        <!--end::Body-->
            
                                        <!--begin::Footer-->
                                        <div
                                            class="d-flex flex-stack flex-wrap justify-content-center gap-2 py-5 ps-8 pe-5 border-top">
                                            <!--begin::Actions-->
                                            <div class="d-flex align-items-center me-3">
                                                <!--begin::Send-->
                                                <div class="btn-group align-items-center  me-4 ">
                                                    <!--begin::Submit-->
            
                                                    <!-- Example split danger button -->
                                                    <!-- app/send-button/send-button.component.html -->
                                                    <div class="btn-group">
                                                        <div class="btn-group">
                                                            <button type="button" class="btn draft-button text-white "
                                                                (click)="onadd()">Save as Draft</button>
                                                            
                                                        </div>
                                                        &nbsp;&nbsp;
                                                        <!--begin::Toggle-->
                                                        <a href="#"
                                                            class="btn btn-sm btn-icon btn-color-primary btn-light btn-active-light-primary d-lg-none"
                                                            data-bs-toggle="tooltip" data-bs-dismiss="click" data-bs-placement="top"
                                                            title="Toggle inbox menu" id="kt_inbox_aside_toggle">
                                                            <i class="ki-duotone ki-burger-menu-2 fs-3 m-0"><span class="path1"></span><span
                                                                    class="path2"></span><span class="path3"></span><span class="path4"></span><span
                                                                    class="path5"></span><span class="path6"></span><span class="path7"></span><span
                                                                    class="path8"></span><span class="path9"></span><span class="path10"></span></i>
                                                        </a>
                                                        &nbsp;&nbsp;
                                                        <button type="button" class="btn send-button  text-white"
                                                            (click)="onadd()">Send</button>
                                                            &nbsp;&nbsp;
                                                            <button class="btn-danger btn-sm text-white me-4"  data-bs-dismiss="modal">
                                                                <i class="fas fa-close text-white"></i> &nbsp;Close
                                                              </button>
                                                    </div> 
                                                </div>
                                            </div>
                                            <!--end::Actions-->
            
                                        </div>
                                        <!--end::Footer-->
                                    </form>
                                    <!--end::Form-->
                                </div>
                            </div>
                            <!--end::Card-->
            
                        </div>
                    </div>
                    <!--end::Content-->
                </div>
            </div>
             </div>   
              <!-- Modal footer -->
                 
          </div>
        </div>
      </div> 
  </form> 
  