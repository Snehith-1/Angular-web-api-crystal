<div class="card">
    <div class="row ms-3 mt-3 d-flex align-items-center">
        <div class="col">
            <h3 class="card-title">Marketing Manager - YTD</h3>
        </div>
        <div class="col me-5">
        </div>
    </div>
    <br />
    <div class="row">
        
        <div class="col-lg-2 col-md-2 col-sm-2" routerLink="/crm/CrmTrnMarketingManagerSummary">
            <div class="cardHead radius-10 border-start border-0 border-3 border-violet"
              >
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">Total Leads</h5>
                            <div *ngFor="let MyTodayTaskCount_List of countlist">
                                <h4 class="my-1 text-info">{{MyTodayTaskCount_List.assigned_leads}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-violet text-white ms-auto"><i
                                class="bi bi-calendar3"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-2 col-md-2 col-sm-2" routerLink="/crm/CrmTrnNewmarketing">
            <div class="cardHead radius-10 border-start border-0 border-3 border-darkblue"
                >
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">New</h5>
                            <div *ngFor="let MyNewTaskCount_List of countlist">
                                <h4 class="my-1 text-info">{{MyNewTaskCount_List.newleads}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-darkblue text-white ms-auto"><i
                                class="bi bi-arrow-up-circle"></i>

                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-lg-2 col-md-2 col-sm-2" routerLink="/crm/CrmTrnProspectmarketing">
            <div class="cardHead radius-10 border-start border-0 border-3 border-darkorange">
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">Prospects</h5>
                            <div *ngFor="let ProspectsCount_List of countlist">
                                <h4 class="my-1 text-info">{{ProspectsCount_List.prospect}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-purple text-white ms-auto"><i
                                class="bi bi-person"></i>

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-2 col-md-2 col-sm-2" routerLink="/crm/CrmTrnPotentialmarketing">
            <div class="cardHead radius-10 border-start border-0 border-3 border-orange">
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">Potentials</h5>
                            <div *ngFor="let PotentialsCount_List of countlist">
                                <h4 class="my-1 text-info">{{PotentialsCount_List.visit}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-darkgreen text-white ms-auto"><i
                                class="bi bi-clipboard-fill"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>



        <div class="col-lg-2 col-md-2 col-sm-2" routerLink="/crm/CrmTrnCustomermarketing">
            <div class="cardHead radius-10 border-start border-0 border-3 border-darkgreen">
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">Customers</h5>
                            <div *ngFor="let CompletedCount_List of countlist">
                                <h4 class="my-1 text-info">{{CompletedCount_List.customer}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-darkgreens text-white ms-auto"><i
                                class="bi bi-check-lg"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-2 col-md-2 col-sm-2" routerLink="/crm/CrmTrnDropmarketing">
            <div class="cardHead radius-10 border-start border-0 border-3 border-purple">
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">Drop</h5>
                            <div *ngFor="let DropCount_List of countlist">
                                <h4 class="my-1 text-info">{{DropCount_List.drop_status}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-darkoranges text-white ms-auto"><i class="fa-solid fa-circle-down"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
    <div class="row">
        <div class="col-lg-2 col-md-2 col-sm-2" routerLink="/crm/CrmTrnUpcomingmarketing">
            <div class="cardHead radius-10 border-start border-0 border-3 border-brown">
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary"> Scheduled</h5>
                            <div *ngFor="let MyUpcomingTaskCount_List of countlist">
                                <h4 class="my-1 text-info">{{MyUpcomingTaskCount_List.upcoming}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-brown text-white ms-auto"><i
                                class="bi bi-calendar-event"></i>

                        </div>
                    </div>
                </div>
            </div>
        </div>
      
          <div class="col-lg-2 col-md-2 col-sm-2" routerLink="/crm/CrmCrmTrnMtd">
            <div class="cardHeadLeads radius-10 border-start border-0 border-4 border-darkorange1">
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">MTD</h5>
                            <div *ngFor="let countlist of countlist">
                                <h4 class="my-1 text-info">{{countlist.team_count1}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-dark text-white ms-auto"><i
                                class="bi bi-clipboard"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div> 
        
        <div class="col-lg-2 col-md-2 col-sm-2" routerLink="/crm/CrmTrnYtd">
            <div class="cardHeadLeads radius-10 border-start border-0 border-4 border-Q2D" style="background-color: rgb(239, 199, 106);">
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">YTD</h5>
                            <div *ngFor="let countlist of countlist">
                                <h4 class="my-1 text-info">{{countlist.team_count1}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-Q2D text-white ms-auto"><i class="bi bi-calendar3"></i>
                    </div>
                </div>
            </div>
        </div>
        </div> 
        <div class="col-lg-2 col-md-2 col-sm-2" routerLink="/crm/CrmTrnLapsedleadmarketing">
            <div class="cardHead radius-10 border-start border-0 border-4 border-cottoncandy" >
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">Lapsed Leads</h5>
                            <div *ngFor="let PotentialsCount_List of countlist">
                                <h4 class="my-1 text-info">{{PotentialsCount_List.Lapsed_Leads}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-darkpink text-white fw-bold ms-auto"><i class="bi bi-calendar-date-fill"></i>

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-2 col-md-2 col-sm-2" routerLink="/crm/CrmTrnLongestleadmarketing">
            <div class="cardHead radius-10 border-start border-0 border-3 border-blue">
                <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">Longest Leads</h5>
                            <div *ngFor="let PotentialsCount_List of countlist">
                                <h4 class="my-1 text-info">{{PotentialsCount_List.Longest_Leads}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-darkblues text-white ms-auto"><i class="bi bi-calendar-month"></i>

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-2 col-md-2 col-sm-2"style="cursor: not-allowed;">
            <div class="cardHeadLeads radius-10 border-start border-0 border-3 border-darkorange "style="background-color:#F5F5F5;">  <div class="card-body12">
                    <div class="d-flex align-items-center">
                        <div>
                            <h5 class="mb-0 text-secondary">Team Count</h5>
                            <div *ngFor="let countlist of countlist">
                                <h4 class="my-1 text-info">{{countlist.team_count}}</h4>
                            </div>
                        </div>
                        <div class="widgets-icons-2 rounded-circle bg-gradient-lightpurple text-white ms-auto"><i
                                class="bi bi-person"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
     <div class="table-responsive">
        <div class="card card-body m-8 shadow-xl p-5 mb-8 bg-white rounded">
            <table id="M2D" class="table table-striped table-row-bordered gy-5 gs-7 ">
                <thead>
                    <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                        <th>S.No</th>
                      
                        <!-- <th>Team/Employee</th> -->
                        <th>Customer Order Refno</th>
                        <th>Customer</th>
                        <!-- <th>Customer Type</th> -->
                        <th class="text-center">Lead Notes</th>
                        <th>Order Type</th>
                        <th>Amount</th>
                        <th>Order Status</th>
                        <!-- <th>Generated by</th> -->
                        <th>Generated date</th>
                        <!-- <th class="text-center">Action</th> -->
                    </tr>
                </thead>
                <tbody>
                    <tr *ngFor="let data of M2D; let i=index;" class="align-middle">
                        <td>{{i+1}}</td>
                        <td>{{data.so_referenceno1}}</td>
                        
                        <!-- <td style="white-space: nowrap;">{{data.customer_name}}</td> -->
                        <td>{{data.customer_name}}</td>
                         
                     <td class="text-center text-nowrap">
                            <label title="Remarks" data-bs-toggle="modal" data-bs-target="#myModalUpdateimage"
                                (click)="popmodal(data.internal_notes,data.customer_name)">
                                <i class="fas fa-question-circle fa-5x text-primary fs-2"></i>
                            </label>
                        </td>
                        <td>{{data.salesorder_date}}</td>
                        <td>{{data.so_type}}</td>
                        <td class="text-right">{{data.Grandtotal}}</td>
                        <td>{{data.salesorder_status}}</td>
                        
                    </tr> 
                </tbody>

            </table>
        </div>
    </div>
</div>

<!---Remarks popup-->
<div class="modal fade" id="myModalUpdateimage" tabindex="-1" data-bs-backdrop='static'
    aria-labelledby="editamlcategoryLabel" aria-hidden="true" data-bs-keyboard="false">
    <div class="modal-dialog modal-md modal-dialog-centered">
        <div class="modal-content">
            <div class="card-header p-1 pb-1 pt-1 rounded-top bg-primary"
                style="display: flex; align-items: center; height: 40px;">
                <h3 style="color: white; font-weight: bold; text-align: left; width: 100%; margin-left: 10px;">
                    {{customer_name}}</h3>
            </div>
            <div class="modal-body">
                <div class="col-12 col-lg-3 col-md-6 col-lg-12">
                    <span class="fw-bold">Remarks: </span>
                    <p class="mt-2 wrap-text" style="max-width: 100%; overflow: auto; max-height: 200px;">
                        {{internal_notes}}</p>
                </div>
                <div class="text-center my-4">
                    <button class="btn-primary btn-sm text-white me-2" data-bs-dismiss="modal"
                        style="background-color: red;">
                        <i class="fas fa-arrow-left text-white"></i> &nbsp;Back
                    </button>&nbsp;
                </div>
            </div>
        </div>
    </div>
</div>
