 <div class="m-12">
    <form  [formGroup]="reactiveForm" #form="ngForm">
      <div class="card shadow-lg rounded-top"   style="top:-50px;">
        <div class="card m-3">
          <div class="m-3 mt-5 border card">
            <div class="card-header bg-lightblue card-height55">
              <div class="card-title m-0">
                <h5 class="fw-bolder text-blue">GRN QC Checker</h5>
              </div>
            </div>
             <div *ngFor="let data of GetGrnQcChecker_list;"> 
                <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom">
                    <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>GRN Ref No</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                            <p>{{data.grn_gid}}</p> 
                        </div>
                    </div>
                    <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>GRN Date</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                            <p>{{data.grn_date}}</p> 
                        </div>
                    </div>
        
                    <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Name</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                            <p>{{data.vendor_companyname}}</p> 
                        </div>
                    </div>
                    <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Contact Person</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                            <p>{{data.vendor_contact_person}}</p> 
                        </div>
                    </div>
                </div>
             </div> 
             <div *ngFor="let data of GetGrnQcChecker_list;"> 
                <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom">
                    <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Vendor Contact Number</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                            <p>{{data.contact_telephonenumber}}</p>
                        </div>
                    </div>
                    <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Emai Address</b>
        
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                            <p>{{data.email_id}}</p>
                        </div>
                    </div>
                    <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b> Vendor Address</b>
        
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                            <p>{{data.address}}</p>
                        </div>
                    </div>
                    <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Raised By</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                            <p>{{data.user_firstname}}</p>
                        </div>
                    </div>
                </div>
             </div> 
             <div *ngFor="let data of GetGrnQcChecker_list;"> 
                <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom">
                    <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>PO Ref No</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                            <p>{{data.purchaseorder_list}}</p> 
                        </div>
                    </div>
                    <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Check by User </b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                           <p>{{data.user_checkername}}</p> 
                        </div>
                    </div>
                    <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>DC NO</b>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <p>{{data.dc_no}}</p> 
                        </div>
                    </div>
               
                <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>File Upload</b>
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                        <input type="file"
                            class="form-control form-control-lg form-control-solid custom-input mb-6 mb-lg-0 borderc-secondary bg-white custom-input"
                            (change)="onChange2($event)" accept=".xlsx , .xls" #fileinput class="form-control" />
                    </div>
                </div>
            </div>
            <div class="d-flex flex-row m-3 mt-6 flex-wrap border-bottom">
                <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b></b>
                </div>
                <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>GRN Remarks</b>
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                        <textarea type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
                            placeholder="" readonly formControlName="grn_remarks" class="form-control"></textarea>
        
                         <p></p>  
                    </div>
                </div>
                <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>GRN Ref</b>
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                        <textarea type="text"
                            class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
                            placeholder="" readonly formControlName="grn_reference" class="form-control"></textarea>
        
                         <p></p>  
                    </div>
                </div>
                </div>
                <div class="table-responsive">
                        <table id="GetGrnQcChecker_lists" class="table table-striped table-row-bordered gy-5 gs-7">
                            <thead>
                                <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                                <th>Product Group</th>
                                <th>Product Code</th>
                                <th>Product</th>
                                <th>Unit</th>
                                <th>Qty Inward</th>
                                <th>Qty Shortage</th>
                                <th>Qty Deliverd</th>
                                <th>Qty Rejected</th>
                                <th>QC Remarks</th>
                                <th>Location</th>
                                <th>Bin</th>
                            </tr>
                            </thead>
                            <tbody>
                                <tr *ngFor="let data of GetGrnQcChecker_lists;"> 
                                <td>{{data.productgroup_name}}</td>
                                <td>{{data.product_code}}</td>
                                <td>{{data.product_name}}</td>
                                <td>{{data.productuom_name}}</td>
                                <td>{{data.qty_delivered}}</td>
                                <td>
                                        <input type="text"
                                        class="form-control form-control-lg form-control-solid  mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
                                        placeholder="Enter Qty" formControlName="shortage_qty " class="form-control" />
                
                                </td>
                                <td>
                                    <input type="text"
                                    class="form-control form-control-lg form-control-solid  mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
                                    placeholder="Enter Qty" formControlName="qty_delivered" class="form-control" />
                                   </td>
                                <td>
                                    <input type="text"
                                    class="form-control form-control-lg form-control-solid  mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
                                    placeholder="Enter Qty" formControlName="rejected_qty" class="form-control" />
            
                                </td>
                                <td>
                                    <textarea type="text"
                                    class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
                                    placeholder=""  formControlName="display_field" class="form-control"></textarea></td>
                                <td>{{data.location_name}}</td>
                                <td>{{data.bin_number}}</td>
                                </tr>
                            
                            </tbody>
                        </table>
                    </div>
                </div>
        
                <div class="text-center my-4">

                    <button class="btn-primary btn-sm text-white me-2" >
                      <i class="fas fa-check text-white fs-5"></i> &nbsp;Submit
                    </button>

                    <button class="btn-primary btn-sm text-white me-4 mt-3-center " routerLink="/pmr/PmrTrnGrncheckerSummary">
                        &nbsp;Back
                    </button>


             </div> 
