        </div>

        <div class="card">
            <div class="card-body">
                <div class="row">

                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <ul class="nav nav-tabs nav-line-tabs nav-line-tabs-20x mb-8 fs-6">
                            <li class="nav-item">
                                <a class="nav-link active" data-bs-toggle="tab" href="#kt_tab_pane_5"
                                    style="color: #009EF7">Shopify Campaign</a>
                            </li>
                            <li class="nav-item ms-9">
                                <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_4"
                                    style="color: #009EF7">WhatsApp Campaign </a>
                            </li>
                            <li class="nav-item ms-9">
                                <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_6"
                                    style="color: #009EF7">Email Campaign</a>
                            </li>
                            <li class="nav-item ms-9">
                                <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_7"
                                    style="color: #009EF7">Facebook Campaign</a>
                            </li>
                            <li class="nav-item ms-9">
                                <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_7"
                                    style="color: #009EF7">SMS Campaign</a>
                            </li>
                            <!-- <li class="nav-item ms-9">
                            <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_7"style="color: #009EF7">Linkedin Campaign</a>
                        </li>
                        <li class="nav-item ms-9">
                            <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_7"style="color: #009EF7">Telegram Campaign</a>
                        </li> -->
                        </ul>
                        <!-- <div class="card shadow-sm p-3"> -->
                        <div class="tab-content" id="myTabContent">
                            <div class="tab-pane fade show active ms-5" id="kt_tab_pane_5" role="tabpanel">
                                <div class="row">
                                    <div class="col-12 ">
                                        <div *ngFor="let datas of campaignserv_list" class="modal-body" >
                                            <div id="kt_app_content" class="app-content  flex-column-fluid ">
                                                <div class="d-flex flex-row m-6 mt-0 flex-wrap">
                                                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                                                        <span class="font-bold">Shopify Store Name<span
                                                                class="ms-1 text-red">*</span></span>
                                                        <input [(ngModel)]="datas.shopify_store_name" type="text" class="
                                                                form-control form-control-lg form-control-solid
                                                                bg-white
                                                                custom-input
                                                                mb-3 mb-lg-0
                                                                borderc-secondary " name="template_name" />
                                                    </div>





                                                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                                                        <span class="font-bold">Store Month and Year<span
                                                                class="ms-1 text-red">*</span></span>
                                                        <input [(ngModel)]="datas.shopify_store_month_year" type="text" class="
                                                                form-control form-control-lg form-control-solid
                                                                bg-white
                                                                custom-input
                                                                mb-3 mb-lg-0
                                                                borderc-secondary " name="sub" />

                                                    </div>

                                                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                                                        <span class="font-bold">Access Token<span
                                                                class="ms-1 text-red">*</span></span>
                                                        <input [(ngModel)]="datas.shopify_access_token" type="text" class="
                                                                form-control form-control-lg form-control-solid
                                                                bg-white
                                                                custom-input
                                                                mb-3 mb-lg-0
                                                                borderc-secondary " name="template_name" />

                                                    </div>
                                                </div><br>
                                                <div class="text-center my-4">
                                                    <!-- <button class="btn-sm text-white me-2" [disabled]="isSubmitting" [ngClass]="{
                                                          'primary-button': !reactiveForm.invalid,
                                                          'secondary-button': reactiveForm.invalid
                                                        }" (click)="validate()" [disabled]="reactiveForm.invalid">
                                                      <i class="fas fa-check text-white fs-5"></i> &nbsp; Submit
                                                    </button> -->
                                                    <button class="btn-danger btn-sm text-white me-4"
                                                        data-bs-dismiss="modal">
                                                        <i class="fas fa-close text-white"></i> &nbsp;Close
                                                    </button>
                                                </div>


                                            </div>
                                        </div>
                                    </div>
                                </div>



                            </div>

                            <div *ngFor="let datas of campaignservice_list" class="row tab-pane fade ms-5" id="kt_tab_pane_4" role="tabpanel">

                                <div id="kt_app_content" class="app-content  flex-column-fluid ">
                                    <div class="d-flex flex-row m-6 mt-0 flex-wrap">
                                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2"
                                           >
                                            <span class="font-bold">Base URL<span class="ms-1 text-red">*</span></span>
                                            <input [(ngModel)]="datas.base_url"   type="text" class="
                                                form-control form-control-lg form-control-solid
                                                bg-white
                                                custom-input
                                                mb-3 mb-lg-0
                                                borderc-secondary " />

                                            <!-- <button title="Edit" (click)="openModaledit(data)" type="button"
                                                data-bs-toggle="modal" data-bs-target="#myModaledit"
                                                class="btn btn-icon btn-sm bg-edit me-2"><span
                                                    class="fa-solid fa-pencil text-white  fs-6"></span></button> -->

                                        </div>


                                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                                            <span class="font-bold">Workspace ID<span
                                                    class="ms-1 text-red">*</span></span>
                                            <input [(ngModel)]="datas.workspace_id" type="text" class="
                                                form-control form-control-lg form-control-solid
                                                bg-white
                                                custom-input
                                                mb-3 mb-lg-0
                                                borderc-secondary " name="sub" />

                                        </div>

                                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                                            <span class="font-bold">Access Token<span
                                                    class="ms-1 text-red">*</span></span>
                                            <input [(ngModel)]="datas.access_token" type="text" class="
                                                form-control form-control-lg form-control-solid
                                                bg-white
                                                custom-input
                                                mb-3 mb-lg-0
                                                borderc-secondary " name="template_name" />

                                        </div>
                                    </div><br>
                                    <div class="text-center my-4">
                                        <!-- <button class="btn-sm text-white me-2" [disabled]="isSubmitting" [ngClass]="{
                                          'primary-button': !reactiveForm.invalid,
                                          'secondary-button': reactiveForm.invalid
                                        }" (click)="validate()" [disabled]="reactiveForm.invalid">
                                      <i class="fas fa-check text-white fs-5"></i> &nbsp; Submit
                                    </button> -->
                                        <button class="btn-danger btn-sm text-white me-4" (click)="onupdate()" data-bs-dismiss="modal">
                                            <i class="fas fa-close text-white"></i> &nbsp;Update
                                        </button>
                                    </div>


                                </div>

                            </div>

                            <div class="tab-pane fade ms-5" id="kt_tab_pane_6" role="tabpanel">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12 lh-lg" *ngFor="let datas of campaignser_list">
                                        <div id="kt_app_content" class="app-content  flex-column-fluid ">
                                            <div class="d-flex flex-row m-6 mt-0 flex-wrap">
                                                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                                                    <span class="font-bold">Base URL<span
                                                            class="ms-1 text-red">*</span></span>
                                                    <input [(ngModel)]="datas.base_url" type="text" class="
                                                        form-control form-control-lg form-control-solid
                                                        bg-white
                                                        custom-input
                                                        mb-3 mb-lg-0
                                                        borderc-secondary " name="template_name" />
                                                </div>





                                                <!-- <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                                            <span class="font-bold">Store Month and Year<span class="ms-1 text-red">*</span></span>
                                            <input formControlName="sub" type="text" class="
                                                        form-control form-control-lg form-control-solid
                                                        bg-white
                                                        custom-input
                                                        mb-3 mb-lg-0
                                                        borderc-secondary " name="sub" />
                                            
                                        </div> -->

                                                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                                                    <span class="font-bold">Access Token<span
                                                            class="ms-1 text-red">*</span></span>
                                                    <input [(ngModel)]="datas.access_token" type="text" class="
                                                        form-control form-control-lg form-control-solid
                                                        bg-white
                                                        custom-input
                                                        mb-3 mb-lg-0
                                                        borderc-secondary " name="template_name" />

                                                </div>
                                            </div><br>
                                            <div class="text-center my-4">
                                                <!-- <button class="btn-sm text-white me-2" [disabled]="isSubmitting" [ngClass]="{
                                                  'primary-button': !reactiveForm.invalid,
                                                  'secondary-button': reactiveForm.invalid
                                                }" (click)="validate()" [disabled]="reactiveForm.invalid">
                                              <i class="fas fa-check text-white fs-5"></i> &nbsp; Submit
                                            </button> -->
                                                <button class="btn-danger btn-sm text-white me-4"
                                                    data-bs-dismiss="modal">
                                                    <i class="fas fa-close text-white"></i> &nbsp;Close
                                                </button>
                                            </div>


                                        </div>

                                    </div>

                                </div>
                                <div class="row tab-pane fade ms-5" id="kt_tab_pane_7" role="tabpanel">
                                    <div class="table-responsive">
                                        <table id="document_list"
                                            class="table table-striped table-row-bordered gy-5 gs-7 ">
                                            <thead>
                                                <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                                                    <th>S.No</th>
                                                    <th> Document Type</th>
                                                    <th>Document Name</th>
                                                    <th>Created By </th>
                                                    <th>Created Date</th>
                                                    <th class="text-center">Action <button title="Download"
                                                            style="color: #009EF7; background: transparent; border:none"><em
                                                                class="fa fa-download"
                                                                style="color: #009EF7;"></em></button></th>

                                                </tr>
                                            </thead>
                                        </table>
                                        <tbody>
                                            <!-- <tr *ngFor="let data of campaignservice_list ; let i=index;">
                        <td >{{i+1}}</td>
                        <td>{{data.hrdocument_name }}</td>
                        <td>{{data.hrdoc_name }}</td>
                        <td>{{data.created_by}}</td>
                        <td>{{data.created_date}}</td>
                        <td class="text-center text-nowrap">
                        
            
    </tr> -->
                                        </tbody>
                                    </div>
                                </div>





                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- <div class="text-center my-4"><button class="btn-primary btn-sm text-white me-4 "(click)="backbutton()">
                        <i class="fas fa-arrow-left"></i> &nbsp;Back</button></div> -->