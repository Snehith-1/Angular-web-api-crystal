onview(params:any){
  const secretKey = 'storyboarderp';
  const param = (params);
  const encryptedParam = AES.encrypt(param,secretKey).toString();
  this.route.navigate(['/smr/SrmTrnCustomerview',encryptedParam]) 
}
onedit(params:any){
  const secretKey = 'storyboarderp';
  const param = (params);
  const encryptedParam = AES.encrypt(param,secretKey).toString();
  this.route.navigate(['/smr/SmrTrnCustomeredit',encryptedParam]) 
}
onaddinfo(){}
openModaldelete(parameter: string){
  this.parameterValue = parameter
}
ondelete(){
  console.log(this.parameterValue);
  var url = 'SmrTrnCustomerSummary/GetDeleteCustomerdetails'
  let param = {
    customer_gid : this.parameterValue 
  }
  this.service.getparams(url,param).subscribe((result: any) => {
    if(result.status ==false){
      this.ToastrService.warning(result.message)
    }
    else{
         
      this.ToastrService.success(result.message)
      this.NgxSpinnerService.hide();
      this.GetSmrTrnCustomerSummary();
    
  }
  });
}
