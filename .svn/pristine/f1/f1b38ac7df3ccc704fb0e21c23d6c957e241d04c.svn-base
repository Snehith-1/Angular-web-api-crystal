<!-- begin::Add PO -->
<div class="card m-3">
    <!-- begin::Heading -->
    <div class="card-header bg-lightblue card-height55">
      <div class="card-title m-0">
        <h5 class="fw-bolder text-blue"> Raise Enquiry</h5>
      </div>
    </div>
    <!-- end::Heading -->
  
    <!-- begin:: PO Add Form-->
    <form [formGroup]="RaiseForm" #form="ngForm">
      <div class="d-flex flex-row m-6 flex-wrap">
  
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>SO Ref. No</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
              placeholder="New Purchase Order"  formControlName="salesorder_gid" class="form-control" />
          </div>
        </div>
  
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>SO Date</b><span class="ms-1 text-red">*</span>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="date"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
               formControlName="salesorder_date" class="form-control" />
          </div>
        </div>
  
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Branch Name<span class="ms-1 text-red">*</span></b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <ng-select formControlName="branch_name" 
              [class.is-invalid]="branch_name.invalid && (branch_name.dirty || branch_name.touched)"
              [ngClass]="{'invalid-border': branch_name.invalid && branch_name.touched}" [items]="branch_list" required
              class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="branch_name"
              bindValue="branch_gid" placeholder="Select an option" [(ngModel)]="mdlBranchName">
            </ng-select>
            <div *ngIf="
            branch_name.invalid &&
                                  (branch_name.dirty || branch_name.touched)
                                " class="invalid-feedback">
              <div *ngIf="branch_name.errors?.['required']">
                Select Branch!.
              </div>
            </div>
          </div>
        </div>

        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Customer Ref. No</b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <input type="text"
                class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
                placeholder="New Purchase Order"  formControlName="so_referencenumber" class="form-control" />
            </div>
          </div>
      </div>
      <div class="d-flex flex-row m-6 flex-wrap">
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Customer Name<span class="ms-1 text-red">*</span></b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <ng-select formControlName="customer_name" (change)="OnChangeCustomer()"
              [class.is-invalid]="customer_name.invalid && (customer_name.dirty || customer_name.touched)"
              [ngClass]="{'invalid-border': customer_name.invalid && customer_name.touched}" [items]="customer_list" required
              class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="customer_name"
              bindValue="customer_gid" placeholder="Select an option" [(ngModel)]="mdlCustomerName">
              <option *ngFor="let customer of customer_list" [value]="customer.customer_gid">
                {{customer.customer_name}}</option>
            </ng-select>
            <div *ngIf="
            customer_name.invalid &&
                                  (customer_name.dirty || customer_name.touched)
                                " class="invalid-feedback">
              <div *ngIf="customer_name.errors?.['required']">
                Customer Name is required.
              </div>
            </div>
          </div>
        </div>
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Contact Person<span class="ms-1 text-red">*</span></b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <ng-select formControlName="customercontact_gid"
              [class.is-invalid]="customercontact_names.invalid && (customercontact_names.dirty || customercontact_names.touched)"
              [ngClass]="{'invalid-border': customercontact_names.invalid && customercontact_names.touched}" [items]="contact_list"
              required class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="customercontact_gid"
              bindValue="customercontact_gid" placeholder="Select an option" [(ngModel)]="mdlContactName">
              <option *ngFor="let contact of contact_list" [value]="contact.customercontact_gid">
                {{contact.customercontact_names}}</option>
            </ng-select>
            <div *ngIf="
                              customercontact_names.invalid &&
                                  (customercontact_names.dirty || customercontact_names.touched)
                                " class="invalid-feedback">
              <div *ngIf="customercontact_names.errors?.['required']">
                Contact Name is required.
              </div>
            </div>
          </div>
        </div>
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Contact Number</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
              placeholder="Auto Populate" readonly formControlName="customer_mobile" class="form-control" />
          </div>
        </div>
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Email Address</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
              placeholder="Vendor Fax" formControlName="customer_email" class="form-control" />
          </div>
        </div>
      </div>
      <div class="d-flex flex-row m-6 flex-wrap">
       
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Remarks</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <textarea type="text" rows="1"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0 borderc-secondary bg-white custom-input"
              placeholder="Auto Populate" readonly formControlName="so_remarks" class="form-control" ></textarea>
          </div>
        </div>
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Customer Address</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <textarea type="text" rows="1"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
              placeholder="Auto Populate" readonly formControlName="customer_address" class="form-control"></textarea>
          </div>
        </div>
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Shipping  Address</b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <textarea type="text" rows="1"
                class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
                placeholder="Auto Populate" readonly formControlName="shipping_to" class="form-control"></textarea>
            </div>
          </div>
          <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Sales Person<span class="ms-1 text-red">*</span></b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <ng-select formControlName="user_name"
                [class.is-invalid]="user_name.invalid && (user_name.dirty || user_name.touched)"
                [ngClass]="{'invalid-border': user_name.invalid && user_name.touched}" [items]="user_list" required
                class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="user_name"
                bindValue="salesperson_gid" placeholder="Select an option" [(ngModel)]="mdlUserName">
                <option value="" disabled selected>Select Sales Person</option>
                <option *ngFor="let user of user_list" [value]="user.salesperson_gid">
                  {{user.customer_name}}</option>
              </ng-select>
              <div *ngIf="
              customer_name.invalid &&
                                    (customer_name.dirty || customer_name.touched)
                                  " class="invalid-feedback">
                <div *ngIf="customer_name.errors?.['required']">
                  Customer Name is required.
                </div>
              </div>
            </div>
          </div>
      </div>
          <div class="d-flex flex-row m-6 flex-wrap">
          
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Expected Start Date</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
              placeholder="1" formControlName="start_date" class="form-control" />
          </div>
        </div>
      
    
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Esimated Time of Arraival</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
              placeholder="Ship Via" formControlName="end_date" class="form-control" />
          </div>
        </div>
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Freight Terms</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
              placeholder="Freight terms" formControlName="freight_terms" class="form-control" />
          </div>
        </div>
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Payment Terms</b>
          <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
            <input type="text"
              class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
              placeholder="Payment Terms" formControlName="payment_terms" class="form-control" />
          </div>
        </div>
      </div>

      <div class="d-flex flex-row m-6 flex-wrap">
        
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Currency<span class="ms-1 text-red">*</span></b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <select class="form-select form-select-solid bg-white form-select-lg borderc-secondary custom-input select2"
                formControlName="currency_code" (change)="OnChangeCurrency($event)" (change)="onCurrencyCodeChange($event)">
                <option value="" disabled selected>Select Currency</option>
                <option *ngFor="let currency of currency_list" [value]="currency.currencyexchange_gid">
                  {{currency.currency_code}}</option>
              </select>
            </div>
          </div>
      
    
        <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Exchane Rate</b>
            <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
              <input type="text"
                class="form-control form-control-lg form-control-solid custom-input mb-3 mb-lg-0  borderc-secondary bg-white custom-input"
                placeholder="Delivery Location" formControlName="exchange_rate" class="form-control" />
            </div>
          </div>
      </div>
  
      
    
      <!-- end:: PO Add Form -->
  
      <!-- begin::Product Summary -->
      <div class="card shadow-sm">
        <!-- begin::Heading -->
        <div class="card-header bg-lightblue card-height55" style="border-radius: 0px;">
          <div class="card-title m-0">
            <h5 class="fw-bolder text-blue">Product Summary</h5>
          </div>
        </div>
        <!-- end::Heading -->
        <!-- begin::Product Form -->
        <form [formGroup]="productform" (ngSubmit)="productSubmit()">
          <div class="d-flex flex-row m-6 flex-wrap" style="border-radius: 0px;">
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Product Name<span class="ms-1 text-red">*</span></b>
              <div class="col-lg-10 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <ng-select formControlName="product_name" (change)="GetOnChangeProductName()" [class.is-invalid]="
                          product_name.invalid &&
                              (product_name.dirty || product_name.touched)
                            " [ngClass]="{'invalid-border': product_name.invalid && product_name.touched}"
                  [items]="productdetails_list" required class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2"
                  bindLabel="product_name" placeholder="Select an option" [(ngModel)]="mdlProductName">
                </ng-select>
                <div *ngIf="
                          product_name.invalid &&
                              (product_name.dirty || product_name.touched)
                            " class="invalid-feedback">
                  <div *ngIf="product_name.errors?.['required']">
                    Product Name is required.
                  </div>
                </div>
              </div>
              
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Product Group</b>
              <div class="col-lg-10 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <input type="text" class="form-control" formControlName="productgroup_name" readonly
                  style="background-color: #e9e7e7">
              </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Customer Product Code</b>
                <div class="col-lg-10 col-12 col-md-11 col-sm-11 fv-row mt-2">
                  <input type="text" class="form-control" formControlName="customerproduct_code" readonly
                    style="background-color: #e9e7e7">
                </div>
              </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Product Code</b>
              <div class="col-lg-10 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <input type="text" class="form-control" formControlName="product_code" readonly
                  style="background-color: #e9e7e7">
              </div>
            </div>
            
         
          </div>
          <div class="d-flex flex-row m-6 flex-wrap">
          
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>Unit</b>
              <div class="col-lg-10 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <input type="text" class="form-control" formControlName="productuom" readonly
                  style="background-color: #e9e7e7;">
              </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12">
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row">
                <div class="d-flex flex-row m-0 flex-wrap">
                  <div class="col-lg-6 col-md-6 col-sm-6"><b>Unit Price</b>
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                      <input type="text" class="form-control" placeholder="0.00" style="text-align: right;"
                        formControlName="unitprice" [(ngModel)]="unitprice" (input)="prodtotalcal()">
                    </div>
                  </div>
                  <div class="col-lg-6 col-md-6 col-sm-6"><b>Quantity</b>
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                      <input type="text" class="form-control" style="text-align: right;" formControlName="quantity"
                        [(ngModel)]="quantity" (input)="prodtotalcal()">
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12">
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row">
                <div class="d-flex flex-row m-0 flex-wrap">
                  <div class="col-lg-6 col-md-6 col-sm-6"><b>Discount(%)</b>
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                      <input type="text" class="form-control" placeholder="0.00" style="text-align: right;"
                        formControlName="margin_percentage" [(ngModel)]="discountpercentage" (input)="prodtotalcal()">
                    </div>
                  </div>
                  <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
                      <input type="text" class="form-control" placeholder="0.00" readonly
                        style="background-color: #e9e7e7; text-align: right;" formControlName="margin_amount"
                        [value]="discountamount" [(ngModel)]="discountamount" (input)="prodtotalcal()">
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12"><b>MRP</b>
              <div class="col-lg-10 col-12 col-md-11 col-sm-11 fv-row mt-2">
                <input type="text" class="form-control" formControlName="selling_price" readonly
                  style="background-color: #e9e7e7">
              </div>
            </div>
           
          </div>
          <div class="d-flex flex-row m-6 flex-wrap">
            <div class="col-12 col-lg-3 col-md-3 col-sm-12">
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row">
                <div class="d-flex flex-row m-0 flex-wrap">
                  <div class="col-lg-6 col-md-6 col-sm-6"><b>Need By Date</b>
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                      <input type="text" class="form-control" placeholder="0.00" style="text-align: right;"
                        formControlName="discountpercentage">
                    </div>
                  </div>
                  <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
                      <textarea type="text" class="form-control" rows="1" readonly
                        style="background-color: #e9e7e7; text-align: right;" placeholder="Description" formControlName="margin_price"></textarea>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12">
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row">
                <div class="d-flex flex-row m-0 flex-wrap">

                  <div class="col-lg-6 col-md-6 col-sm-6"><b>Tax 1</b>
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    
                      <ng-select formControlName="tax_name" (change)="OnChangeTaxAmount1()"
                        [class.is-invalid]="tax_name.invalid && (tax_name.dirty || tax_name.touched)"
                        [ngClass]="{'invalid-border': tax_name.invalid && tax_name.touched}" [items]="tax_list"
                        required class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="tax_name"
                        bindValue="tax_gid" placeholder="Select an option" [(ngModel)]="mdlTaxName1">
                      </ng-select>
                    </div>
                  </div>
                  <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
                      <input type="text" class="form-control" placeholder="0.00" readonly
                        style="text-align: right; background-color: #e9e7e7;" formControlName="tax_amount"
                        [(ngModel)]="tax_amount" (input)="OnChangeTaxAmount1()">
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12">
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row">
                <div class="d-flex flex-row m-0 flex-wrap">
                  <div class="col-lg-6 col-md-6 col-sm-6"><b>Tax 2</b>
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    
                      <ng-select formControlName="tax_name2" (change)="OnChangeTaxAmount2()"
                        [class.is-invalid]="tax_name2.invalid && (tax_name2.dirty || tax_name2.touched)"
                        [ngClass]="{'invalid-border': tax_name2.invalid && tax_name2.touched}" [items]="tax_list"
                        required class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="tax_name2"
                        bindValue="tax_gid" placeholder="Select an option" [(ngModel)]="mdlTaxName2">
                      </ng-select>
                    </div>
                  </div>
                  <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
                      <input type="text" class="form-control" placeholder="0.00" readonly
                        style="text-align: right; background-color: #e9e7e7;" formControlName="tax_amount2"
                        [(ngModel)]="tax_amount2" (input)="OnChangeTaxAmount2()">
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-12 col-lg-3 col-md-3 col-sm-12">
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row">
                <div class="d-flex flex-row m-0 flex-wrap">
                  <div class="col-lg-6 col-md-6 col-sm-6"><b>Tax 3</b>
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                      <ng-select formControlName="tax_name3" (change)="OnChangeTaxAmount3()"
                        [class.is-invalid]="tax_name3.invalid && (tax_name3.dirty || tax_name3.touched)"
                        [ngClass]="{'invalid-border': tax_name3.invalid && tax_name3.touched}" [items]="tax_list"
                        required class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="tax_name3"
                        bindValue="tax_gid" placeholder="Select an option" [(ngModel)]="mdlTaxName3">
  
                      </ng-select>
                    </div>
                  </div>
                  <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
                      <input type="text" class="form-control" placeholder="0.00" readonly
                        style="text-align: right; background-color: #e9e7e7;" formControlName="tax_amount3"
                        [(ngModel)]="tax_amount3" (input)="OnChangeTaxAmount3()">
                    </div>
                  </div>
                </div>
              </div>
            </div>
            </div>
            <div class="d-flex flex-row m-6 flex-wrap">
            <div class="col-12 col-lg-3 col-md-3 col-sm-12">
              <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row">
                <div class="d-flex flex-row m-0 flex-wrap">
                  <div class="col-lg-6 col-md-6 col-sm-6"><b>Total Amount</b>
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                      <input type="text" class="form-control" placeholder="0.00"
                        style="background-color: #e9e7e7; text-align: right;" formControlName="price"
                        [(ngModel)]="price" (input)="prodtotalcal()">
                    </div>
                  </div>
                  <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-8">
                      <button type="button" class="btn-primary" style="margin-left: 20px;"
                        (click)="productSubmit()">ADD</button>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </form>
        <!-- end::Product Form -->
        <!-- begin::table -->
        <div class="table-responsive">
          <div class="card card-body table-responsive m-0 shadow-lg p-3 mb-5 bg-white rounded">
            <table id="POproductlist" class="table table-striped table-row-bordered gy-4 gs-2">
              <thead class="table_head">
                <tr class="text-white fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                  <th>S.No</th>
                  <th>Product Name</th>
                  <th>Product Group</th>
                  <th>Customer Product Code</th>
                  <th>Product Code</th>
                  <!-- <th>HSN Code / Description</th> -->
                  <th>Unit</th>
                  <th style="text-align: right;">Unit Price</th>
                  <th style="text-align: right;">MRP</th>
                  <th style="text-align: center;">Quantity</th>
                  <th style="text-align: right;">Discount (%)</th>
                  <th style="text-align: right;">Need By Date</th>
                  <th style="text-align: right; width: 120px;">Tax</th>
                  <th style="text-align: right;">Total Amount</th>
                  <th class="text-center">Action</th>
                </tr>
              </thead>
              <tbody>
                <tr *ngFor="let data of POproductlist; let i=index;">
                  <td>{{i+1}}</td>
                  <td>{{data.product_name}}</td>
                  <td>{{data.productgroup_name}}</td>
                  <td>{{data.customerproduct_code}}</td>
                  <td>{{data.product_code}}</td>
                  <td>{{data.uom_name}}</td>
                  <td style="text-align: right;">{{data.unit_price | number}}</td>
                  <td style="text-align: right;">{{data.selling_price | number}}</td>
                  <td style="text-align: center;">{{data.qty_quoted}}</td>
                  <td style="text-align: right;">{{data.margin_percentage}}</td>
                  <td style="text-align: right;">{{data.product_requireddate}}</td>
                  <td style="text-align: right;">{{data.tax_percentage}}&nbsp;{{data.tax_percentage2}}&nbsp;{{data.taxpercentage3}}</td>
                  <td style="text-align: right;">{{data.price}}</td>
                  <td class="text-center text-nowrap">
                    <button title="Delete" type="button" (click)="ondelete(data.product_gid)" data-bs-toggle="modal"
                      data-bs-target="#myModaldelete" class="btn btn-icon btn-sm bg-danger me-2"><span
                        class="fa-solid fa-trash text-white  fs-6"></span></button>
                  </td>
                </tr>
              </tbody>
            </table>
          </div>
        </div>
        <!-- end::table -->
        <!-- begin::calculation part -->
        <div class="card">
          <div class="d-flex mt-4 me-15">
            <div>
              <label class="lab">Payment :</label>
              <input type="text" class="in" formControlName="payment_days">
              <span class="spn">Days</span>
            </div>
            <div>
              <label class="lab ms-10">Delivery Period :</label>
              <input type="text" class="in" formControlName="delivery_days">
              <span class="spn">Days</span>
            </div>
          </div>
  
       
          
          <!-- end::calculation part -->
    
        <!-- end::Product Summary -->
  
        <div class="card m-3">
          <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4">
              <label class="tbl_lab">Net Amount:</label>
              <input type="text" formControlName="totalamount" readonly class="tbl_in" placeholder="0.00"
              [(ngModel)]="totalamount" (input)="finaltotal()">
              <span class="tbl_spn">{{ selectedCurrencyCode }}</span>
            </div>
          </div>
          <br>
          <div class="d-flex justify-content-end mt-0">
            <div class="col-ms-2">
              <label class="tbl_lab">Tax :</label>
            </div>
            <div class="col-lg-1">
              <ng-select formControlName="tax_name" (change)="OnTax()"
                [class.is-invalid]="tax_name.invalid && (tax_name.dirty || tax_name.touched)"
                [ngClass]="{'invalid-border': tax_name.invalid && tax_name.touched}" [items]="tax_list" required
                class="custom col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-0" bindLabel="tax_name" bindValue="tax_gid"
                placeholder="Select Tax" [(ngModel)]="mdlTaxName2">
              </ng-select>
            </div>
            <div>
              <input type="text" formControlName="total_amount" class="tbl_in mt-3 ms-12 me-19" placeholder="0.00"
                (input)="OnTax()" readonly>
              <span class="tbl_spn">{{ selectedCurrencyCode }}</span>
            </div>
          </div>
          <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4">
              <label class="tbl_lab">Add On Charges :</label>
              <input type="text" formControlName="addoncharge" class="tbl_in" placeholder="0.00"
              [(ngModel)]="addoncharge" (input)="finaltotal()">
              <span class="tbl_spn">{{ selectedCurrencyCode }}</span>
            </div>
          </div>
          <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4">
              <label class="tbl_lab">Additional Discount :</label>
              <input type="text" formControlName="additional_discount" class="tbl_in" placeholder="0.00"
              [(ngModel)]="additional_discount" (input)="finaltotal()">
              <span class="tbl_spn">{{ selectedCurrencyCode }}</span>
            </div>
          </div>
          <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4">
              <label class="tbl_lab">Freight Charges :</label>
              <input type="text" formControlName="freightcharges" class="tbl_in" placeholder="0.00"
              [(ngModel)]="freightcharges" (input)="finaltotal()">
              <span class="tbl_spn">{{ selectedCurrencyCode }}</span>
            </div>
          </div>
          <br>
          <div class="d-flex justify-content-end mt-0">
            <div class="col-ms-1">
              <label class="tbl_lab">Freight Tax :</label>
            </div>
           
            <div>
              <input type="text" formControlName="taxamount1" class="tbl_in mt-3 ms-12 me-19" placeholder="0.00"
                (input)="OnTax()" readonly>
              <span class="tbl_spn">{{ selectedCurrencyCode }}</span>
            </div>
          </div>
          <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4">
              <label class="tbl_lab">Buy Back/Scrap :</label>
              <input type="text" formControlName="buybackorscrap" class="tbl_in" placeholder="0.00"
              [(ngModel)]="buybackorscrap" (input)="finaltotal()">
              <span class="tbl_spn">{{ selectedCurrencyCode }}</span>
            </div>
          </div>
          <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4">
              <label class="tbl_lab">Packing Charges :</label>
              <input type="text" formControlName="packing_charges" class="tbl_in" placeholder="0.00">
              <span class="tbl_spn">{{ selectedCurrencyCode }}</span>
            </div>
          </div>
          <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4">
              <label class="tbl_lab">Insurance Charges :</label>
              <input type="text" formControlName="insurance_charges" class="tbl_in" placeholder="0.00"
              [(ngModel)]="insurance_charges" (input)="finaltotal()">
              <span class="tbl_spn">{{ selectedCurrencyCode }}</span>
            </div>
          </div>
          <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4">
              <label class="tbl_lab">Round Off :</label>
              <input type="text" formControlName="roundoff" class="tbl_in" placeholder="0.00"
              [(ngModel)]="roundoff" (input)="finaltotal()">
              <span class="tbl_spn">{{ selectedCurrencyCode }}</span>
            </div>
          </div>
          <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4">
              <label class="tbl_lab">Grand Total :</label>
              <input type="text" formControlName="grandtotal" readonly class="tbl_in" placeholder="0.00"
                [(ngModel)]="grandtotal" (input)="finaltotal()">
              <span class="tbl_spn">{{ selectedCurrencyCode }}</span>
            </div>
          </div>
          <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4">
              <label class="tbl_lab">Payment :</label>
              <input type="text" formControlName="payment_days" class="tbl_in" placeholder="0">
              <span class="tbl_spn">Days</span>
            </div>
          </div>
          <div class="d-flex justify-content-end mt-4">
            <div class="col-md-4">
              <label class="tbl_lab">Delivery Period :</label>
              <input type="text" formControlName="delivery_days" class="tbl_in" placeholder="0">
              <span class="tbl_spn">Days</span>
            </div>
          </div>
       
        <div>
          <div class="card-header bg-lightblue card-height55">
            <div class="card-title m-0">
              <h5 class="fw-bolder text-blue">Terms and Conditions</h5>
            </div>
          </div>
          <div class="d-flex flex-row m-1 flex-wrap">
            <!-- <div class="col-10 col-lg-5 col-md-10 col-sm-10 fv-row mt-2">
              <angular-editor [config]="config" formControlName="template_content"></angular-editor>
            </div> -->
          </div>
        </div>
  
        <!-- begin::Submit button -->
        <div class="text-center my-4">
          <button type="submit" class="btn-success btn-sm text-white me-2" (click)="onSubmit()">
            <i class="bi bi-check-square fs-5"></i>
            <span class="ms-3">Submit</span> </button>
          <button class="btn-primary btn-sm text-white me-4" (click)="redirecttolist()">
            <i class="ki-duotone ki-entrance-right fs-5">
              <i class="path1"></i>
              <i class="path2"></i>
            </i>&nbsp;Back</button>
        </div>
        <!-- end::Submit button -->
      </div> </div></div>
    </form>
</div>
    <!-- end::Add PO -->