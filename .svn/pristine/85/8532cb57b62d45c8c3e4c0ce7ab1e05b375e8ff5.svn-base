    <div class="table-responsive">
        <div class="card card-body m-8 shadow-xl p-5 mb-8 bg-white rounded">
            <table id="upcoming" class="table table-striped table-row-bordered gy-5 gs-7 ">
                <thead>
                    <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                      
                        <th>S.No</th>
                        <!-- <th>Branch</th>
                        <th>Team/Employee</th> -->
                        <!-- <th>Leads Name</th> -->
                        <th>Lead Name</th>
                       
                        <th>Contact</th>
                        <th>Customer Type</th>
                        <th>Lead Notes</th>
                        <th>Lead Status</th>
                        <!-- <th>Assign Status</th> -->
                        <th>Created by</th>
                        <th class="text-center">Action</th>
                       
                    </tr>
                </thead>
                <tbody>

                   <tr *ngFor="let data of upcoming; let i=index;">
    <td style="white-space: nowrap;">{{i+1}}</td>
                        <!-- <td style="white-space: nowrap;">{{data.branch_name}}</td> -->
                        <!-- <td>{{data.user_firstname}}</td> -->
                        <!-- <td>{{data.user_firstname}}</td> -->
                        <td>{{data.leadbank_name}}</td>
                        <td>{{data.Contact}}</td>
                        <th>{{data.customer_type}}</th>
                        <td class="text-center text-nowrap">
                            <button title="Remarks" type="button" (click)="popmodal(data.remarks,data.leadbank_name)" data-bs-toggle="modal"
                                data-bs-target="#myModalUpdateimage9" class="btn btn-icon btn-sm bg-secondary">
                                <i class="fa-solid fa-question  fs-4"style="color:grey; font-size: 20px;"></i>
                            </button>
                        </td>
                        <td>{{data.lead_status}}</td>
                        <!-- <td>{{data.assign_to}}</td> -->
                           <td>{{data.created_by}}</td>
                           <td class="text-center text-nowrap">
                            <!-- <button title="Schedule"type="button" class="btn-success btn btn-icon btn-sm " data-bs-toggle="modal"
                            data-bs-target="#myModalschedule"><i class="fas fa-clock"></i></button>&nbsp;
                        <button title="Transfer" type="button"class="btn-secondary btn-sm text-white me-4" data-bs-toggle="modal"
                            data-bs-target="#myModaltransfer"><i class="bi bi-arrows-move"></i></button>&nbsp;
                        <button title="Bin" type="button"class="btn-danger btn-sm text-white me-4" ><span
                                class="fa-solid fa-trash text-white  fs-6"></span></button>&nbsp; -->
                                <!-- (click)="OnBin()" -->
                                <button title="Schedule" type="button" class="btn btn-icon btn-sm bg-gradient-darkoranges me-2" ><i class="fas fa-clock"></i></button>
                <button title="Transfer" type="button" class="btn btn-icon btn-sm bg-gradient-purple me-2"><i class="bi bi-arrows-move"></i></button>      
                <button title="Bin" type="button" data-bs-toggle="modal" data-bs-target="#myModaldelete"class="btn btn-icon btn-sm bg-danger me-2"><span
                  class="fa-solid fa-trash text-white  fs-6"></span></button>
                        </td>
                       
                        <br> <br>

        
                    </tr>
                           
                </tbody>

            </table>
