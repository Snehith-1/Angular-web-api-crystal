<div class="card">
    <div class="row ms-3 mt-3 d-flex align-items-center">
      <div class="col">
        <div class="col-12 card shadow-lg p-3 mb-3 bg-white rounded">
          <div class="col-12 card  bg-white rounded">
              <div class="card-header justify-content-center " style="background-color:rgb(25, 167, 206)">
                <h3 class="card-title fs-4 fw-bolder text-white">Sales  Report Month Wise </h3>
            </div>
                <div class="card card-body p-3 mb-5 ">
    <div class="table-responsive">
      
        <table id="GetMonthwiseOrderReport_List" class="table table-striped table-row-bordered gy-5 gs-7 ">
          <thead>
            <tr class="fw-bold fs-7 text-nowrap bg-lightblue text-blue align-middle">
              <th>S.NO </th>
              <th>Month</th>
              <th>Total SO </th>
              <th>Total Order Value(INR)</th>
              <th>Total Invoice Value(INR)</th>
              <th>Total Payment Value(INR)</th>
            </tr>
          </thead>
          <tbody>
            <tr *ngFor="let data of GetMonthwiseOrderReport_List; let i=index;">
                <td>{{i+1}}</td>
                <td>{{data.month_wise}}</td>
                <td>{{data.so_total}}</td>
                <td>{{data.total}}</td>
                <td>{{data.total_invoice}}</td>
                <td>{{data.total_payment}}</td>
            </tr>
          </tbody>
        </table>
      
    </div>
  </div>
  <div class="card">
    <div class="row  d-flex align-items-center">
      <div class="col">
        <div class="col-12 card shadow-lg p-3 mb-3 bg-white rounded">
          <div class="col-12 card  bg-white rounded">
              <div class="card-header justify-content-center " style="background-color:rgb(25, 167, 206)">
                <h3 class="card-title fs-4 fw-bolder text-white">Sales  Report Order Wise </h3>
            </div>
                <div class="card card-body p-3 ">
    <div class="table-responsive">
      
        <table id="GetOrderwiseOrderReport_List" class="table table-striped table-row-bordered gy-5 gs-7 ">
          <thead>
            <tr class="fw-bold fs-7 text-nowrap bg-lightblue text-blue align-middle">
              <th>S.NO </th>
              <th>Month</th>
              <th>Total SO </th>
              <th>Total Order Value(INR)</th>
              <th>Total Invoice Value(INR)</th>
              <th>Total Payment Value(INR)</th>
            </tr>
          </thead>
          <tbody>
            <tr *ngFor="let data of GetOrderwiseOrderReport_List; let i=index;">
              <td>{{i+1}}</td>
              <td>{{data.month_wise}}</td>
              <td>{{data.so_total}}</td>
              <td>{{data.total}}</td>
              <td>{{data.total_invoice}}</td>
              <td>{{data.total_payment}}</td>
            </tr>
          </tbody>
        </table>
   
    </div>
  </div>