<!-- The Modal ADD -->
<form [formGroup]="reactiveForm" #form="ngForm">
    <div class="modal" id="myModaladd" data-bs-backdrop="static" data-bs-keyboard="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <!-- Modal Header -->
                <div class="card-header p-1 pb-2 pt-1 rounded-top btn-send">
                    <h2 style="margin-top:10px;color:white;font-weight: bold;">&nbsp;&nbsp;Create new contact</h2>
                </div>
                <!-- Modal body -->
                <div class="modal-body">
                    <div class="row"><span class="font-bold"><b>Display Name</b>
                        <span
                            class="ms-1 text-red">*</span></span>
                        <div class="col-lg-12 col-md-12 col-sm-12 fv-row mt-2 d-flex ">
                            <input formControlName="displayName" type="text" class="
                     form-control form-control-lg form-control-solid
                     custom-input
                     mb-3 mb-lg-0
                     bg-white
                     custom-input
                   " [ngStyle]="{
                     'border-color':
                     displayName.invalid && displayName.touched
                         ? 'red'
                         : 'rgb(209, 209, 209)'
                   }" placeholder="Enter Customer Name"pattern="^(?!\s*$).+"  name="displayName" required />
                        </div>
                    </div>
                    <div *ngIf="displayName.invalid && (displayName.dirty || displayName.touched)">
                        <div *ngIf="displayName.errors?.['required']">
                            <span style="color:red;">Customer name is required.</span>
                        </div>
                    </div>

                    <div *ngIf="displayName.invalid && (displayName.dirty || displayName.touched)">
                        <div *ngIf="displayName.errors?.['pattern']">
                            <span style="color:red;">Invalid Format</span>
                        </div>
                    </div>
                    <br>
                    <div class="row"><span class="font-bold"><b>First Name</b>
                       </span>
                        <div class="col-lg-12 col-md-12 col-sm-12 fv-row mt-2 d-flex ">
                       <input formControlName="firstName" type="text" class="
                     form-control form-control-lg form-control-solid
                     custom-input
                     mb-3 mb-lg-0
                     bg-white
                     custom-input
                     border-secondary
                   " 
                    placeholder="Enter First Name"pattern="^(?!\s*$).+"  name="firstName" required />
                        </div>
                    </div>
                    <br>
                    <div class="row"><span class="font-bold"><b>Last Name</b>
                        </span>
                        <div class="col-lg-12 col-md-12 col-sm-12 fv-row mt-2 d-flex ">
                            <input formControlName="lastName" type="text" class="
                     form-control form-control-lg form-control-solid
                     custom-input
                     mb-3 mb-lg-0
                     bg-white
                     custom-input
                     border-secondary
                   " 
                   placeholder="Enter Last Name"pattern="^(?!\s*$).+"  name="lastName" />
                        </div>
                    </div>
                    <br>
                    <div class="row">
                        <span class="font-bold"><b>Mobile Number</b></span>
                        <div class="col-lg-12 col-12 col-md-11 col-sm-11 fv-row mt-2">
                          <input type="text" id="mobile" name="mobile" formControlName="value"
                          class="
                              form-control form-control-lg form-control-solid
                              custom-input
                              mb-3 mb-lg-0
                              bg-white
                              custom-input
                            " [ngStyle]="{
                              'border-color':
                              value.invalid && value.touched
                                  ? 'red'
                                  : 'rgb(209, 209, 209)'
                            }"  pattern="^\+91\d{10}$" placeholder="Enter Mobile"  maxlength="13"  autocomplete="off" >
                             
                            <div *ngIf="value.errors?.['pattern']"class="error-text">
                              Mobile contains 10 numbers starts with +91.
                            </div>
                        </div>
                      </div>

                      <div class="col-lg-12 col-md-12 col-sm-12 fv-row mt-2 d-flex ">
                        <div class="col-lg-6 col-md-6 col-sm-6 fv-row mt-2">
                        <span class="font-bold"><b>Gender</b></span>
                        <div class="d-flex justify-content-start mt-4 ms-2">
                            <div class="
                              form-check
                              form-check-custom
                              form-check-primary
                              form-check-solid
                              form-check-sm
                            ">
                                <input class="form-check-input" type="radio" name="gender" checked="checked"
                                    formControlName="gender" value="M" />
                                <label class="form-check-label text-gray-700 text-nowrap"
                                    for="kt_builder_header_content_menu">Male</label>
                            </div>
                            <div class="
                              form-check
                              form-check-custom
                              form-check-primary
                              form-check-solid
                              form-check-sm
                              ms-10
                            ">
                                <input class="form-check-input" type="radio" name="gender"
                                    formControlName="gender" value="F" />
                                <label class="form-check-label text-gray-700 text-nowrap"
                                    for="kt_builder_header_content_page-title">Female</label>
                            </div>
                        </div>
                    </div>
                <br>
                
               
                    </div>
                  
                    <!-- Modal footer -->
                    <div class="text-center my-4">
                        <button (click)="onsubmit()"  [ngClass]="{
                'primary-button': !reactiveForm.invalid,
                'secondary-button': reactiveForm.invalid
              }" data-bs-dismiss="modal" class="btn-send">
                            <i class="fas fa-check text-white fs-5 "></i> &nbsp;Submit
                        </button> &nbsp;&nbsp;
                        <button class="btn-danger btn-sm text-white me-4" (click)="onclose()" data-bs-dismiss="modal">
                            Cancel
                        </button>
                    </div>

                </div>
            </div>
        </div>
    </div>
</form>

