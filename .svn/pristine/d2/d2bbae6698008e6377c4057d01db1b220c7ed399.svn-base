<div class="card shadow-sm">
    <div class="card-header">
        <h3 class="card-title">GRN Inward</h3>
    </div>
    <br>
    <div> &nbsp; &nbsp;
        <!-- ADD Function -->
        <button type="button" class="btn-primary btn-sm text-white me-4" data-bs-toggle="modal"
            data-bs-target="#myModaladd" (click)="onadd()">
            <i class></i>  Add GRN Inward
        </button>
    </div>
    <div class="table-responsive">
        <div class="card card-body m-3 shadow-lg p-3 mb-5 bg-white rounded">
            <table id="GrnInward_lists" class="table table-striped table-row-bordered gy-5 gs-7 ">
                <thead>
                    <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                        <th>S.No </th>
                        <th>GRN Date </th>
                        <th>GRN Ref.No.</th>
                        <th>PO Ref.No.</th>
                        <th>Ref. No</th>
                        <th>Vendor</th>
                        <th>Cost Center </th>
                        <th>PO Amount</th>
                        <th>Inward On</th>
                        <th>GRN Status</th>
                        <th>DC No</th>
                        <th class="text-center">Action</th>
                    </tr>
                </thead>
                <tbody>
                    <tr *ngFor="let data of GrnInward_lists; let i=index;">
                        <td>{{i+1}}</td>
                        <td>{{data.grn_date}}</td>
                        <td>{{data.grn_gid}}</td>
                        <td>{{data.grnrefno}}</td>
                        <td>{{data.refrence_no}}</td>
                        <td>{{data.vendor_companyname}}</td>
                        <td>{{data.costcenter_name}}</td>
                        <td>{{data.po_amount}}</td>
                        <td>{{data.created_date}}</td>
                        <td>{{data.invoice_flag}}</td>
                        <td>{{data.dc_no}}</td>
                        <td class="text-center text-nowrap">

                            <!-- routerLink="/pmr/PmrTrnGrninwardView -->

                            <button title="View" type="button" class="btn btn-icon btn-sm bg-primary me-2" (click)="onview(data.grn_gid)" >  <span
                                 class="fa-solid fa-eye text-white  fs-6"></span></button>       


                            <button title="Edit" type="button" class="btn btn-icon btn-sm bg-success me-2"><span
                                    class="fa-solid fa-pencil text-white  fs-6"></span></button> 
                                    
                          
                           <button title="Pdf" type="button" data-bs-toggle="modal" data-bs-target="#myModalPdf"
                                class="btn btn-icon btn-sm bg-danger me-2"><span  class="fa-solid fa-file text-white  fs-6"></span></button>
                                   
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>