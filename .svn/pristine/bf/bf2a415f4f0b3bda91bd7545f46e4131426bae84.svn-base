<div class="card shadow-sm">
    <div class="card-header">
        <h3 class="card-title">Lead Bank Summary</h3>
    </div>
    <br>
    <div> &nbsp; &nbsp;
        <!-- ADD Function -->
        <button title="Add Lead" class="btn-primary btn-sm text-white me-4" routerLink="/crm/CrmTrnLeadbankadd">
            <i class="fas fa-plus"></i> &nbsp;&nbsp;&nbsp;
            Add Lead</button>
        <button title="Import Excel" type="button" class="btn btn-icon btn-sm bg-success me-2" style="width: 5%;"
            data-bs-toggle="modal" data-bs-target="#myModalexcelimport"><i class="bi bi-file-earmark-arrow-up"></i></button>
        <button title="Export Excel" type="button" class="btn btn-icon btn-sm bg-warning me-2" style="width: 5%;"
            (click)="exportExcel()"><i class="bi bi-file-earmark-arrow-down"></i></button>
    <br>
    <div class="table-responsive">
        <div class="card card-body m-3 shadow-xl p-3 mb-5 bg-white rounded">
            <table id="leadbank_list" class="table table-striped table-row-bordered gy-5 gs-7 ">
                <thead>
                    <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                        <th>S.No</th>
                        <th>Created Date</th>
                        <th>Company Name</th>
                        <th>Contact Details</th>
                        <!-- <th>Region</th> -->
                        <th>Source/Industry</th>
                        <th>Created By</th>
                        <th>Status</th>
                        <th>Assigned To</th>
                        <th class="text-center">Action</th>
                    </tr>
                </thead>
                <tbody>
                    <tr *ngFor="let data of leadbank_list; let i=index;">
                        <td>{{i+1}}</td>
                        <td>{{data.created_date}}</td>
                        <td>{{data.leadbank_name}}</td>
                        <td>{{data.contact_details}}</td>
                        <!-- <td>{{data.region_name}}</td> -->
                        <td>{{data.source_name}}</td>
                        <td>{{data.created_by}}</td>
                        <td>{{data.lead_status}}</td>
                        <td>{{data.assign_to}}</td>
                        <td class="text-center text-nowrap">
                            <button title="View" type="button" class="btn btn-icon btn-sm bg-primary me-2"
                                (click)="onview(data.leadbank_gid)"><span
                                    class="fa-solid fa-eye text-white  fs-6"></span></button>
                            <button title="Edit" type="button" class="btn btn-icon btn-sm bg-success me-2"
                                (click)="onedit(data.leadbank_gid)"><span
                                    class="fa-solid fa-pencil text-white  fs-6"></span></button>
                            <button title="Delete" type="button" (click)="openModaldelete(data.leadbank_gid)"
                                data-bs-toggle="modal" data-bs-target="#myModaldelete"
                                class="btn btn-icon btn-sm bg-danger me-2"><span
                                    class="fa-solid fa-trash text-white  fs-6"></span></button>
                            <button title="Branch" type="button" class="btn btn-icon btn-sm bg-warning me-2"
                            (click)="onbranch(data.country_gid,data.region_gid,data.leadbank_gid)" >
                                <span class="fas fa-code-branch" routerLink="/crm/CrmTrnLeadBankbranch" ></span> <!-- Branch Icon -->
                            </button>
                            <button title="Contact" type="button" class="btn btn-icon btn-sm bg-info me-2" >
                                <span class="bi bi-telephone me-1 " (click)="oncontact(data.leadbank_gid)" ></span>
                            </button>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
<div class="modal" id="myModaldelete" data-bs-backdrop="static" data-bs-keyboard="false">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="text-center my-4">
                <h2><b>Are you sure?</b></h2>
                <br>
                <p style="display: block;">Do You Want To Delete the Record ?</p><br>
                <br>
                <div class="text-center my-4">
                    <button class="btn-primary btn-sm text-white me-2" data-bs-dismiss="modal">
                        <i class="fas fa-close text-white"></i> &nbsp;Close
                    </button>&nbsp;
                    <button class="btn-danger btn-sm text-white me-4" (click)="ondelete()" data-bs-dismiss="modal">
                        <i class="fas fa-trash text-white fs-5 "></i> &nbsp;Delete
                    </button>
                    <!-- (click)="ondelete()"  -->
                </div>
            </div>
        </div>
    </div>
</div>
<br>

<!-- The Modal Import -->
<form [formGroup]="reactiveForm" #form="ngForm">
    <div class="modal" id="myModalexcelimport" data-bs-backdrop="static" data-bs-keyboard="false">
        <div class="modal-dialog">
            <div class="modal-content">

                <!-- Modal Header -->

                <div class="card-header p-1 pb-2 pt-1 rounded-top bg-primary">
                    <h2 style="margin-top:10px;color:white;font-weight: bold;">&nbsp;&nbsp;<i
                            class="fas fa-lock fw-bolder fs-3"></i> &nbsp;&nbsp;LEAD BANK EXCEL FILE IMPORT</h2>

                </div>

                <!-- Modal body -->
                <div class="modal-body">
                    <div class="col-12 col-lg-4 col-md-6 col-sm-12">
                        <span class="font-bold"><b>Select an Excel File to Import</b><span
                                class="ms-1 text-red">*</span></span>
                        <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" (change)="onChange1($event)">
                            <input style="width: 300%;" accept="image/*" type="file" #fileInput class="
                          form-control form-control-lg form-control-solid bg-white
                          mb-3 mb-lg-0  borderc-secondary custom-input
                        " name="fName" />

                        </div>
                    </div>
                    <br>
                    <!-- Modal footer -->
                    <div class="text-center my-4">
                        <button type="submit" class="btn-success btn-sm text-white me-2" (click)="importexcel()">
                            <i class="bi bi-check-square fs-5"></i>&nbsp;Upload</button>


                        <button class="btn-primary btn-sm text-white me-4 " (click)="onclose()">
                            <i class="fas fa-arrow-left"></i> &nbsp; Back
                        </button>

                    </div>

                </div>
            </div>
        </div>
    </div>
</form>