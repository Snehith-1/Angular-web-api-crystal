<div class="card shadow-sm">
    <div class="card-header">
        <h3 class="card-title">Search Leads</h3>
    </div>
    <br>
    <form [formGroup]="reactiveFormSubmit" #form="ngForm">
        <div class="d-flex flex-row m-6 mt-0 flex-wrap">
            <div class="col-12 col-lg-6 col-md-6 col-sm-12">
                <span class="font-bold"><b>Company Name</b><span class="ms-1 text-red">*</span></span>
                <input style=" width:455px;" formControlName="company_name" type="text" class="
                     form-control form-control-lg form-control-solid
                     custom-input
                     mb-3 mb-lg-0
                     bg-white
                     custom-input
                   " placeholder="Enter Company Name" pattern="^(?!\s*$).+" name="company_name" required />

            </div>
            <br>

            <div class="col-12 col-lg-4 col-md-6 col-sm-12">
                <span class="font-bold"><b>Exisiting Customer</b></span>
                <div class="d-flex justify-content-start mt-4 ms-2">
                    <!--begin::Check-->
                    <div class="
                                form-check
                                form-check-custom
                                form-check-primary
                                form-check-solid
                                form-check-sm
                                ">
                        <input class="form-check-input" type="radio" name="existing_customer" checked="checked"
                            formControlName="existing_customer" value="Yes" />
                        <!--begin::Label-->
                        <label class="form-check-label text-gray-700 text-nowrap"
                            for="kt_builder_header_content_menu">Yes</label>
                        <!--end::Label-->
                    </div>
                    <!--end::Check-->
                    <!--begin::Check-->
                    <div class="
                                form-check
                                form-check-custom
                                form-check-primary
                                form-check-solid
                                form-check-sm
                                ms-10
                                ">
                        <input class="form-check-input" type="radio" name="existing_customer"
                            formControlName="existing_customer" value="No" />
                        <!--begin::Label-->
                        <label class="form-check-label text-gray-700 text-nowrap"
                            for="kt_builder_header_content_page-title">No</label>
                        <!--end::Label-->
                    </div>
                    <!--end::Check-->
                </div>
            </div>
        </div>


        <div class="d-flex flex-row m-6 mt-0 flex-wrap">
            <div class="col-12 col-lg-6 col-md-6 col-sm-12 ">
                <span class="font-bold"><b>Source</b><span class="ms-1 text-red"></span></span>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <ng-select style=" width: 455px; " formControlName="source_name" [items]="source_list" required
                        class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="source_name"
                        bindValue="source_gid" placeholder="Select Source"></ng-select>
                </div>
            </div>

            <div class="col-12 col-lg-4 col-md-6 col-sm-12">
                <span class="font-bold"><b>Region</b><span class="ms-1 text-red"></span></span>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <ng-select style=" width: 455px;" formControlName="region" [items]="region_list" required
                        class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2" bindLabel="region_name"
                        bindValue="region_gid" placeholder="Select Region "></ng-select>
                </div>
            </div>
        </div>
        <br>

        <div class="d-flex flex-row m-6 mt-0 flex-wrap">
            <div class="col-12 col-lg-6 col-md-6 col-sm-12 ">
                <span class="font-bold"><b>Industry</b><span class="ms-1 text-red"></span></span>
                <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                    <ng-select style=" width: 455px;" formControlName="industry" [items]="industry_list" required
                        class="custom col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2"
                        bindLabel="categoryindustry_name" bindValue="categoryindustry_gid"
                        placeholder="Select Industry "></ng-select>
                </div>
            </div>
            <br>

            <div class="col-12 col-lg-4 col-md-6 col-sm-12"><span class="font-bold"><b>City</b><span
                        class="ms-1 text-red"></span></span>
                <div class="col-12 col-lg-4 col-md-6 col-sm-12 ">
                    <div class="col-lg-11 col-12 col-md-11 col-sm-11 fv-row mt-2">
                        <input style=" width: 455px;" formControlName="city" type="text" class="
                         form-control form-control-lg form-control-solid
                         custom-input
                         mb-3 mb-lg-0
                         bg-white
                         custom-input
                       " placeholder="Enter Region Code" pattern="^(?!\s*$).+" name="city" required />
                    </div>
                </div>
            </div>
        </div>
        <br>

        <div class="d-flex flex-row m-6 mt-0 flex-wrap">
            <div class="col-12 col-lg-6 col-md-6 col-sm-12 ">
                <div class="row"><span class="font-bold"><b>State</b><span class="ms-1 text-red"></span></span>
                    <div class="col-lg-12 col-md-12 col-sm-12 fv-row mt-2 d-flex ">
                        <input style=" width: 455px; " formControlName="state" type="text" class="
                         form-control form-control-lg form-control-solid
                         custom-input
                         mb-3 mb-lg-0
                         bg-white
                         custom-input
                       " placeholder="Enter Region Code" pattern="^(?!\s*$).+" name="state" required />
                    </div>
                </div>
            </div>
            <br>

            <div class="col-12 col-lg-4 col-md-6 col-sm-12">
                <span class="font-bold"><b>Company/Branch</b></span>
                <div class="d-flex justify-content-start mt-4 ms-2">
                    <!--begin::Check-->
                    <div class="
                                    form-check
                                    form-check-custom
                                    form-check-primary
                                    form-check-solid
                                    form-check-sm
                                    ">
                        <input class="form-check-input" type="radio" name="company" checked="checked"
                            formControlName="company" value="Company" />
                        <!--begin::Label-->
                        <label class="form-check-label text-gray-700 text-nowrap"
                            for="kt_builder_header_content_menu">Company</label>
                        <!--end::Label-->
                    </div>
                    <!--end::Check-->
                    <!--begin::Check-->
                    <div class="
                                    form-check
                                    form-check-custom
                                    form-check-primary
                                    form-check-solid
                                    form-check-sm
                                    ms-10
                                    ">
                        <input class="form-check-input" type="radio" name="company" formControlName="company"
                            value="Branch" />
                        <!--begin::Label-->
                        <label class="form-check-label text-gray-700 text-nowrap"
                            for="kt_builder_header_content_page-title">Branch</label>
                        <!--end::Label-->
                    </div>
                    <!--end::Check-->
                </div>
            </div>
        </div>
        <div class="text-center my-4">
            <button type="button" class="btn-primary btn-sm text-white ms-2"
                style="width: 130px;text-align: center;height: 50px;margin:auto" (click)="GetAssignSummary()">
                <i class="fas fa-check text-white fs-5"></i> &nbsp;&nbsp;&nbsp; Search
            </button>
        </div>
    </form>

    <div class="table-responsive">
        <div class="card card-body m-3 shadow-lg p-3 mb-5 bg-white rounded">
            <table id="assign_list" class="table table-striped table-row-bordered gy-5 gs-7 ">
                <thead>
                    <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                        <th>
                            <input type="checkbox" id="checkAll" (change)="$event ? masterToggle() : null"
                                [checked]="selection.hasValue() && isAllSelected()"
                                [indeterminate]="selection.hasValue() && !isAllSelected()" />
                            Check All
                        </th>
                        <th>Created Date</th>
                        <th>Company Details</th>
                        <th>Contact Details</th>
                        <th>Region/City/State</th>
                        <th>Source</th>
                        <th>Remarks</th>
                        <th>Created By</th>
                    </tr>
                </thead>
                <tbody>
                    <tr *ngFor="let data of assign_list; let i=index;">
                        <td> <input type="checkbox" (click)="$event.stopPropagation()"
                                (change)="$event ? selection.toggle(data) : null"
                                [checked]="selection.isSelected(data)" />
                        </td>
                        <td>{{data.created_date}}</td>
                        <td>{{data.leadbank_name}}</td>
                        <td>{{data.contact_details}}</td>
                        <td>{{data.region_name}}</td>
                        <td>{{data.source_name}}</td>
                        <td><textarea [(ngModel)]="data.schedule_remarks" style="width: 70px;"></textarea>
                        </td>
                        <td>{{data.created_by}}</td>
                    </tr>
                </tbody>
            </table>
            <br>
            <div class="text-center my-4">
                <button type="button" class="btn-primary btn-sm text-white ms-2"
                    style="width: 130px;text-align: center;height: 50px;margin:auto" (click)="OnSubmit()">
                    <i class="fas fa-check text-white fs-5"></i> &nbsp;&nbsp;&nbsp; Assign
                </button>
                <button type="button" class="btn-primary btn-sm text-white ms-2"
                    style="width: 130px;text-align: center;height: 50px;margin:auto">
                    <i class="fas fa-arrow-left text-white fs-5"></i> &nbsp;&nbsp;&nbsp; Back
                </button>
            </div>
        </div>
    </div>
</div>