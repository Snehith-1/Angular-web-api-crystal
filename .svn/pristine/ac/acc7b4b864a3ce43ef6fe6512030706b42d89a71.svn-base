        </div>
    </div>
    <div class="d-flex flex-row m-3 mt-5 flex-wrap ">
        <div class="col-12 col-lg-12 col-md-6 col-sm-12">
            <div class="card-header justify-content-center">
                <h3 class="card-title"><b> Today Sales Order </b></h3>
            </div>
            <br>
            <div class="row">
                <div class="col-lg-3 col-md-3 col-sm-3" routerLink="/smr/SmrRptTodaySalesreport">
                    <div class="cardHead radius-10 border-start border-0 border-3 border-violet">
                        <div class="card-body12">
                            <div class="d-flex align-items-center">
                                <div>
                                    <h5 class="mb-0 text-secondary">Sales Order </h5>
                                    <div *ngFor="let MyTodayTaskCount_List of MyenquiryCount_list">
                                        <h4 class="my-1 text-info">{{MyTodayTaskCount_List.todaytask_count}}</h4>
                                    </div>
                                </div>
                                <div class="widgets-icons-2 rounded-circle bg-gradient-violet text-white ms-auto"><i
                                        class="bi bi-calendar3"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-3" routerLink="/smr/SmrRptTodayDeliveryreport">
                    <div class="cardHead radius-10 border-start border-0 border-3 border-darkblue">
                        <div class="card-body12">
                            <div class="d-flex align-items-center">
                                <div>
                                    <h5 class="mb-0 text-secondary">Delivery Order</h5>
                                    <div *ngFor="let MyNewTaskCount_List of MyenquiryCount_list">
                                        <h4 class="my-1 text-info">{{MyNewTaskCount_List.newlead_count}}</h4>
                                    </div>
                                </div>
                                <div class="widgets-icons-2 rounded-circle bg-gradient-darkblue text-white ms-auto"><i
                                        class="bi bi-asterisk"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-3" routerLink="/smr/SmrRptTodayInvoicereport">
                    <div class="cardHead radius-10 border-start border-0 border-3 border-darkorange"  style="background-color: #fcd3ec;">
                        <div class="card-body12">
                            <div class="d-flex align-items-center">
                                <div>
                                    <h5 class="mb-0 text-secondary">Invoice</h5>
                                    <div *ngFor="let ProspectsCount_List of MyenquiryCount_list">
                                        <h4 class="my-1 text-info">{{ProspectsCount_List.prospects_count}}</h4>
                                    </div>
                                </div>
                                <div class="widgets-icons-2 rounded-circle bg-gradient-purple text-white ms-auto"><i
                                        class="bi bi-browser-edge"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-3" routerLink="/smr/SmrRptTodayPaymentreport">
                    <div class="cardHead radius-10 border-start border-0 border-3 border-orange">
                        <div class="card-body12">
                            <div class="d-flex align-items-center">
                                <div>
                                    <h5 class="mb-0 text-secondary">Payment</h5>
                                    <div *ngFor="let PotentialsCount_List of MyenquiryCount_list">
                                        <h4 class="my-1 text-info">{{PotentialsCount_List.potential_count}}</h4>
                                    </div>
                                </div>
                                <div class="widgets-icons-2 rounded-circle bg-gradient-darkgreen text-white ms-auto"><i
                                        class="bi bi-clipboard-fill"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="table-responsive">
                    <div class="card card-body m-3 p-3 mb-5 bg-white rounded table-responsive">
                        <table id="task_list" class="table table-striped table-row-bordered gy-5 gs-7 ">
                            <thead>
                                <tr class="fw-bold fs-5 text-nowrap bg-primary text-white align-middle">
                                    <th>S.No</th>
                                    <th>SO Date</th>
                                    <th>So Reference No</th>
                                    <th>Branch Name</th>
                                    <th>Customer</th>
                                    <th>Contact Details</th>
                                    <th>SalesOrder Value</th>
                                    <th>Created By</th>
                                    <th>Status</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr *ngFor="let data of task_list; let i=index;">
                                    <td>{{i+1}}</td>
                                    <td>{{data.leadbank_name}}</td>
                                    <td>{{data.leadbank_code}}</td>
                                    <td>{{data.contact_details}}</td>
                                    <td>{{data.region_name}}</td>
                                    <td>{{data.leadstage_name}}</td>
                                    <td>{{data.schedule_type}}</td>
                                    <td>{{data.schedule_date}}</td>
                                    <td>{{data.name}}</td>
                                    <td>{{data.schedule_remarks}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>


            <div class="row">
                <div class="table-responsive">
                    <div class="card card-body m-3 p-3 mb-5 bg-white rounded table-responsive">

                        <table id="task_list" class="table table-striped table-row-bordered gy-5 gs-7">
                            <thead>
                                <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                                    <th>S.No</th>
                                    <th>Product Group</th>
                                    <th>Product</th>
                                    <th>Unit</th>
                                    <th>Order Qty</th>
                                    <th>Unit Price</th>
                                    <th>Discount</th>
                                    <th>Tax Details</th>
                                    <th>Total Price</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr *ngFor="let data of task_list;let i=index;">
                                    <td>{{i+1}}</td>
                                    <td>{{data.product_code }}</td>
                                    <td>{{data.product_name }}</td>
                                    <td>{{data.productuom_name }}</td>
                                    <td>{{data.qty_invoice }}</td>
                                    <td>{{data.product_price }}</td>
                                    <td>{{data.product_totalprice }}</td>
                                    <td>{{data.discount_percentage }}
                                        <br>{{data.discount_amount }}
                                    </td>
                                    <td>{{data.tax_name1}}&nbsp;&nbsp;: {{data.tax_amount1}}
                                        <br>{{data.tax_name2}}&nbsp;&nbsp;: {{data.tax_amount2}}
                                        <br>{{data.tax_name3}}&nbsp;&nbsp;: {{data.tax_amount3}}
                                    </td>
                                    <td>{{data.product_totalprice}}</td>
                                    <td>{{data.product_remarks }}</td>
                                </tr>

                            </tbody>

                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

            <div class="card">
                <div class="d-flex row justify-content-end mt-2">
                    <div class="col-md-3 d-flex align-items-center">
                        <label class="col-sm-6 col-form-label fw-bolder">Addon Amount:</label>
                        <input type="text" class="form-control" formControlName="addon_charge" style="width:70px"
                            placeholder="0.00">&nbsp;
                        <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
                    </div>
                </div>

                <div class="d-flex row justify-content-end mt-2">
                    <div class="col-md-3 d-flex align-items-center">
                        <label class="col-sm-6 col-form-label fw-bolder">Additional Amount:</label>
                        <input type="text" class="form-control" readonly formControlName="additional_discount"
                            style="width:70px" placeholder="0.00">&nbsp;
                        <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
                    </div>
                </div>

                <div class="d-flex row justify-content-end mt-2">
                    <div class="col-md-3 d-flex align-items-center">
                        <label class="col-sm-6 col-form-label fw-bolder">Payment Days : </label>
                        <input type="text" class="form-control" formControlName="freight_charges" readonly
                            style="width:70px" placeholder="0.00">&nbsp;
                        <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
                    </div>
                </div>
                <div class="d-flex row justify-content-end mt-2">
                    <div class="col-md-3 d-flex align-items-center">
                        <label class="col-sm-6 col-form-label fw-bolder">Delivery Days : </label>
                        <input type="text" class="form-control" formControlName="buyback_charges" readonly
                            style="width:70px" placeholder="0.00">&nbsp;
                        <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
                    </div>
                </div>
                <div class="d-flex row justify-content-end mt-2">
                    <div class="col-md-3 d-flex align-items-center">
                        <label class="col-sm-6 col-form-label fw-bolder">Grand Total:</label>
                        <input type="text" class="form-control" readonly formControlName="Grandtotal"
                            style="width:120px" placeholder="0.00">&nbsp;
                        <!-- <span style=" color:  rgb(0, 0, 80);font-weight: bold;">{{SelectedCurrencyCode}}</span> -->
                    </div>
                </div>




            </div>
        </div>

        <div>
