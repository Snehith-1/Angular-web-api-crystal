<div class="card shadow-lg rounded-top">
  <div class="card-title p-5 pb-6 pt-6 rounded-top bg-primary">
    <h3 class="fw-bolder text-white"><i class="fa fa-user fw-bolder fs-3"></i> &nbsp;&nbsp;&nbsp; My Profile</h3>
  </div>

  <div class="card">
    <div class="card-body">
      <div class="row">
        <div class="col-lg-3 col-md-12 col-sm-12">

          <div class="card shadow-sm p-3 ">
            <div class="col-lg-12 col-md-6 col-sm-12 text-center symbol symbol-150px symbol-circle">
              <img src="assets\media\images\images.png" width="120%" height="200">
            </div>

            <div class="text-center mt-6">
              <label for="">{{emp_code}}</label>
            </div>
            <div class="text-center mt-2">
              <label for="">{{emp_name}}</label>
            </div>
            <div class="text-center mt-2">
              <label for="">{{emp_designation}}</label>
            </div>
            <div class="text-center mt-2">
              <label for="">{{emp_department}}</label>
            </div>

          <!-- <div class="text-center mt-2">
            <label for="">{{emp_mobilenumber}}</label>
          </div>
        
          <div class="text-center mt-2">
            <label for="">{{emp_email}}</label>
          </div> -->
          </div>
          
          <!-- <div class="text-center mt-2">
            <label>Gender:</label>&nbsp;
            <label for="">{{emp_Gender}}</label>
          </div>
          <div class="text-center mt-2">
            <label>Date Of Joining:</label>&nbsp;
            <label for="">{{emp_Joiningdate}}</label>
          </div>
          <div class="text-center mt-2">
            <label>Address:</label>&nbsp;
            <label for="">{{emp_address}}</label>
          </div> -->


        </div>

        <div class="col-lg-9 col-md-12 col-sm-12">

          <ul class="nav nav-tabs nav-line-tabs nav-line-tabs-20x mb-8 fs-6">

            <li class="nav-item">

              <a class="nav-link active" data-bs-toggle="tab" href="#kt_tab_pane_4">Personal Details</a>

            </li>
            <li class="nav-item ms-9">

              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_5">Change Password</a>

            </li>

            <li class="nav-item ms-9">

              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_6">Work Experience</a>

            </li>
            <li class="nav-item ms-9">

              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_7">Nomination</a>

            </li>
            <li class="nav-item ms-9">

              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_8">Statutory</a>

            </li>

            <li class="nav-item ms-9">

              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_9">Emergency Contact</a>

            </li>
            <li class="nav-item ms-9">

              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_10">Dependent</a>

            </li>

            <li class="nav-item ms-9">

              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_11">Education</a>

            </li>

            <li class="nav-item ms-9">

              <a class="nav-link" data-bs-toggle="tab" href="#kt_tab_pane_12">Policy</a>

            </li>


          </ul>
          <div class="card shadow-sm p-3">

            <div class="tab-content" id="myTabContent">

              <!-- Personal details -->

              <div class="tab-pane fade show active ms-5" id="kt_tab_pane_4" role="tabpanel">

                <form [formGroup]="reactiveForm7Edit" #form="ngForm">
                  <div class="row" style="padding-right: 10px; padding-left: 10px;">
                    <div class="card-title p-5 pb-6 pt-6 rounded-top bg-primary">
                      <h3 class="fw-bolder text-white"><i class="fa fa-info-circle"></i>&nbsp;&nbsp;&nbsp; Personal
                        Details</h3>
                    </div>
                    <div style="display: flex; flex-direction: row; margin: 10px;">
                      <div class="row"><span class="font-bold" style="margin-left:1px;"><b>First Name</b><span
                            class="ms-1 text-red">*</span></span>

                        <input formControlName="firstedit_name" type="text" style="width:100%; margin-left:1px;" class="         
                 form-control form-control-lg form-control-solid borderc-secondary
                 custom-input
                 mb-3 mb-lg-1
                    bg-white
                    custom-input
                    " [ngStyle]="{
                    'border-color':
                    firstedit_name.invalid && firstedit_name.touched
                    ? 'red'
                   : 'rgb(209, 209, 209)'
                    }" placeholder="Enter First Name" pattern="^(?!\s*$).+" name="firstedit_name" required />
                      </div>
                      <div *ngIf="firstedit_name.invalid && (firstedit_name.dirty || firstedit_name.touched)">
                        <div *ngIf="firstedit_name.errors?.['required']">
                          <span style="color:red; margin-left:-35px;">First Name</span>
                        </div>
                      </div>
                      <div class="row"><span class="font-bold" style="margin-left:115px"><b>Last Name</b></span>
                        <input formControlName="lastedit_name" style="margin-left:119px; width: 63%;" type="text" class="
                    form-control form-control-lg form-control-solid borderc-secondary
                    custom-input
          mb-3 mb-lg-1
          bg-white
          custom-input
          " [ngStyle]="{
          'border-color':
          lastedit_name.invalid && lastedit_name.touched
          ? 'red'
          : 'rgb(209, 209, 209)'
          }" placeholder="Enter Last Name" pattern="^(?!\s*$).+" name="lastedit_name" />
                      </div>
                    </div>

                    <div style="display: flex; flex-direction: row; margin: 10px;">
                      <div class="row"><span class="font-bold" style="margin-left:1px;"><b>Gender</b></span>
                        <label for="html" style="margin-left:1px;"><b>MALE</b>
                          <input type="radio" value="male" formControlName="gender_edit" style="margin-left:10px;"
                            value="MALE">
                          <label for="html" style="margin-left:25px;"><b>FEMALE</b>
                            <input type="radio" value="female" formControlName="gender_edit" style="margin-left:10px;"
                              value="FEMALE"></label>
                        </label>
                      </div>


                      <div class="row"><span class="font-bold" style="margin-left:160px"><b>Date Of Birth</b></span>
                        <input formControlName="dateof_birthedit" style="margin-left:167px; width:49%;" type="date"
                          class="
          form-control form-control-lg form-control-solid borderc-secondary
          custom-input
          mb-3 mb-lg-1
          bg-white
          custom-input
          " [ngStyle]="{
          'border-color':
          dateof_birthedit.invalid && dateof_birthedit.touched
          ? 'red'
          : 'rgb(209, 209, 209)'
          }" placeholder="Enter Date" pattern="^(?!\s*$).+" name="dateof_birthedit" />
                      </div>
                    </div>

                    <div style="display: flex; flex-direction: row; margin: 10px;">
                      <div class="row"><span class="font-bold" style="margin-left:1px;"><b>Mobile Number</b></span>

                        <input formControlName="mobile_edit" type="number" style="width:99%; margin-left:1px;" class="
         form-control form-control-lg form-control-solid borderc-secondary
         custom-input
         mb-3 mb-lg-1
         bg-white
         custom-input
         " [ngStyle]="{
         'border-color':
         mobile_edit.invalid && mobile_edit.touched
         ? 'red'
         : 'rgb(209, 209, 209)'
         }" placeholder="Enter Mobile Number" pattern="^(?!\s*$).+" name="mobile_edit" />
                      </div>


                      <div class="row"><span class="font-bold" style="margin-left:119px"><b>Personal Number</b></span>
                        <input formControlName="personal_noedit" style="margin-left:125px; width: 63%;" type="number"
                          class="
          form-control form-control-lg form-control-solid borderc-secondary
          custom-input
          mb-3 mb-lg-1
          bg-white
          custom-input
          " [ngStyle]="{
          'border-color':
          personal_noedit.invalid && personal_noedit.touched
          ? 'red'
          : 'rgb(209, 209, 209)'
          }" placeholder="Enter Personal Number" pattern="^(?!\s*$).+" name="personal_noedit" />
                      </div>
                    </div>

                    <div style="display: flex; flex-direction: row; margin: 10px;">
                      <div class="row"><span class="font-bold" style="margin-left:1px;"><b>Qualification</b></span>

                        <input formControlName="qualification_edit" type="text" style="width:87%; margin-left:1px;"
                          class="
         form-control form-control-lg form-control-solid borderc-secondary
         custom-input
         mb-3 mb-lg-1
         bg-white
         custom-input
         " [ngStyle]="{
         'border-color':
         qualification_edit.invalid && qualification_edit.touched
         ? 'red'
         : 'rgb(209, 209, 209)'
         }" placeholder="Enter Qualification" pattern="^(?!\s*$).+" name="qualification_edit" />
                      </div>


                      <div class="row"><span class="font-bold" style="margin-left:90px"><b>Blood Group</b></span>
                        <select formControlName="blood_groupedit" style="margin-left:97px; width:59%;" class="
  form-control form-control-lg form-control-solid borderc-secondary
  custom-input
  mb-3 mb-lg-1
  bg-white
  custom-input" placeholder="Enter Blood Group">
                          <option>--Select an option--</option>
                          <option value="A+">A+</option>
                          <option value="B+">B+</option>
                          <option value="AB+">AB+</option>
                          <option value="AB-">AB-</option>
                          <option value="O+">O+</option>
                          <option value="O-">O-</option>
                          <option value="A1B+">A1B+</option>
                          <option value="A1B-">A1B-</option>
                        </select>
                      </div>
                    </div>

                    <div class="row"><span class="font-bold" style="margin-left:12px"><b>Experience</b></span>
                      <input formControlName="experience_edit" style="margin-left:10px; width: 38.5%;" type="number" class="
        form-control form-control-lg form-control-solid borderc-secondary
        custom-input
        mb-3 mb-lg-1
        bg-white
        custom-input
        " [ngStyle]="{
        'border-color':
        experience_edit.invalid && experience_edit.touched
        ? 'red'
        : 'rgb(209, 209, 209)'
        }" placeholder="Enter Experience" pattern="^(?!\s*$).+" name="experience_edit" />
                    </div>
                    <div class="text-center my-4">
                      <button (click)="detailsupdate()" [disabled]="reactiveForm7Edit.invalid" [ngClass]="{
                      'primary-button': !reactiveForm7Edit.invalid,
                      'secondary-button': reactiveForm7Edit.invalid
                    }" data-bs-dismiss="modal">
                        <i class="fas fa-check text-white fs-5 "></i> &nbsp;Update
                      </button> &nbsp;&nbsp;
                    </div>


                  </div>
                </form>
              </div>

              <!--Change Password-->


              <div class="row tab-pane fade ms-5" id="kt_tab_pane_5" role="tabpanel">

                <form [formGroup]="reactiveForm8Edit" #form="ngForm">
                  <div class="row" style="padding-right: 10px; padding-left: 10px;">
                    <div class="card-title p-5 pb-6 pt-6 rounded-top bg-primary">
                      <h3 class="fw-bolder text-white"><i class="fa fa-key"></i>&nbsp;&nbsp;&nbsp; Change Password
                      </h3>
                    </div>
                    <div style="display: flex; flex-direction: row; margin: 10px;">
                      <div class="row"><span class="font-bold" style="margin-left: 150px;"><b>Current Password</b><span
                            class="ms-1 text-red">*</span></span>

                        <input formControlName="curredit_pwd" type="text" style="margin-left:152px; width:40%;" class="
          form-control form-control-lg form-control-solid borderc-secondary
          custom-input
          mb-3 mb-lg-1
          bg-white
          custom-input
        " [ngStyle]="{
          'border-color':
          curredit_pwd.invalid && curredit_pwd.touched
              ? 'red'
              : 'rgb(209, 209, 209)'
        }" placeholder="Enter Current Password" pattern="^(?!\s*$).+" name="curredit_pwd" required />
                      </div>
                      <div *ngIf="curredit_pwd.invalid && (curredit_pwd.dirty || curredit_pwd.touched)">
                        <div *ngIf="curredit_pwd.errors?.['required']">
                          <span style="color:red; margin-left:-179px;">Current Password is required.</span>
                        </div>
                      </div>
                    </div>
                    <div style="display: flex; flex-direction: row; margin: 5px;">
                      <div class="row"><span class="font-bold" style="margin-left:157px"><b>New Password</b><span
                            class="ms-1 text-red">*</span></span>
                        <input formControlName="newedit_pwd" style="margin-left:157px; width: 40%;" type="password" class="
          form-control form-control-lg form-control-solid borderc-secondary
          custom-input
          mb-3 mb-lg-1
          bg-white
          custom-input
          " [ngStyle]="{
          'border-color':
          newedit_pwd.invalid && newedit_pwd.touched
          ? 'red'
          : 'rgb(209, 209, 209)'
          }" placeholder="Enter New Password" pattern="^(?!\s*$).+" name="newedit_pwd" required />
                      </div>
                      <div *ngIf="newedit_pwd.invalid && (newedit_pwd.dirty || newedit_pwd.touched)">
                        <div *ngIf="newedit_pwd.errors?.['required']">
                          <span style="color:red; margin-left:-175px;">New Password is required.</span>
                        </div>
                      </div>
                    </div>
                    <div style="display: flex; flex-direction: row; margin: 5px;">
                      <div class="row"><span class="font-bold" style="margin-left:157px"><b>Confirm Password</b><span
                            class="ms-1 text-red">*</span></span>
                        <input formControlName="confedit_pwd" style="margin-left:160px; width: 39.5%;" type="password"
                          class="
          form-control form-control-lg form-control-solid borderc-secondary
          custom-input
          mb-3 mb-lg-1
          bg-white
          custom-input
          " [ngStyle]="{
          'border-color':
          confedit_pwd.invalid && confedit_pwd.touched
          ? 'red'
          : 'rgb(209, 209, 209)'
        }" placeholder="Enter Confirm Password" pattern="^(?!\s*$).+" name="confedit_pwd" required />
                      </div>
                      <div *ngIf="confedit_pwd.invalid && (confedit_pwd.dirty || confedit_pwd.touched)">
                        <div *ngIf="confedit_pwd.errors?.['required']">
                          <span style="color:red; margin-left:-140px;">Conf Password is required.</span>
                        </div>
                      </div>
                    </div>
                    <div class="text-center my-4">
                      <button (click)="passwordupdate()" [disabled]="reactiveForm8Edit.invalid" [ngClass]="{
                      'primary-button': !reactiveForm8Edit.invalid,
                      'secondary-button': reactiveForm8Edit.invalid
                    }" data-bs-dismiss="modal">
                        <i class="fas fa-check text-white fs-5 "></i> &nbsp;Update
                      </button> &nbsp;&nbsp;
                    </div>
                  </div>
                </form>
              </div>

              <!--Work Experience-->


              <div class="tab-pane fade ms-5" id="kt_tab_pane_6" role="tabpanel">

                <form [formGroup]="reactiveForm6" #form="ngForm">
                  <div class="row" style="padding-right: 10px; padding-left: 10px;">
                    <div class="card-title p-5 pb-6 pt-6 rounded-top bg-primary">
                      <h3 class="fw-bolder text-white"><i class="fa-solid fa-briefcase"></i>&nbsp;&nbsp;&nbsp; Work
                        Experience</h3>
                    </div>
                    <div style="display: flex; flex-direction: row; margin: 10px;">
                      <div class="row"><span class="font-bold" style="margin-left:1px;"><b>Employee's Previous
                            Company</b><span class="ms-1 text-red">*</span></span>

                        <input formControlName="empl_prevcomp" type="text" style="width:80%; margin-left:1px;" class="
          form-control form-control-lg form-control-solid borderc-secondary
          custom-input
          mb-3 mb-lg-1
          bg-white
          custom-input
        " [ngStyle]="{
          'border-color':
          empl_prevcomp.invalid && empl_prevcomp.touched
              ? 'red'
              : 'rgb(209, 209, 209)'
        }" placeholder=" Previous Company" pattern="^(?!\s*$).+" name="empl_prevcomp" required />
                      </div>
                      <div *ngIf="empl_prevcomp.invalid && (empl_prevcomp.dirty || empl_prevcomp.touched)">
                        <div *ngIf="empl_prevcomp.errors?.['required']">
                          <span style="color:red;">Employee's Previous Comp required.</span>
                        </div>
                      </div>

                      <div class="row"><span class="font-bold" style="margin-left:70px"><b>Employee Code</b><span
                            class="ms-1 text-red">*</span></span>
                        <input formControlName="empl_code" style="margin-left:75px; width: 66%;" type="text" class="
         form-control form-control-lg form-control-solid borderc-secondary
         custom-input
         mb-3 mb-lg-1
         bg-white
         custom-input
         " [ngStyle]="{
         'border-color':
         empl_code.invalid && empl_code.touched
         ? 'red'
         : 'rgb(209, 209, 209)'
         }" placeholder="Employee Code" pattern="^(?!\s*$).+" name="empl_code" required />
                      </div>
                      <div *ngIf="empl_code.invalid && (empl_code.dirty || empl_code.touched)">
                        <div *ngIf="empl_code.errors?.['required']">
                          <span style="color:red;">Empl Code</span>
                        </div>
                      </div>
                    </div>


                    <div style="display: flex; flex-direction: row; margin: 10px;">
                      <div class="row"><span class="font-bold" style="margin-left:1px;"><b>Previous Occupation</b><span
                            class="ms-1 text-red">*</span></span>

                        <input formControlName="prev_occp" type="text" style="width:93%; margin-left:1px;" class="
         form-control form-control-lg form-control-solid borderc-secondary
         custom-input
         mb-3 mb-lg-1
         bg-white
         custom-input
         " [ngStyle]="{
         'border-color':
         prev_occp.invalid && prev_occp.touched
         ? 'red'
         : 'rgb(209, 209, 209)'
         }" placeholder="Previous Occupation" pattern="^(?!\s*$).+" name="prev_occp" required />
                      </div>
                      <div *ngIf="prev_occp.invalid && (prev_occp.dirty || prev_occp.touched)">
                        <div *ngIf="prev_occp.errors?.['required']">
                          <span style="color:red;">Previous Occupation required.</span>
                        </div>
                      </div>


                      <div class="row"><span class="font-bold" style="margin-left:115px"><b>Department</b><span
                            class="ms-1 text-red">*</span></span>
                        <input formControlName="department" style="margin-left:118px; width: 59%;" type="text" class="
         form-control form-control-lg form-control-solid borderc-secondary
         custom-input
         mb-3 mb-lg-1
         bg-white
         custom-input
         " [ngStyle]="{
         'border-color':
         department.invalid && department.touched
         ? 'red'
         : 'rgb(209, 209, 209)'
         }" placeholder="Enter Department" pattern="^(?!\s*$).+" name="department" required />
                      </div>
                      <div *ngIf="department.invalid && (department.dirty || department.touched)">
                        <div *ngIf="department.errors?.['required']">
                          <span style="color:red;">Department</span>
                        </div>
                      </div>
                    </div>

                    <div style="display: flex; flex-direction: row; margin: 10px;">
                      <div class="row"><span class="font-bold" style="margin-left:1px;"><b>Date of Joining</b><span
                            class="ms-1 text-red">*</span></span>

                        <input formControlName="date_ofjoining" type="date" style="width:85%; margin-left:1px;"
                          size="15" class="
          form-control form-control-lg form-control-solid borderc-secondary
          custom-input
          mb-3 mb-lg-1
          bg-white
          custom-input
          " [ngStyle]="{
          'border-color':
          date_ofjoining.invalid && date_ofjoining.touched
          ? 'red'
          : 'rgb(209, 209, 209)'
          }" placeholder="Enter Date of Joining" pattern="^(?!\s*$).+" name="date_ofjoining" required />
                      </div>
                      <div *ngIf="date_ofjoining.invalid && (date_ofjoining.dirty || date_ofjoining.touched)">
                        <div *ngIf="date_ofjoining.errors?.['required']">
                          <span style="color:red;">Date</span>
                        </div>
                      </div>

                      <div class="row"><span class="font-bold" style="margin-left:178px"><b>Date of Relieving</b><span
                            class="ms-1 text-red">*</span></span>
                        <input formControlName="date_ofreleiving" style="margin-left:183px; width: 35%;" type="date"
                          class="
        form-control form-control-lg form-control-solid borderc-secondary
        custom-input
        mb-3 mb-lg-1
        bg-white
        custom-input
        " [ngStyle]="{
        'border-color':
        date_ofreleiving.invalid && date_ofreleiving.touched
        ? 'red'
        : 'rgb(209, 209, 209)'
        }" placeholder="Enter Date of Relieving" pattern="^(?!\s*$).+" name="date_ofreleiving" required />
                      </div>
                      <div *ngIf="date_ofreleiving.invalid && (date_ofreleiving.dirty || date_ofreleiving.touched)">
                        <div *ngIf="date_ofreleiving.errors?.['required']">
                          <span style="color:red;">Date</span>
                        </div>
                      </div>
                    </div>

                    <div style="display: flex; flex-direction: row; margin: 10px;">
                      <div class="row"><span class="font-bold" style="margin-left:1px;"><b>Working Period</b></span>

                        <input formControlName="work_period" type="text" style="width:95%; margin-left:1px;" size="15"
                          class="
         form-control form-control-lg form-control-solid borderc-secondary
         custom-input
         mb-3 mb-lg-1
         bg-white
         custom-input
         " [ngStyle]="{
         'border-color':
         work_period.invalid && work_period.touched
         ? 'red'
         : 'rgb(209, 209, 209)'
         }" placeholder="Working Period" pattern="^(?!\s*$).+" name="work_period" required />
                      </div>


                      <div class="row"><span class="font-bold" style="margin-left:200px"><b>HR Name & Contact
                            Info</b><span class="ms-1 text-red">*</span></span>
                        <input formControlName="HR_name" style="margin-left:205px; width: 47.5%;" type="text" class="
          form-control form-control-lg form-control-solid borderc-secondary
          custom-input
          mb-3 mb-lg-1
          bg-white
          custom-input
          " [ngStyle]="{
          'border-color':
          HR_name.invalid && HR_name.touched
          ? 'red'
          : 'rgb(209, 209, 209)'
           }" placeholder="Enter HR" pattern="^(?!\s*$).+" name="HR_name" required />
                      </div>
                      <div *ngIf="HR_name.invalid && (HR_name.dirty || HR_name.touched)">
                        <div *ngIf="HR_name.errors?.['required']">
                          <span style="color:red;">HR</span>
                        </div>
                      </div>
                    </div>


              <div style="display: flex; flex-direction: row; margin: 10px;">
              <div class="row"><span class="font-bold" style="margin-left:-5px;"><b>Reason To Leave Previous Company</b></span>

                        <input formControlName="reason" type="text" style="width:73%; margin-left:1px;" class="
        form-control form-control-lg form-control-solid borderc-secondary
        custom-input
        mb-3 mb-lg-1
        bg-white
        custom-input
        " [ngStyle]="{
        'border-color':
        reason.invalid && reason.touched
        ? 'red'
        : 'rgb(209, 209, 209)'
        }" placeholder="Enter Reason" pattern="^(?!\s*$).+" name="reason" required />
                      </div>


                      <div class="row"><span class="font-bold" style="margin-left:50px"><b>Reporting To</b></span>
                        <input formControlName="report" style="margin-left: 50px; width: 72.5%;" type="text" class="
         form-control form-control-lg form-control-solid borderc-secondary
         custom-input
         mb-3 mb-lg-1
         bg-white
         custom-input
         " [ngStyle]="{
         'border-color':
          report.invalid && report.touched
          ? 'red'
          : 'rgb(209, 209, 209)'
          }" placeholder="Enter Reporting To" pattern="^(?!\s*$).+" name="report" required />
                      </div>
                    </div>

                    <div style="display: flex; flex-direction: row; margin: 10px;">
                    <div class="row"><span class="font-bold" style="margin-left:-5px"><b>Remarks</b></span>
                      <textarea id="w3review" formControlName="rmrks" rows="4" cols="18"
                      style="resize:none; margin-left:1px;" placeholder="Enter Remarks"
                      pattern="^(?!\s*$).+" name="rmrks" required></textarea>
                    </div>
