    <div class="table-responsive">
        <div class="card card-body m-3 shadow-lg p-3 mb-5 bg-white rounded">
            <table id="assign_list" class="table table-striped table-row-bordered gy-5 gs-7 ">
                <thead>
                    <tr class="fw-bold fs-5 text-nowrap bg-lightblue text-blue">
                        <th>
                            <input type="checkbox" id="checkAll" (change)="$event ? masterToggle() : null"
                                [checked]="selection.hasValue() && isAllSelected()"
                                [indeterminate]="selection.hasValue() && !isAllSelected()" />
                            Check All
                        </th>
                        <th>Created Date</th>
                        <th>Company Details</th>
                        <th>Contact Details</th>
                        <th>Region/City/State</th>
                        <th>Source</th>
                        <th>Remarks</th>
                        <th>Created By</th>
                    </tr>
                </thead>
                <tbody>
                    <tr *ngFor="let data of assign_list; let i=index;">
                        <td> <input type="checkbox" (click)="$event.stopPropagation()"
                                (change)="$event ? selection.toggle(data) : null"
                                [checked]="selection.isSelected(data)" />
                        </td>
                        <td>{{data.created_date}}</td>
                        <td>{{data.leadbank_name}}</td>
                        <td>{{data.contact_details}}</td>
                        <td>{{data.region_name}}</td>
                        <td>{{data.source_name}}</td>
                        <!-- <td><textarea [(ngModel)]="data.schedule_remarks" style="width: 70px;"></textarea>
                        </td> -->
                        <td class="text-center text-nowrap">
                            <button title="View Details" type="button" (click)="popmodal(data)" data-bs-toggle="modal"
                                data-bs-target="#myModalUpdateimage9" class="btn btn-icon btn-sm bg-secondary">
                                <i class="fa-solid fa-question  fs-4"style="color:grey; font-size: 20px;"></i>
                            </button>
                        </td>
                        <td>{{data.created_by}}</td>
                    </tr>
                </tbody>
            </table>
            <br>
            <div class="text-center my-4">
                <button type="button" class="btn-success btn-sm text-white ms-2"
                    style="width: 130px;text-align: center;height: 50px;margin:auto" (click)="OnSubmit()">
                    <i class="fas fa-check text-white fs-5"></i> &nbsp;&nbsp;&nbsp; Assign
                </button>
                <button type="button" class="btn-primary btn-sm text-white ms-2"
                    style="width: 130px;text-align: center;height: 50px;margin:auto" (click)="redirecttolist()">
                    <i class="fas fa-arrow-left text-white fs-5"></i> &nbsp;&nbsp;&nbsp; Back
                </button>
                
            </div>
        </div>
    </div>
</div>

<!-- ---------Remarks popup--------- -->
<div class="modal" id="myModalUpdateimage9"  data-bs-keyboard="false">
    <div class="modal-dialog">
        <div class="modal-content  d-flex justify-content-center">
            <div class="card-header p-1 pb-1 pt-1 rounded-top bg-primary" style="display: flex; align-items: center; height: 40px;">
                <h2 style="color: white; font-weight: bold; text-align: center; width: 100%;">&nbsp;&nbsp;View</h2>
            </div>
            <div class="modal-body">
                <div class="col-12 col-lg-3 col-md-6 col-lg-12">
                    <span class="fw-bold">Remarks: </span>
                    <p class="mt-2 wrap-text" style="max-width: 100%; overflow: auto; max-height: 200px;">{{remarks}}</p>
                </div>
                <div class="text-center my-4">
                    <button class="btn-primary btn-sm text-white me-2" data-bs-dismiss="modal" style="background-color: red;">
                        <i class="fas fa-arrow-left text-white"></i> &nbsp;Back
                    </button>&nbsp;
                </div>
            </div>
        </div>
    </div>
</div>